n      key: book.id,\n      class: \"book-card\"\n    }, [_createElementVNode(\"img\", {\n      loading: \"lazy\",\n      src: book.image,\n      alt: book.title,\n      class: \"book-image\"\n    }, null, 8 /* PROPS */, _hoisted_8), _createElementVNode(\"div\", _hoisted_9, [_createElementVNode(\"h3\", _hoisted_10, _toDisplayString(book.title), 1 /* TEXT */), _createElementVNode(\"p\", _hoisted_11, _toDisplayString(book.author), 1 /* TEXT */), _createElementVNode(\"div\", _hoisted_12, [(_openBlock(true), _createElementBlock(_Fragment, null, _renderList(book.tags, tag => {\n      return _openBlock(), _createElementBlock(\"span\", {\n        key: tag,\n        class: \"tag\"\n      }, _toDisplayString(tag), 1 /* TEXT */);\n    }), 128 /* KEYED_FRAGMENT */))]), _createElementVNode(\"div\", _hoisted_13, [_createElementVNode(\"div\", _hoisted_14, [_createElementVNode(\"span\", _hoisted_15, \"当前数量: \" + _toDisplayString(book.currentCount) + \"/\" + _toDisplayString(book.totalCount), 1 /* TEXT */), _createElementVNode(\"span\", {\n      class: _normalizeClass([\"book-status\", `status-${book.currentCount === 0 ? 'borrowed' : 'available'}`])\n    }, _toDisplayString(book.currentCount === 0 ? '不可借阅' : '可借阅'), 3 /* TEXT, CLASS */)]), _createElementVNode(\"button\", {\n      class: \"borrow-button\",\n      onClick: $event => $options.borrowBook(book.id),\n      disabled: book.currentCount === 0\n    }, \" 借阅 \", 8 /* PROPS */, _hoisted_16)])])]);\n  }), 128 /* KEYED_FRAGMENT */)), $options.filteredBooks.length === 0 ? (_openBlock(), _createElementBlock(\"div\", _hoisted_17, \" 没有找到匹配的书籍 \")) : _createCommentVNode(\"v-if\", true)])])]), _createCommentVNode(\" 消息提示 \"), $data.message.show ? (_openBlock(), _createElementBlock(\"div\", {\n    key: 0,\n    class: _normalizeClass([\"message\", `message-${$data.message.type}`])\n  }, _toDisplayString($data.message.text), 3 /* TEXT, CLASS */)) : _createCommentVNode(\"v-if\", true)]);\n}","map":{"version":3,"names":["class","_createElementBlock","_hoisted_1","_createElementVNode","_hoisted_2","_hoisted_3","_hoisted_4","type","placeholder","$data","searchQuery","$event","onInput","_cache","args","$options","filterBooks","_hoisted_5","categoryFilter","onChange","sortBy","sortBooks","value","_hoisted_6","_hoisted_7","_Fragment","_renderList","paginatedBooks","book","key","id","loading","src","image","alt","title","_hoisted_9","_hoisted_10","_toDisplayString","_hoisted_11","author","_hoisted_12","tags","tag","_hoisted_13","_hoisted_14","_hoisted_15","currentCount","totalCount","_normalizeClass","onClick","borrowBook","disabled","_hoisted_16","filteredBooks","length","_hoisted_17","_createCommentVNode","message","show","text"],"sources":["/Users/zhouzhou/Desktop/883Fronted/resource/src/views/Student/StudentLibrary.vue"],"sourcesContent":["<template>\n  <div class=\"student-library\">\n    <main class=\"library-container\">\n      <section class=\"search-filter-section\">\n        <div class=\"search-box\">\n          <input \n            type=\"text\" \n            placeholder=\"搜索书籍...\" \n            v-model=\"searchQuery\"\n            @input=\"filterBooks\"\n          >\n          <button class=\"search-btn\"><i class=\"bi bi-search\"></i></button>\n        </div>\n        <div class=\"filter-options\">\n          <select v-model=\"categoryFilter\" @change=\"filterBooks\">\n            <option value=\"\">所有分类</option>\n            <option value=\"文学\">文学</option>\n            <option value=\"科技\">科技</option>\n            <option value=\"历史\">历史</option>\n            <option value=\"艺术\">艺术</option>\n          </select>\n          <select v-model=\"sortBy\" @change=\"sortBooks\">\n            <option value=\"\">排序方式</option>\n            <option value=\"popular\">热门度</option>\n            <option value=\"newest\">最新上架</option>\n            <option value=\"title\">书名</option>\n          </select>\n        </div>\n      </section>\n\n      <section class=\"books-section\">\n        <h2 class=\"section-title\">根据你的兴趣推荐</h2>\n        <div class=\"books-grid\">\n          <div \n            v-for=\"book in paginatedBooks\" \n            :key=\"book.id\" \n            class=\"book-card\"\n          >\n            <img loading=\"lazy\" :src=\"book.image\" :alt=\"book.title\" class=\"book-image\">\n            <div class=\"book-info\">\n              <h3 class=\"book-title\">{{ book.title }}</h3>\n              <p class=\"book-author\">{{ book.author }}</p>\n              <div class=\"tags\">\n                <span v-for=\"tag in book.tags\" :key=\"tag\" class=\"tag\">{{ tag }}</span>\n              </div>\n              <div class=\"book-meta\">\n                <div class=\"meta-row\">\n                  <span class=\"book-current-count\">当前数量: {{ book.currentCount }}/{{ book.totalCount }}</span>\n                  <span class=\"book-status\" :class=\"`status-${book.currentCount === 0 ? 'borrowed' : 'available'}`\">\n                    {{ book.currentCount === 0 ? '不可借阅' : '可借阅' }}\n                  </span>\n                </div>\n                <button \n                  class=\"borrow-button\" \n                  @click=\"borrowBook(book.id)\"\n                  :disabled=\"book.currentCount === 0\"\n                >\n                  借阅\n                </button>\n              </div>\n            </div>\n          </div>\n          \n          <div v-if=\"filteredBooks.length === 0\" class=\"no-books\">\n            没有找到匹配的书籍\n          </div>\n        </div>\n      </section>\n    </main>\n  \n    <!-- 消息提示 -->\n    <div v-if=\"message.show\" class=\"message\" :class=\"`message-${message.type}`\">\n      {{ message.text }}\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'StudentLibrary',\n  components: {\n  },\n  data() {\n    return {\n      // 书籍数据\n      books: [\n        {\n          id: 1,\n          title: \"JavaScript高级程序设计\",\n          author: \"Nicholas C. Zakas\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/JS高级\",\n          tags: [\"编程\", \"前端\"],\n          status: \"available\",\n          totalCount: 5,\n          currentCount: 3\n        },\n        {\n          id: 2,\n          title: \"CSS权威指南\",\n          author: \"Eric A. Meyer\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/CSS权威\",\n          tags: [\"前端\", \"设计\"],\n          status: \"available\",\n          totalCount: 3,\n          currentCount: 0\n        },\n        {\n          id: 3,\n          title: \"深入浅出Node.js\",\n          author: \"朴灵\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/Node.js\",\n          tags: [\"后端\", \"JavaScript\"],\n          status: \"borrowed\",\n          totalCount: 4,\n          currentCount: 1\n        },\n        {\n          id: 4,\n          title: \"算法导论\",\n          author: \"Thomas H. Cormen\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/算法导论\",\n          tags: [\"计算机\", \"算法\"],\n          status: \"available\",\n          totalCount: 6,\n          currentCount: 2\n        },\n        {\n          id: 5,\n          title: \"设计心理学\",\n          author: \"Donald A. Norman\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/设计心理\",\n          tags: [\"设计\", \"心理学\"],\n          status: \"available\",\n          totalCount: 3,\n          currentCount: 3\n        },\n        {\n          id: 6,\n          title: \"人类简史\",\n          author: \"Yuval Noah Harari\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/人类简史\",\n          tags: [\"历史\", \"社会学\"],\n          status: \"available\",\n          totalCount: 8,\n          currentCount: 5\n        },\n        {\n          id: 7,\n          title: \"Python编程：从入门到实践\",\n          author: \"Eric Matthes\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/Python\",\n          tags: [\"编程\", \"Python\"],\n          status: \"borrowed\",\n          totalCount: 4,\n          currentCount: 0\n        },\n        {\n          id: 8,\n          title: \"经济学原理\",\n          author: \"N. Gregory Mankiw\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/经济学\",\n          tags: [\"经济\", \"教材\"],\n          status: \"available\",\n          totalCount: 5,\n          currentCount: 2\n        },\n        {\n          id: 9,\n          title: \"百年孤独\",\n          author: \"加西亚·马尔克斯\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/百年孤独\",\n          tags: [\"文学\", \"经典\"],\n          status: \"available\",\n          totalCount: 7,\n          currentCount: 3\n        },\n        {\n          id: 10,\n          title: \"活着\",\n          author: \"余华\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/活着\",\n          tags: [\"文学\", \"小说\"],\n          status: \"available\",\n          totalCount: 6,\n          currentCount: 4\n        },\n        {\n          id: 11,\n          title: \"三体\",\n          author: \"刘慈欣\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/三体\",\n          tags: [\"科幻\", \"文学\"],\n          status: \"available\",\n          totalCount: 5,\n          currentCount: 1\n        },\n        {\n          id: 12,\n          title: \"数据结构与算法\",\n          author: \"严蔚敏\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/数据结构\",\n          tags: [\"计算机\", \"算法\"],\n          status: \"available\",\n          totalCount: 4,\n          currentCount: 4\n        }\n      ],\n      // 筛选和搜索\n      searchQuery: '',\n      categoryFilter: '',\n      sortBy: '',\n      // 分页\n      currentPage: 1,\n      pageSize: 12,\n      // 消息提示\n      message: {\n        show: false,\n        text: '',\n        type: 'success'\n      }\n    }\n  },\n  computed: {\n    // 筛选后的书籍\n    filteredBooks() {\n      let filtered = this.books\n      \n      // 搜索筛选\n      if (this.searchQuery) {\n        const query = this.searchQuery.toLowerCase()\n        filtered = filtered.filter(book => \n          book.title.toLowerCase().includes(query) || \n          book.author.toLowerCase().includes(query) ||\n          book.tags.some(tag => tag.toLowerCase().includes(query))\n        )\n      }\n      \n      // 分类筛选\n      if (this.categoryFilter) {\n        filtered = filtered.filter(book => \n          book.tags.some(tag => tag === this.categoryFilter)\n        )\n      }\n      \n      return filtered\n    },\n    // 排序后的书籍\n    sortedBooks() {\n      const books = [...this.filteredBooks]\n      \n      switch (this.sortBy) {\n        case 'title':\n          return books.sort((a, b) => a.title.localeCompare(b.title))\n        case 'newest':\n          return books.sort((a, b) => b.id - a.id)\n        case 'popular':\n          // 模拟热门度排序（这里简单使用ID倒序）\n          return books.sort((a, b) => b.id - a.id)\n        default:\n          return books\n      }\n    },\n    // 分页后的书籍\n    paginatedBooks() {\n      const startIndex = (this.currentPage - 1) * this.pageSize\n      return this.sortedBooks.slice(startIndex, startIndex + this.pageSize)\n    },\n    // 总页数\n    totalPages() {\n      return Math.ceil(this.sortedBooks.length / this.pageSize)\n    }\n  },\n  methods: {\n    // 借阅书籍\n    borrowBook(bookId) {\n      const book = this.books.find(b => b.id === bookId)\n      if (book && book.currentCount > 0) {\n        book.currentCount--\n        this.showMessage(`成功借阅《${book.title}》`, 'success')\n        \n        // 重置到第一页，确保用户能看到变化\n        this.currentPage = 1\n      }\n    },\n    // 筛选书籍\n    filterBooks() {\n      this.currentPage = 1 // 重置到第一页\n    },\n    // 排序书籍\n    sortBooks() {\n      this.currentPage = 1 // 重置到第一页\n    },\n    // 上一页\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--\n      }\n    },\n    // 下一页\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++\n      }\n    },\n    // 显示消息\n    showMessage(text, type = 'success') {\n      this.message.text = text\n      this.message.type = type\n      this.message.show = true\n      \n      setTimeout(() => {\n        this.message.show = false\n      }, 3000)\n    }\n  },\n  watch: {\n    // 当筛选条件变化时重置页码\n    searchQuery() {\n      this.currentPage = 1\n    },\n    categoryFilter() {\n      this.currentPage = 1\n    }\n  }\n}\n</script>\n\n<style scoped>\n/* 其他样式保持不变，主要修改 book-meta 相关样式 */\n\n.meta-row {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 0.5rem;\n}\n\n.book-current-count {\n  font-size: 0.85rem;\n  color: #666;\n}\n\n.book-status {\n  font-size: 0.8rem;\n  padding: 2px 8px;\n  border-radius: 4px;\n  font-weight: 500;\n  min-width: 60px;\n  text-align: center;\n}\n\n.status-available {\n  background-color: #E8F5E9;\n  color: var(--secondary-color);\n}\n\n.status-borrowed {\n  background-color: #FFEBEE;\n  color: #E53935;\n}\n\n.book-meta {\n  display: flex;\n  flex-direction: column;\n  margin-top: 15px;\n}\n\n.borrow-button {\n  background-color: var(--secondary-color);\n  color: white;\n  border: none;\n  padding: 8px 16px;\n  border-radius: 4px;\n  cursor: pointer;\n  font-size: 14px;\n  transition: var(--transition);\n  width: 100%;\n}\n\n.borrow-button:hover:not(:disabled) {\n  background-color: #3D8B40;\n}\n\n.borrow-button:disabled {\n  background-color: #BDBDBD;\n  cursor: not-allowed;\n}\n\n/* 其他原有样式保持不变 */\n.student-library {\n  --primary-color: #2A5CAA;\n  --secondary-color: #4CAF50;\n  --background-color: #F8F9FA;\n  --text-color: #333;\n  --light-text: #666;\n  --border-color: #E0E0E0;\n  --card-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\n  --hover-shadow: 0 4px 12px rgba(0, 0, 0, 0.15);\n  --transition: all 0.3s ease;\n  --light-color: #FFFFFF;\n  --gray-color: #F5F5F5;\n  --shadow-color: rgba(0, 0, 0, 0.1);\n  --border-radius: 8px;\n}\n\n.student-library {\n  font-family: 'Roboto', 'PingFang SC', 'Microsoft YaHei', sans-serif;\n  background-color: var(--background-color);\n  color: var(--text-color);\n  margin: 0;\n  padding: 0;\n  line-height: 1.6;\n  min-height: 100vh;\n  display: flex;\n  flex-direction: column;\n}\n\n.library-container {\n  max-width: 1400px;\n  margin: 20px auto;\n  padding: 0 20px;\n  flex: 1;\n  width: 100%;\n}\n\n/* 搜索和筛选区域 - 更新为与AdminLibrary一致的样式 */\n.search-filter-section {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  gap: 1rem;\n  margin-bottom: 2rem;\n  background-color: var(--light-color);\n  padding: 1.5rem;\n  border-radius: var(--border-radius);\n  box-shadow: 0 2px 8px var(--shadow-color);\n}\n\n.search-box {\n  display: flex;\n  flex: 1;\n  min-width: 300px;\n}\n\n.search-box input {\n  flex: 1;\n  padding: 0.75rem 1rem;\n  border: 1px solid var(--border-color);\n  border-radius: var(--border-radius) 0 0 var(--border-radius);\n  font-size: 1rem;\n  transition: var(--transition);\n}\n\n.search-box input:focus {\n  outline: none;\n  border-color: var(--primary-color);\n}\n\n.search-btn {\n  padding: 0 1.25rem;\n  background-color: var(--primary-color);\n  color: var(--light-color);\n  border: none;\n  border-radius: 0 var(--border-radius) var(--border-radius) 0;\n  cursor: pointer;\n  transition: var(--transition);\n}\n\n.search-btn:hover {\n  background-color: #1E4B8B;\n}\n\n.filter-options {\n  display: flex;\n  gap: 1rem;\n}\n\n.filter-options select {\n  padding: 0.75rem;\n  border: 1px solid var(--border-color);\n  border-radius: var(--border-radius);\n  font-size: 1rem;\n  background-color: var(--light-color);\n  transition: var(--transition);\n}\n\n.filter-options select:focus {\n  outline: none;\n  border-color: var(--primary-color);\n}\n\n/* 推荐区域样式 */\n.section-title {\n  font-size: 22px;\n  color: var(--primary-color);\n  margin-bottom: 20px;\n  padding-bottom: 10px;\n  border-bottom: 1px solid var(--border-color);\n}\n\n/* 书籍列表样式 */\n.books-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));\n  gap: 25px;\n  margin-bottom: 30px;\n  width: 100%;\n  max-width: 100%;\n  grid-auto-flow: dense;\n}\n\n.book-card {\n  background-color: white;\n  border-radius: 8px;\n  overflow: hidden;\n  box-shadow: var(--card-shadow);\n  transition: var(--transition);\n  width: 100%;\n}\n\n.book-card:hover {\n  transform: translateY(-5px);\n  box-shadow: var(--hover-shadow);\n}\n\n.book-image {\n  width: 100%;\n  height: 180px;\n  object-fit: cover;\n}\n\n.book-info {\n  padding: 15px;\n}\n\n.book-title {\n  margin: 0 0 8px 0;\n  font-size: 16px;\n  font-weight: 500;\n}\n\n.book-author {\n  color: var(--light-text);\n  font-size: 14px;\n  margin-bottom: 10px;\n}\n\n.tags {\n  display: flex;\n  gap: 8px;\n  margin-top: 10px;\n}\n\n.tag {\n  background-color: #E8F5E9;\n  color: var(--secondary-color);\n  padding: 4px 8px;\n  border-radius: 4px;\n  font-size: 12px;\n}\n\n/* 空状态 */\n.no-books {\n  grid-column: 1 / -1;\n  text-align: center;\n  padding: 3rem;\n  color: var(--light-text);\n  font-size: 1.1rem;\n}\n\n/* 消息提示样式 */\n.message {\n  position: fixed;\n  top: 20px;\n  right: 20px;\n  padding: 12px 20px;\n  border-radius: 4px;\n  color: white;\n  opacity: 0;\n  transform: translateX(100%);\n  transition: all 0.3s ease;\n  z-index: 1000;\n}\n\n.message-success {\n  background-color: var(--secondary-color);\n}\n\n.message-error {\n  background-color: #E53935;\n}\n\n.message.show {\n  opacity: 1;\n  transform: translateX(0);\n}\n\n/* 响应式设计 */\n@media (max-width: 1024px) {\n  .search-filter-section {\n    flex-direction: column;\n    align-items: stretch;\n    gap: 15px;\n  }\n  \n  .search-box {\n    flex: 1;\n  }\n  \n  .books-grid {\n    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\n  }\n}\n\n@media (max-width: 768px) {\n  .books-grid {\n    grid-template-columns: repeat(auto-fit, minmax(220px, 1fr));\n  }\n  \n  .library-container {\n    padding: 0 15px;\n    margin: 15px auto;\n  }\n  \n  .filter-options {\n    flex-direction: column;\n    gap: 10px;\n  }\n  \n  .