{"ast":null,"code":"import \"core-js/modules/es.array.concat.js\";\nimport \"core-js/modules/es.array.includes.js\";\nimport \"core-js/modules/es.string.includes.js\";\nimport \"core-js/modules/es.string.starts-with.js\";\nimport { computed, ref, watch } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nexport default {\n  name: 'StuSidebar',\n  setup: function setup() {\n    var route = useRoute();\n    var router = useRouter();\n    var courseId = ref('');\n    watch(function () {\n      return route.params.courseId;\n    }, function (newId) {\n      courseId.value = newId || '';\n    }, {\n      immediate: true\n    });\n    var inCourseContext = computed(function () {\n      return !!courseId.value;\n    });\n\n    // ËèúÂçïÈÖçÁΩÆ\n    var menuConfig = {\n      // 1. ËØæÁ®ãÂÜÖ\n      courseSmart: [{\n        name: 'Â≠¶‰π†‰∏≠ÂøÉ',\n        children: [{\n          name: 'ËÄÉÂã§Á≠æÂà∞',\n          path: 'attendance/index',\n          icon: 'üìç'\n        }, {\n          name: 'ÊàëÁöÑ‰Ωú‰∏ö',\n          path: 'homework/list',\n          icon: 'üìù'\n        }, {\n          name: 'ÊàêÁª©Êü•ËØ¢',\n          path: 'grades/index',\n          icon: 'üìä'\n        }, {\n          name: 'ËØæÁ®ãËµÑÊ∫ê',\n          path: 'resources/list',\n          icon: 'üìÇ'\n        }, {\n          name: 'ËØ∑ÂÅáÁî≥ËØ∑',\n          path: 'leave/apply',\n          icon: 'üìÖ'\n        }]\n      }, {\n        name: 'ËØæÁ®ãÊ¶ÇÂÜµ',\n        children: [{\n          name: 'ËØæÁ®ãÈ¶ñÈ°µ',\n          path: 'overview',\n          icon: 'üè†'\n        }]\n      }],\n      // 2. Ê†°Âõ≠ÁîüÊ¥ª (‰øÆÂ§çÁÇπÔºöÁ°Æ‰øùËøôÈáåÁöÑÈÖçÁΩÆÊ≠£Á°Æ)\n      campus: [{\n        name: 'Ê†°Âõ≠ÁîüÊ¥ª',\n        children: [{\n          name: 'ÂÆøËàçÂàÜÈÖç',\n          path: '/student/studorm',\n          icon: 'üõèÔ∏è'\n        }, {\n          name: 'Ê†°Âõ≠ËÆ∫Âùõ',\n          path: '/student/forum',\n          icon: 'üí¨'\n        }]\n      }],\n      // 3. ËµÑÊ∫êÁÆ°ÁêÜ\n      resource: [{\n        name: 'ËµÑÊ∫êÁÆ°ÁêÜ',\n        children: [{\n          name: 'ÊïôÂÆ§È¢ÑÁ∫¶',\n          path: '/student/classroom',\n          icon: 'üèõÔ∏è'\n        }, {\n          name: '‰π¶Á±çÂÄüÈòÖ',\n          path: '/student/library',\n          icon: 'üìñ'\n        }]\n      }],\n      // 4. ÈªòËÆ§ÔºàÊïôÂ≠¶Ôºâ\n      teachingDefault: [{\n        name: 'ÊïôÂ≠¶ÁÆ°ÁêÜ',\n        children: [{\n          name: 'ÊàëÁöÑËØæÁ®ã',\n          path: '/student/teachinghome',\n          icon: 'üìö'\n        }, {\n          name: 'ËØ∑ÂÅáËÆ∞ÂΩï',\n          path: '/student/leave',\n          icon: 'üìÖ'\n        }]\n      }]\n    };\n\n    // === Ê†∏ÂøÉ‰øÆÂ§çÔºöÂà§Êñ≠ÂΩìÂâçÂ∫îËØ•ÊòæÁ§∫Âì™ÁªÑËèúÂçï ===\n    var currentMenuGroups = computed(function () {\n      var path = route.path;\n\n      // A. ËØæÁ®ãÂÜÖ -> ‰ºòÂÖàÁ∫ßÊúÄÈ´ò\n      if (inCourseContext.value) {\n        return menuConfig.courseSmart;\n      }\n\n      // B. Ê†°Âõ≠ÁîüÊ¥ª -> „ÄêÂÖ≥ÈîÆ‰øÆÂ§ç„ÄëËøôÈáåÂøÖÈ°ªÁ≤æÁ°ÆÂåπÈÖçÂà∞ studorm Âíå forum\n      if (path.includes('/student/studorm') || path.includes('/student/forum') || path.includes('/student/campushome') || path.includes('postdetail')) {\n        return menuConfig.campus;\n      }\n\n      // C. ËµÑÊ∫êÁÆ°ÁêÜ\n      if (path.includes('/student/classroom') || path.includes('/student/library') || path.includes('/student/resourcehome')) {\n        return menuConfig.resource;\n      }\n\n      // D. ÈªòËÆ§ÊòæÁ§∫ÊïôÂ≠¶ÁÆ°ÁêÜ\n      return menuConfig.teachingDefault;\n    });\n\n    // ËÆ°ÁÆóÊ†áÈ¢ò\n    var currentModuleTitle = computed(function () {\n      var path = route.path;\n      if (inCourseContext.value) return 'Êô∫ËÉΩÂ≠¶‰π†';\n      if (path.includes('studorm') || path.includes('forum')) return 'Ê†°Âõ≠ÁîüÊ¥ª';\n      if (path.includes('resource') || path.includes('library') || path.includes('classroom')) return 'ËµÑÊ∫êÁÆ°ÁêÜ';\n      return 'ÊïôÂ≠¶ÁÆ°ÁêÜ';\n    });\n\n    // Ë∑ØÂæÑËß£Êûê\n    var resolvePath = function resolvePath(path) {\n      if (!path.startsWith('/')) {\n        return \"/student/course/\".concat(courseId.value, \"/\").concat(path);\n      }\n      return path;\n    };\n\n    // È´ò‰∫ÆÈÄªËæë\n    var isItemActive = function isItemActive(item) {\n      var fullPath = resolvePath(item.path);\n      return route.path === fullPath || route.path.startsWith(fullPath);\n    };\n    return {\n      currentMenuGroups: currentMenuGroups,\n      currentModuleTitle: currentModuleTitle,\n      resolvePath: resolvePath,\n      isItemActive: isItemActive\n    };\n  }\n};","map":{"version":3,"names":["computed","ref","watch","useRoute","useRouter","name","setup","route","router","courseId","params","newId","value","immediate","inCourseContext","menuConfig","courseSmart","children","path","icon","campus","resource","teachingDefault","currentMenuGroups","includes","currentModuleTitle","resolvePath","startsWith","concat","isItemActive","item","fullPath"],"sources":["D:\\vvvvvvvvvscode_workspace\\fronted\\883Fronted_Final\\src\\components\\layout\\siderbars\\StuSidebar.vue"],"sourcesContent":["<template>\r\n  <aside class=\"app-sidebar\">\r\n    <div class=\"sidebar-header\">\r\n      <span class=\"sidebar-title\">{{ currentModuleTitle }}</span>\r\n    </div>\r\n\r\n    <nav class=\"sidebar-nav\">\r\n      <div v-for=\"group in currentMenuGroups\" :key=\"group.name\" class=\"menu-group\">\r\n        <div class=\"group-title\">\r\n          {{ group.name }}\r\n        </div>\r\n        <router-link v-for=\"item in group.children\" :key=\"item.path\" :to=\"resolvePath(item.path)\" class=\"menu-item\"\r\n          :class=\"{ active: isItemActive(item) }\">\r\n          <!-- <span class=\"item-icon\">{{ item.icon }}</span> -->\r\n          <span class=\"item-text\">{{ item.name }}</span>\r\n        </router-link>\r\n      </div>\r\n    </nav>\r\n  </aside>\r\n</template>\r\n\r\n<script>\r\nimport { computed, ref, watch } from 'vue'\r\nimport { useRoute, useRouter } from 'vue-router'\r\n\r\nexport default {\r\n  name: 'StuSidebar',\r\n  setup() {\r\n    const route = useRoute()\r\n    const router = useRouter()\r\n    const courseId = ref('')\r\n\r\n    watch(() => route.params.courseId, (newId) => {\r\n      courseId.value = newId || ''\r\n    }, { immediate: true })\r\n\r\n    const inCourseContext = computed(() => !!courseId.value)\r\n\r\n    // ËèúÂçïÈÖçÁΩÆ\r\n    const menuConfig = {\r\n      // 1. ËØæÁ®ãÂÜÖ\r\n      courseSmart: [\r\n        {\r\n          name: 'Â≠¶‰π†‰∏≠ÂøÉ',\r\n          children: [\r\n            { name: 'ËÄÉÂã§Á≠æÂà∞', path: 'attendance/index', icon: 'üìç' },\r\n            { name: 'ÊàëÁöÑ‰Ωú‰∏ö', path: 'homework/list', icon: 'üìù' },\r\n            { name: 'ÊàêÁª©Êü•ËØ¢', path: 'grades/index', icon: 'üìä' },\r\n            { name: 'ËØæÁ®ãËµÑÊ∫ê', path: 'resources/list', icon: 'üìÇ' },\r\n            { name: 'ËØ∑ÂÅáÁî≥ËØ∑', path: 'leave/apply', icon: 'üìÖ' }\r\n          ]\r\n        },\r\n        {\r\n          name: 'ËØæÁ®ãÊ¶ÇÂÜµ',\r\n          children: [\r\n            { name: 'ËØæÁ®ãÈ¶ñÈ°µ', path: 'overview', icon: 'üè†' }\r\n          ]\r\n        }\r\n      ],\r\n      // 2. Ê†°Âõ≠ÁîüÊ¥ª (‰øÆÂ§çÁÇπÔºöÁ°Æ‰øùËøôÈáåÁöÑÈÖçÁΩÆÊ≠£Á°Æ)\r\n      campus: [\r\n        {\r\n          name: 'Ê†°Âõ≠ÁîüÊ¥ª',\r\n          children: [\r\n            { name: 'ÂÆøËàçÂàÜÈÖç', path: '/student/studorm', icon: 'üõèÔ∏è' },\r\n            { name: 'Ê†°Âõ≠ËÆ∫Âùõ', path: '/student/forum', icon: 'üí¨' }\r\n          ]\r\n        }\r\n      ],\r\n      // 3. ËµÑÊ∫êÁÆ°ÁêÜ\r\n      resource: [\r\n        {\r\n          name: 'ËµÑÊ∫êÁÆ°ÁêÜ',\r\n          children: [\r\n            { name: 'ÊïôÂÆ§È¢ÑÁ∫¶', path: '/student/classroom', icon: 'üèõÔ∏è' },\r\n            { name: '‰π¶Á±çÂÄüÈòÖ', path: '/student/library', icon: 'üìñ' }\r\n          ]\r\n        }\r\n      ],\r\n      // 4. ÈªòËÆ§ÔºàÊïôÂ≠¶Ôºâ\r\n      teachingDefault: [\r\n        {\r\n          name: 'ÊïôÂ≠¶ÁÆ°ÁêÜ',\r\n          children: [\r\n            { name: 'ÊàëÁöÑËØæÁ®ã', path: '/student/teachinghome', icon: 'üìö' },\r\n            { name: 'ËØ∑ÂÅáËÆ∞ÂΩï', path: '/student/leave', icon: 'üìÖ' }\r\n          ]\r\n        }\r\n      ]\r\n    }\r\n\r\n    // === Ê†∏ÂøÉ‰øÆÂ§çÔºöÂà§Êñ≠ÂΩìÂâçÂ∫îËØ•ÊòæÁ§∫Âì™ÁªÑËèúÂçï ===\r\n    const currentMenuGroups = computed(() => {\r\n      const path = route.path\r\n\r\n      // A. ËØæÁ®ãÂÜÖ -> ‰ºòÂÖàÁ∫ßÊúÄÈ´ò\r\n      if (inCourseContext.value) {\r\n        return menuConfig.courseSmart\r\n      }\r\n\r\n      // B. Ê†°Âõ≠ÁîüÊ¥ª -> „ÄêÂÖ≥ÈîÆ‰øÆÂ§ç„ÄëËøôÈáåÂøÖÈ°ªÁ≤æÁ°ÆÂåπÈÖçÂà∞ studorm Âíå forum\r\n      if (path.includes('/student/studorm') || path.includes('/student/forum') || path.includes('/student/campushome') || path.includes('postdetail')) {\r\n        return menuConfig.campus\r\n      }\r\n\r\n      // C. ËµÑÊ∫êÁÆ°ÁêÜ\r\n      if (path.includes('/student/classroom') || path.includes('/student/library') || path.includes('/student/resourcehome')) {\r\n        return menuConfig.resource\r\n      }\r\n\r\n      // D. ÈªòËÆ§ÊòæÁ§∫ÊïôÂ≠¶ÁÆ°ÁêÜ\r\n      return menuConfig.teachingDefault\r\n    })\r\n\r\n    // ËÆ°ÁÆóÊ†áÈ¢ò\r\n    const currentModuleTitle = computed(() => {\r\n      const path = route.path\r\n      if (inCourseContext.value) return 'Êô∫ËÉΩÂ≠¶‰π†'\r\n      if (path.includes('studorm') || path.includes('forum')) return 'Ê†°Âõ≠ÁîüÊ¥ª'\r\n      if (path.includes('resource') || path.includes('library') || path.includes('classroom')) return 'ËµÑÊ∫êÁÆ°ÁêÜ'\r\n      return 'ÊïôÂ≠¶ÁÆ°ÁêÜ'\r\n    })\r\n\r\n    // Ë∑ØÂæÑËß£Êûê\r\n    const resolvePath = (path) => {\r\n      if (!path.startsWith('/')) {\r\n        return `/student/course/${courseId.value}/${path}`\r\n      }\r\n      return path\r\n    }\r\n\r\n    // È´ò‰∫ÆÈÄªËæë\r\n    const isItemActive = (item) => {\r\n      const fullPath = resolvePath(item.path)\r\n      return route.path === fullPath || route.path.startsWith(fullPath)\r\n    }\r\n\r\n    return {\r\n      currentMenuGroups,\r\n      currentModuleTitle,\r\n      resolvePath,\r\n      isItemActive\r\n    }\r\n  }\r\n}