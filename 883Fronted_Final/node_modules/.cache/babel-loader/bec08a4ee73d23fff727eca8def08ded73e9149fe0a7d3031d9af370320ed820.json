{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nexport default {\n  name: 'TeacherStudentClassroom',\n  data() {\n    return {\n      activeTab: 'available',\n      searchQuery: '',\n      capacityFilter: '',\n      timeFilter: '',\n      showBookingModal: false,\n      selectedClassroom: null,\n      bookingForm: {\n        date: '',\n        startTime: '',\n        endTime: '',\n        purpose: ''\n      },\n      // ÊïôÂÆ§Êï∞ÊçÆ\n      classrooms: [{\n        id: 1,\n        name: \"101ÊïôÂÆ§\",\n        capacity: 50,\n        location: \"‰∏ªÊïôÂ≠¶Ê•º1Ê•º\",\n        image: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/101ÊïôÂÆ§\",\n        schedule: [{\n          date: \"2023-06-01\",\n          time: \"08:00-10:00\",\n          available: true\n        }, {\n          date: \"2023-06-01\",\n          time: \"10:00-12:00\",\n          available: false\n        }, {\n          date: \"2023-06-01\",\n          time: \"14:00-16:00\",\n          available: true\n        }]\n      }, {\n        id: 2,\n        name: \"201Â§öÂ™í‰ΩìÊïôÂÆ§\",\n        capacity: 80,\n        location: \"‰∏ªÊïôÂ≠¶Ê•º2Ê•º\",\n        image: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/201Â§öÂ™í‰ΩìÊïôÂÆ§\",\n        schedule: [{\n          date: \"2023-06-01\",\n          time: \"08:00-10:00\",\n          available: false\n        }, {\n          date: \"2023-06-01\",\n          time: \"10:00-12:00\",\n          available: true\n        }, {\n          date: \"2023-06-01\",\n          time: \"14:00-16:00\",\n          available: true\n        }]\n      }, {\n        id: 3,\n        name: \"301ÂÆûÈ™åÂÆ§\",\n        capacity: 30,\n        location: \"ÂÆûÈ™åÊ•º3Ê•º\",\n        image: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/301ÂÆûÈ™åÂÆ§\",\n        schedule: [{\n          date: \"2023-06-01\",\n          time: \"08:00-10:00\",\n          available: true\n        }, {\n          date: \"2023-06-01\",\n          time: \"10:00-12:00\",\n          available: true\n        }, {\n          date: \"2023-06-01\",\n          time: \"14:00-16:00\",\n          available: false\n        }]\n      }, {\n        id: 4,\n        name: \"401‰ºöËÆÆÂÆ§\",\n        capacity: 20,\n        location: \"Ë°åÊîøÊ•º4Ê•º\",\n        image: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/401‰ºöËÆÆÂÆ§\",\n        schedule: [{\n          date: \"2023-06-01\",\n          time: \"08:00-10:00\",\n          available: true\n        }, {\n          date: \"2023-06-01\",\n          time: \"10:00-12:00\",\n          available: true\n        }, {\n          date: \"2023-06-01\",\n          time: \"14:00-16:00\",\n          available: true\n        }]\n      }, {\n        id: 5,\n        name: \"501Èò∂Ê¢ØÊïôÂÆ§\",\n        capacity: 120,\n        location: \"‰∏ªÊïôÂ≠¶Ê•º5Ê•º\",\n        image: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/501Èò∂Ê¢ØÊïôÂÆ§\",\n        schedule: [{\n          date: \"2023-06-01\",\n          time: \"08:00-10:00\",\n          available: false\n        }, {\n          date: \"2023-06-01\",\n          time: \"10:00-12:00\",\n          available: false\n        }, {\n          date: \"2023-06-01\",\n          time: \"14:00-16:00\",\n          available: true\n        }]\n      }, {\n        id: 6,\n        name: \"601ËÆ°ÁÆóÊú∫ÊïôÂÆ§\",\n        capacity: 60,\n        location: \"‰ø°ÊÅØÊ•º6Ê•º\",\n        image: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/601ËÆ°ÁÆóÊú∫ÊïôÂÆ§\",\n        schedule: [{\n          date: \"2023-06-01\",\n          time: \"08:00-10:00\",\n          available: true\n        }, {\n          date: \"2023-06-01\",\n          time: \"10:00-12:00\",\n          available: true\n        }, {\n          date: \"2023-06-01\",\n          time: \"14:00-16:00\",\n          available: false\n        }]\n      }],\n      // È¢ÑÁ∫¶ËÆ∞ÂΩï\n      bookings: [{\n        id: 1,\n        classroom: \"201Â§öÂ™í‰ΩìÊïôÂÆ§\",\n        date: \"2023-06-01\",\n        time: \"10:00-12:00\",\n        purpose: \"Áè≠Á∫ß‰ºöËÆÆ\",\n        status: \"approved\"\n      }, {\n        id: 2,\n        classroom: \"301ÂÆûÈ™åÂÆ§\",\n        date: \"2023-06-02\",\n        time: \"14:00-16:00\",\n        purpose: \"Áâ©ÁêÜÂÆûÈ™åËØæ\",\n        status: \"pending\"\n      }, {\n        id: 3,\n        classroom: \"501Èò∂Ê¢ØÊïôÂÆ§\",\n        date: \"2023-06-03\",\n        time: \"08:00-10:00\",\n        purpose: \"Â≠¶ÊúØËÆ≤Â∫ß\",\n        status: \"rejected\"\n      }]\n    };\n  },\n  computed: {\n    filteredClassrooms() {\n      let filtered = this.classrooms;\n\n      // ÊêúÁ¥¢Êù°‰ª∂\n      if (this.searchQuery) {\n        const query = this.searchQuery.toLowerCase();\n        filtered = filtered.filter(classroom => classroom.name.toLowerCase().includes(query));\n      }\n\n      // ÂÆπÈáèÊù°‰ª∂\n      if (this.capacityFilter) {\n        filtered = filtered.filter(classroom => {\n          switch (this.capacityFilter) {\n            case '30':\n              return classroom.capacity < 30;\n            case '50':\n              return classroom.capacity >= 30 && classroom.capacity <= 50;\n            case '100':\n              return classroom.capacity > 50 && classroom.capacity <= 100;\n            case '100+':\n              return classroom.capacity > 100;\n            default:\n              return true;\n          }\n        });\n      }\n\n      // Êó∂Èó¥Êù°‰ª∂\n      if (this.timeFilter) {\n        filtered = filtered.filter(classroom => {\n          return classroom.schedule.some(slot => {\n            const [startHour] = slot.time.split(':').map(Number);\n            switch (this.timeFilter) {\n              case 'morning':\n                return startHour >= 8 && startHour < 12;\n              case 'afternoon':\n                return startHour >= 13 && startHour < 17;\n              case 'evening':\n                return startHour >= 18 && startHour < 21;\n              default:\n                return true;\n            }\n          });\n        });\n      }\n      return filtered;\n    }\n  },\n  methods: {\n    // Ëé∑ÂèñÂèØÈ¢ÑÁ∫¶Êó∂Èó¥ÊÆµ\n    availableTimes(classroom) {\n      return classroom.schedule.filter(slot => slot.available);\n    },\n    // Ëé∑ÂèñÁä∂ÊÄÅÊñáÊú¨\n    getStatusText(status) {\n      const statusMap = {\n        approved: 'Â∑≤ÊâπÂáÜ',\n        pending: 'ÂæÖÂÆ°Êâπ',\n        rejected: 'Â∑≤ÊãíÁªù'\n      };\n      return statusMap[status] || status;\n    },\n    // ÊâìÂºÄÈ¢ÑÁ∫¶Ê®°ÊÄÅÊ°Ü\n    openBookingModal(classroom) {\n      this.selectedClassroom = classroom;\n      this.showBookingModal = true;\n\n      // ËÆæÁΩÆÈªòËÆ§Êó∂Èó¥\n      const tomorrow = new Date();\n      tomorrow.setDate(tomorrow.getDate() + 1);\n      this.bookingForm.date = tomorrow.toISOString().split('T')[0];\n      this.bookingForm.startTime = '08:00';\n      this.bookingForm.endTime = '10:00';\n    },\n    // ÂÖ≥Èó≠Ê®°ÊÄÅÊ°Ü\n    closeModal() {\n      this.showBookingModal = false;\n      this.selectedClassroom = null;\n      this.resetBookingForm();\n    },\n    // ÈáçÁΩÆÈ¢ÑÁ∫¶Ë°®Âçï\n    resetBookingForm() {\n      this.bookingForm = {\n        date: '',\n        startTime: '',\n        endTime: '',\n        purpose: ''\n      };\n    },\n    // Êèê‰∫§È¢ÑÁ∫¶\n    submitBooking() {\n      // ÂÆâÂÖ®Ê£ÄÊü•\n      if (!this.selectedClassroom) {\n        alert('ËØ∑ÈÄâÊã©Ë¶ÅÈ¢ÑÁ∫¶ÁöÑÊïôÂÆ§');\n        return;\n      }\n\n      // È™åËØÅË°®ÂçïÊï∞ÊçÆ\n      if (!this.bookingForm.date || !this.bookingForm.startTime || !this.bookingForm.endTime || !this.bookingForm.purpose) {\n        alert('ËØ∑Â°´ÂÜôÂÆåÊï¥ÁöÑÈ¢ÑÁ∫¶‰ø°ÊÅØ');\n        return;\n      }\n\n      // È™åËØÅÊó∂Èó¥ÈÄªËæë\n      if (this.bookingForm.startTime >= this.bookingForm.endTime) {\n        alert('ÁªìÊùüÊó∂Èó¥ÂøÖÈ°ªÊôö‰∫éÂºÄÂßãÊó∂Èó¥');\n        return;\n      }\n      const newBooking = {\n        id: Date.now(),\n        // ‰ΩøÁî®Êó∂Èó¥Êà≥‰Ωú‰∏∫ID\n        classroom: this.selectedClassroom.name,\n        date: this.bookingForm.date,\n        time: `${this.bookingForm.startTime}-${this.bookingForm.endTime}`,\n        purpose: this.bookingForm.purpose,\n        status: \"pending\"\n      };\n      this.bookings.unshift(newBooking);\n      this.closeModal();\n      alert('È¢ÑÁ∫¶Êèê‰∫§ÊàêÂäüÔºåËØ∑Á≠âÂæÖÂÆ°ÊâπÔºÅ');\n    },\n    // ÂèñÊ∂àÈ¢ÑÁ∫¶\n    cancelBooking(bookingId) {\n      if (confirm('Á°ÆÂÆöË¶ÅÂèñÊ∂àÊ≠§È¢ÑÁ∫¶ÂêóÔºü')) {\n        const index = this.bookings.findIndex(b => b.id === bookingId);\n        if (index !== -1) {\n          this.bookings.splice(index, 1);\n          alert('È¢ÑÁ∫¶Â∑≤ÂèñÊ∂à');\n        }\n      }\n    },\n    // Á≠õÈÄâÊïôÂÆ§ÔºàËÆ°ÁÆóÂ±ûÊÄß‰ºöËá™Âä®Â§ÑÁêÜÔºåËøôÈáå‰øùÁïôÁî®‰∫é‰∫ã‰ª∂ÁªëÂÆöÔºâ\n    filterClassrooms() {\n      // ËÆ°ÁÆóÂ±ûÊÄß‰ºöËá™Âä®Êõ¥Êñ∞ÔºåËøôÈáåÂèØ‰ª•Ê∑ªÂä†ÂÖ∂‰ªñÈÄªËæë\n    }\n  },\n  mounted() {\n    // È°µÈù¢Âä†ËΩΩÊó∂ÁöÑÂàùÂßãÂåñÈÄªËæë\n    console.log('ÊïôÂÆ§È¢ÑÁ∫¶È°µÈù¢Â∑≤Âä†ËΩΩ');\n  }\n};","map":{"version":3,"names":["name","data","activeTab","searchQuery","capacityFilter","timeFilter","showBookingModal","selectedClassroom","bookingForm","date","startTime","endTime","purpose","classrooms","id","capacity","location","image","schedule","time","available","bookings","classroom","status","computed","filteredClassrooms","filtered","query","toLowerCase","filter","includes","some","slot","startHour","split","map","Number","methods","availableTimes","getStatusText","statusMap","approved","pending","rejected","openBookingModal","tomorrow","Date","setDate","getDate","toISOString","closeModal","resetBookingForm","submitBooking","alert","newBooking","now","unshift","cancelBooking","bookingId","confirm","index","findIndex","b","splice","filterClassrooms","mounted","console","log"],"sources":["/Users/zhouzhou/Desktop/resource/src/views/TeaStuClassroom.vue"],"sourcesContent":["<template>\n  <div class=\"teacher-student-classroom\">\n    <main class=\"container\">\n      <h1 class=\"page-title\">ÊïôÂÆ§È¢ÑÁ∫¶</h1>\n      \n      <div class=\"tabs\">\n        <button \n          class=\"tab-btn\" \n          :class=\"{ active: activeTab === 'available' }\"\n          @click=\"activeTab = 'available'\"\n        >\n          ÂèØÁî®ÊïôÂÆ§\n        </button>\n        <button \n          class=\"tab-btn\" \n          :class=\"{ active: activeTab === 'bookings' }\"\n          @click=\"activeTab = 'bookings'\"\n        >\n          ÊàëÁöÑÈ¢ÑÁ∫¶\n        </button>\n      </div>\n      \n      <!-- ÊêúÁ¥¢ÂíåÁ≠õÈÄâÂå∫Âüü -->\n      <div class=\"search-filter\">\n        <div class=\"search-box\">\n          <i class=\"bi bi-search\"></i>\n          <input \n            type=\"text\" \n            placeholder=\"ÊêúÁ¥¢ÊïôÂÆ§ÂêçÁß∞ÊàñÁºñÂè∑...\" \n            v-model=\"searchQuery\"\n            @input=\"filterClassrooms\"\n          >\n        </div>\n        <div class=\"filter-group\">\n          <select v-model=\"capacityFilter\" @change=\"filterClassrooms\">\n            <option value=\"\">ÊâÄÊúâÂÆπÈáè</option>\n            <option value=\"30\">30‰∫∫‰ª•‰∏ã</option>\n            <option value=\"50\">30-50‰∫∫</option>\n            <option value=\"100\">50-100‰∫∫</option>\n            <option value=\"100+\">100‰∫∫‰ª•‰∏ä</option>\n          </select>\n          <select v-model=\"timeFilter\" @change=\"filterClassrooms\">\n            <option value=\"\">ÊâÄÊúâÊó∂Èó¥</option>\n            <option value=\"morning\">‰∏äÂçà (8:00-12:00)</option>\n            <option value=\"afternoon\">‰∏ãÂçà (13:00-17:00)</option>\n            <option value=\"evening\">Êôö‰∏ä (18:00-21:00)</option>\n          </select>\n        </div>\n      </div>\n      \n      <!-- ÂèØÁî®ÊïôÂÆ§Ê†áÁ≠æÈ°µ -->\n      <div class=\"tab-content\" :class=\"{ active: activeTab === 'available' }\">\n        <div class=\"classroom-grid\">\n          <div \n            v-for=\"classroom in filteredClassrooms\" \n            :key=\"classroom.id\" \n            class=\"classroom-card\"\n          >\n            <img \n              :src=\"classroom.image\" \n              :alt=\"classroom.name\" \n              class=\"classroom-img\" \n              loading=\"lazy\"\n            >\n            <div class=\"classroom-info\">\n              <h3 class=\"classroom-name\">{{ classroom.name }}</h3>\n              <div class=\"classroom-meta\">\n                <span class=\"classroom-capacity\">\n                  <i class=\"bi bi-people\"></i> {{ classroom.capacity }}‰∫∫\n                </span>\n                <span class=\"classroom-location\">\n                  <i class=\"bi bi-geo-alt\"></i> {{ classroom.location }}\n                </span>\n              </div>\n              <div class=\"classroom-schedule\">\n                <div \n                  v-for=\"slot in availableTimes(classroom).slice(0, 2)\" \n                  :key=\"`${slot.date}-${slot.time}`\" \n                  class=\"schedule-item\"\n                >\n                  <span><i class=\"bi bi-calendar\"></i> {{ slot.date }}</span>\n                  <span><i class=\"bi bi-clock\"></i> {{ slot.time }}</span>\n                </div>\n                <div v-if=\"availableTimes(classroom).length === 0\" class=\"no-slots\">\n                  ÊöÇÊó†ÂèØÁî®Êó∂Èó¥ÊÆµ\n                </div>\n              </div>\n              <button \n                class=\"btn-primary\" \n                @click=\"openBookingModal(classroom)\"\n                :disabled=\"availableTimes(classroom).length === 0\"\n              >\n                {{ availableTimes(classroom).length === 0 ? 'Â∑≤Á∫¶Êª°' : 'Á´ãÂç≥È¢ÑÁ∫¶' }}\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n      \n      <!-- ÊàëÁöÑÈ¢ÑÁ∫¶Ê†áÁ≠æÈ°µ -->\n      <div class=\"tab-content\" :class=\"{ active: activeTab === 'bookings' }\">\n        <div class=\"bookings-list\">\n          <div \n            v-for=\"booking in bookings\" \n            :key=\"booking.id\" \n            class=\"booking-item\"\n          >\n            <div class=\"booking-header\">\n              <span class=\"booking-title\">{{ booking.classroom }}</span>\n              <span class=\"booking-status\" :class=\"`status-${booking.status}`\">\n                {{ getStatusText(booking.status) }}\n              </span>\n            </div>\n            <div class=\"booking-details\">\n              <div class=\"booking-detail\">\n                <i class=\"bi bi-calendar\"></i>\n                <span>{{ booking.date }}</span>\n              </div>\n              <div class=\"booking-detail\">\n                <i class=\"bi bi-clock\"></i>\n                <span>{{ booking.time }}</span>\n              </div>\n              <div class=\"booking-detail\">\n                <i class=\"bi bi-journal-text\"></i>\n                <span>{{ booking.purpose }}</span>\n              </div>\n            </div>\n            <button \n              v-if=\"booking.status === 'pending'\" \n              class=\"btn-cancel\" \n              @click=\"cancelBooking(booking.id)\"\n            >\n              <i class=\"bi bi-x-circle\"></i> ÂèñÊ∂àÈ¢ÑÁ∫¶\n            </button>\n          </div>\n          <div v-if=\"bookings.length === 0\" class=\"no-bookings\">\n            ÊöÇÊó†È¢ÑÁ∫¶ËÆ∞ÂΩï\n          </div>\n        </div>\n      </div>\n    </main>\n    \n    <!-- È¢ÑÁ∫¶Ê®°ÊÄÅÊ°Ü -->\n    <div class=\"modal\" :class=\"{ show: showBookingModal }\" @click=\"closeModal\">\n      <div class=\"modal-content\" @click.stop>\n        <span class=\"close-btn\" @click=\"closeModal\">&times;</span>\n        <h2>È¢ÑÁ∫¶ÊïôÂÆ§</h2>\n        <form @submit.prevent=\"submitBooking\">\n          <div class=\"form-group\">\n            <label for=\"classroomName\">ÊïôÂÆ§ÂêçÁß∞</label>\n            <input \n              type=\"text\" \n              id=\"classroomName\" \n              :value=\"selectedClassroom ? selectedClassroom.name : ''\" \n              readonly\n            >\n          </div>\n          <div class=\"form-group\">\n            <label for=\"bookingDate\">È¢ÑÁ∫¶Êó•Êúü</label>\n            <input type=\"date\" id=\"bookingDate\" v-model=\"bookingForm.date\" required>\n          </div>\n          <div class=\"form-group\">\n            <label for=\"startTime\">ÂºÄÂßãÊó∂Èó¥</label>\n            <input type=\"time\" id=\"startTime\" v-model=\"bookingForm.startTime\" required>\n          </div>\n          <div class=\"form-group\">\n            <label for=\"endTime\">ÁªìÊùüÊó∂Èó¥</label>\n            <input type=\"time\" id=\"endTime\" v-model=\"bookingForm.endTime\" required>\n          </div>\n          <div class=\"form-group\">\n            <label for=\"purpose\">‰ΩøÁî®ÁõÆÁöÑ</label>\n            <textarea \n              id=\"purpose\" \n              rows=\"3\" \n              v-model=\"bookingForm.purpose\" \n              required\n              placeholder=\"ËØ∑Â°´ÂÜôÊïôÂÆ§‰ΩøÁî®ÁõÆÁöÑ...\"\n            ></textarea>\n          </div>\n          <button type=\"submit\" class=\"btn-primary\">Êèê‰∫§È¢ÑÁ∫¶</button>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'TeacherStudentClassroom',\n  data() {\n    return {\n      activeTab: 'available',\n      searchQuery: '',\n      capacityFilter: '',\n      timeFilter: '',\n      showBookingModal: false,\n      selectedClassroom: null,\n      bookingForm: {\n        date: '',\n        startTime: '',\n        endTime: '',\n        purpose: ''\n      },\n      // ÊïôÂÆ§Êï∞ÊçÆ\n      classrooms: [\n        {\n          id: 1,\n          name: \"101ÊïôÂÆ§\",\n          capacity: 50,\n          location: \"‰∏ªÊïôÂ≠¶Ê•º1Ê•º\",\n          image: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/101ÊïôÂÆ§\",\n          schedule: [\n            { date: \"2023-06-01\", time: \"08:00-10:00\", available: true },\n            { date: \"2023-06-01\", time: \"10:00-12:00\", available: false },\n            { date: \"2023-06-01\", time: \"14:00-16:00\", available: true }\n          ]\n        },\n        {\n          id: 2,\n          name: \"201Â§öÂ™í‰ΩìÊïôÂÆ§\",\n          capacity: 80,\n          location: \"‰∏ªÊïôÂ≠¶Ê•º2Ê•º\",\n          image: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/201Â§öÂ™í‰ΩìÊïôÂÆ§\",\n          schedule: [\n            { date: \"2023-06-01\", time: \"08:00-10:00\", available: false },\n            { date: \"2023-06-01\", time: \"10:00-12:00\", available: true },\n            { date: \"2023-06-01\", time: \"14:00-16:00\", available: true }\n          ]\n        },\n        {\n          id: 3,\n          name: \"301ÂÆûÈ™åÂÆ§\",\n          capacity: 30,\n          location: \"ÂÆûÈ™åÊ•º3Ê•º\",\n          image: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/301ÂÆûÈ™åÂÆ§\",\n          schedule: [\n            { date: \"2023-06-01\", time: \"08:00-10:00\", available: true },\n            { date: \"2023-06-01\", time: \"10:00-12:00\", available: true },\n            { date: \"2023-06-01\", time: \"14:00-16:00\", available: false }\n          ]\n        },\n        {\n          id: 4,\n          name: \"401‰ºöËÆÆÂÆ§\",\n          capacity: 20,\n          location: \"Ë°åÊîøÊ•º4Ê•º\",\n          image: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/401‰ºöËÆÆÂÆ§\",\n          schedule: [\n            { date: \"2023-06-01\", time: \"08:00-10:00\", available: true },\n            { date: \"2023-06-01\", time: \"10:00-12:00\", available: true },\n            { date: \"2023-06-01\", time: \"14:00-16:00\", available: true }\n          ]\n        },\n        {\n          id: 5,\n          name: \"501Èò∂Ê¢ØÊïôÂÆ§\",\n          capacity: 120,\n          location: \"‰∏ªÊïôÂ≠¶Ê•º5Ê•º\",\n          image: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/501Èò∂Ê¢ØÊïôÂÆ§\",\n          schedule: [\n            { date: \"2023-06-01\", time: \"08:00-10:00\", available: false },\n            { date: \"2023-06-01\", time: \"10:00-12:00\", available: false },\n            { date: \"2023-06-01\", time: \"14:00-16:00\", available: true }\n          ]\n        },\n        {\n          id: 6,\n          name: \"601ËÆ°ÁÆóÊú∫ÊïôÂÆ§\",\n          capacity: 60,\n          location: \"‰ø°ÊÅØÊ•º6Ê•º\",\n          image: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/601ËÆ°ÁÆóÊú∫ÊïôÂÆ§\",\n          schedule: [\n            { date: \"2023-06-01\", time: \"08:00-10:00\", available: true },\n            { date: \"2023-06-01\", time: \"10:00-12:00\", available: true },\n            { date: \"2023-06-01\", time: \"14:00-16:00\", available: false }\n          ]\n        }\n      ],\n      // È¢ÑÁ∫¶ËÆ∞ÂΩï\n      bookings: [\n        {\n          id: 1,\n          classroom: \"201Â§öÂ™í‰ΩìÊïôÂÆ§\",\n          date: \"2023-06-01\",\n          time: \"10:00-12:00\",\n          purpose: \"Áè≠Á∫ß‰ºöËÆÆ\",\n          status: \"approved\"\n        },\n        {\n          id: 2,\n          classroom: \"301ÂÆûÈ™åÂÆ§\",\n          date: \"2023-06-02\",\n          time: \"14:00-16:00\",\n          purpose: \"Áâ©ÁêÜÂÆûÈ™åËØæ\",\n          status: \"pending\"\n        },\n        {\n          id: 3,\n          classroom: \"501Èò∂Ê¢ØÊïôÂÆ§\",\n          date: \"2023-06-03\",\n          time: \"08:00-10:00\",\n          purpose: \"Â≠¶ÊúØËÆ≤Â∫ß\",\n          status: \"rejected\"\n        }\n      ]\n    }\n  },\n  computed: {\n    filteredClassrooms() {\n      let filtered = this.classrooms\n      \n      // ÊêúÁ¥¢Êù°‰ª∂\n      if (this.searchQuery) {\n        const query = this.searchQuery.toLowerCase()\n        filtered = filtered.filter(classroom => \n          classroom.name.toLowerCase().includes(query)\n        )\n      }\n      \n      // ÂÆπÈáèÊù°‰ª∂\n      if (this.capacityFilter) {\n        filtered = filtered.filter(classroom => {\n          switch(this.capacityFilter) {\n            case '30':\n              return classroom.capacity < 30\n            case '50':\n              return classroom.capacity >= 30 && classroom.capacity <= 50\n            case '100':\n              return classroom.capacity > 50 && classroom.capacity <= 100\n            case '100+':\n              return classroom.capacity > 100\n            default:\n              return true\n          }\n        })\n      }\n      \n      // Êó∂Èó¥Êù°‰ª∂\n      if (this.timeFilter) {\n        filtered = filtered.filter(classroom => {\n          return classroom.schedule.some(slot => {\n            const [startHour] = slot.time.split(':').map(Number)\n            switch(this.timeFilter) {\n              case 'morning':\n                return startHour >= 8 && startHour < 12\n              case 'afternoon':\n                return startHour >= 13 && startHour < 17\n              case 'evening':\n                return startHour >= 18 && startHour < 21\n              default:\n                return true\n            }\n          })\n        })\n      }\n      \n      return filtered\n    }\n  },\n  methods: {\n    // Ëé∑ÂèñÂèØÈ¢ÑÁ∫¶Êó∂Èó¥ÊÆµ\n    availableTimes(classroom) {\n      return classroom.schedule.filter(slot => slot.available)\n    },\n    \n    // Ëé∑ÂèñÁä∂ÊÄÅÊñáÊú¨\n    getStatusText(status) {\n      const statusMap = {\n        approved: 'Â∑≤ÊâπÂáÜ',\n        pending: 'ÂæÖÂÆ°Êâπ',\n        rejected: 'Â∑≤ÊãíÁªù'\n      }\n      return statusMap[status] || status\n    },\n    \n    // ÊâìÂºÄÈ¢ÑÁ∫¶Ê®°ÊÄÅÊ°Ü\n    openBookingModal(classroom) {\n      this.selectedClassroom = classroom\n      this.showBookingModal = true\n      \n      // ËÆæÁΩÆÈªòËÆ§Êó∂Èó¥\n      const tomorrow = new Date()\n      tomorrow.setDate(tomorrow.getDate() + 1)\n      this.bookingForm.date = tomorrow.toISOString().split('T')[0]\n      this.bookingForm.startTime = '08:00'\n      this.bookingForm.endTime = '10:00'\n    },\n    \n    // ÂÖ≥Èó≠Ê®°ÊÄÅÊ°Ü\n    closeModal() {\n      this.showBookingModal = false\n      this.selectedClassroom = null\n      this.resetBookingForm()\n    },\n    \n    // ÈáçÁΩÆÈ¢ÑÁ∫¶Ë°®Âçï\n    resetBookingForm() {\n      this.bookingForm = {\n        date: '',\n        startTime: '',\n        endTime: '',\n        purpose: ''\n      }\n    },\n    \n    // Êèê‰∫§È¢ÑÁ∫¶\n    submitBooking() {\n      // ÂÆâÂÖ®Ê£ÄÊü•\n      if (!this.selectedClassroom) {\n        alert('ËØ∑ÈÄâÊã©Ë¶ÅÈ¢ÑÁ∫¶ÁöÑÊïôÂÆ§')\n        return\n      }\n      \n      // È™åËØÅË°®ÂçïÊï∞ÊçÆ\n      if (!this.bookingForm.date || !this.bookingForm.startTime || !this.bookingForm.endTime || !this.bookingForm.purpose) {\n        alert('ËØ∑Â°´ÂÜôÂÆåÊï¥ÁöÑÈ¢ÑÁ∫¶‰ø°ÊÅØ')\n        return\n      }\n      \n      // È™åËØÅÊó∂Èó¥ÈÄªËæë\n      if (this.bookingForm.startTime >= this.bookingForm.endTime) {\n        alert('ÁªìÊùüÊó∂Èó¥ÂøÖÈ°ªÊôö‰∫éÂºÄÂßãÊó∂Èó¥')\n        return\n      }\n      \n      const newBooking = {\n        id: Date.now(), // ‰ΩøÁî®Êó∂Èó¥Êà≥‰Ωú‰∏∫ID\n        classroom: this.selectedClassroom.name,\n        date: this.bookingForm.date,\n        time: `${this.bookingForm.startTime}-${this.bookingForm.endTime}`,\n        purpose: this.bookingForm.purpose,\n        status: \"pending\"\n      }\n      \n      this.bookings.unshift(newBooking)\n      this.closeModal()\n      alert('È¢ÑÁ∫¶Êèê‰∫§ÊàêÂäüÔºåËØ∑Á≠âÂæÖÂÆ°ÊâπÔºÅ')\n    },\n    \n    // ÂèñÊ∂àÈ¢ÑÁ∫¶\n    cancelBooking(bookingId) {\n      if (confirm('Á°ÆÂÆöË¶ÅÂèñÊ∂àÊ≠§È¢ÑÁ∫¶ÂêóÔºü')) {\n        const index = this.bookings.findIndex(b => b.id === bookingId)\n        if (index !== -1) {\n          this.bookings.splice(index, 1)\n          alert('È¢ÑÁ∫¶Â∑≤ÂèñÊ∂à')\n        }\n      }\n    },\n    \n    // Á≠õÈÄâÊïôÂÆ§ÔºàËÆ°ÁÆóÂ±ûÊÄß‰ºöËá™Âä®Â§ÑÁêÜÔºåËøôÈáå‰øùÁïôÁî®‰∫é‰∫ã‰ª∂ÁªëÂÆöÔºâ\n    filterClassrooms() {\n      // ËÆ°ÁÆóÂ±ûÊÄß‰ºöËá™Âä®Êõ¥Êñ∞ÔºåËøôÈáåÂèØ‰ª•Ê∑ªÂä†ÂÖ∂‰ªñÈÄªËæë\n    }\n  },\n  mounted() {\n    // È°µÈù¢Âä†ËΩΩÊó∂ÁöÑÂàùÂßãÂåñÈÄªËæë\n    console.log('ÊïôÂÆ§È¢ÑÁ∫¶È°µÈù¢Â∑≤Âä†ËΩΩ')\n  }\n}\n</script>\n\n<style scoped>\n/* Âü∫Á°ÄÊ†∑Âºè */\n:root {\n  --primary-color: #2A5CAA;\n  --secondary-color: #4CAF50;\n  --light-color: #F5F7FA;\n  --dark-color: #333333;\n  --gray-colo