n    }, { immediate: true })\r\n\r\n    const inCourseContext = computed(() => !!courseId.value)\r\n\r\n    // === ËèúÂçïÈÖçÁΩÆÔºöÂ∑¶‰æßÂè™Êîæ‚ÄúÂ§ßÊ®°Âùó‚Äù ===\r\n    const menuConfig = {\r\n      // 1. ËØæÁ®ãÂÜÖËèúÂçï\r\n      courseSmart: [\r\n        {\r\n          name: 'ÊïôÂ≠¶ÁÆ°ÁêÜ',\r\n          items: [\r\n            // ÁÇπÂáªËøô‰∫õÔºåÂéªÂæÄÂêÑËá™ÁöÑÈªòËÆ§È°µÈù¢ÔºàÈ°∂Ê†è‰ºöÊòæÁ§∫Â≠êËèúÂçïÔºâ\r\n            { name: 'ËÄÉÂã§ÁÆ°ÁêÜ', path: 'attendance/launch', icon: 'üìç' },\r\n            { name: '‰Ωú‰∏öÁÆ°ÁêÜ', path: 'homework/publish', icon: 'üìù' },\r\n            { name: 'ÊàêÁª©ÁÆ°ÁêÜ', path: 'grades/detail', icon: 'üìä' },\r\n            { name: 'ËØæÁ®ãËµÑÊ∫ê', path: 'resources/list', icon: 'üìÇ' },\r\n            { name: 'Â≠¶ÁîüÁÆ°ÁêÜ', path: 'students/detail', icon: 'üë®‚Äçüéì' },\r\n            { name: 'ËØ∑ÂÅáÂÆ°Êâπ', path: 'leave/approval', icon: 'üìÖ' } // ÂÅáËÆæ‰Ω†ÊúâËøô‰∏™\r\n          ]\r\n        },\r\n        {\r\n          name: 'ËØæÁ®ãÊ¶ÇÂÜµ',\r\n          items: [\r\n            { name: 'ËØæÁ®ãÈ¶ñÈ°µ', path: 'overview', icon: 'üè†' },\r\n            { name: 'Áü•ËØÜÂõæË∞±', path: 'learning-path', icon: 'üß†' },\r\n          ]\r\n        }\r\n      ],\r\n      // 2. ËØæÁ®ãÂ§ñËèúÂçï (ÈùûËØæÁ®ãËØ¶ÊÉÖÈ°µ)\r\n      default: [\r\n        {\r\n          name: 'Â∑•‰ΩúÂè∞',\r\n          items: [\r\n            { name: 'ÊàëÁöÑËØæÁ®ã', path: '/teacher/teachinghome', icon: 'üìö' },\r\n            { name: 'ËØ∑ÂÅá‰∏≠ÂøÉ', path: '/teacher/leave', icon: 'üìÖ' },\r\n          ]\r\n        },\r\n        {\r\n          name: 'ÂÖ¨ÂÖ±ËµÑÊ∫ê',\r\n          items: [\r\n            { name: 'ÊïôÂÆ§È¢ÑÁ∫¶', path: '/teacher/classroom', icon: 'üèõÔ∏è' },\r\n            { name: 'Âõæ‰π¶È¶Ü', path: '/teacher/library', icon: 'üìñ' }\r\n          ]\r\n        }\r\n      ]\r\n    }\r\n\r\n    const currentMenuGroups = computed(() => {\r\n      if (inCourseContext.value) return menuConfig.courseSmart\r\n      return menuConfig.default\r\n    })\r\n\r\n    const currentModuleTitle = computed(() => {\r\n      if (inCourseContext.value) return 'Êô∫ËÉΩÊïôÂ≠¶Á≥ªÁªü'\r\n      return 'ÊïôÂ∏à‰∏≠ÂøÉ'\r\n    })\r\n\r\n    const resolvePath = (path) => {\r\n      if (!path.startsWith('/')) {\r\n        return `/teacher/course/${courseId.value}/${path}`\r\n      }\r\n      return path\r\n    }\r\n\r\n    // È´ò‰∫ÆÈÄªËæëÔºöÂè™Ë¶ÅÂΩìÂâçË∑ØÁî±ÂåÖÂê´Ëøô‰∏™Ê®°ÂùóÁöÑÂÖ≥ÈîÆËØçÔºå‰æßËæπÊ†èÂ∞±È´ò‰∫Æ\r\n    const isItemActive = (item) => {\r\n      const fullPath = resolvePath(item.path)\r\n      const currentPath = route.path\r\n\r\n      // ÊèêÂèñÊ®°ÂùóÂêç (‰æãÂ¶Ç attendance) ËøõË°åÊ®°Á≥äÂåπÈÖç\r\n      // item.path Ê†ºÂºèÂ¶Ç \"attendance/launch\" -> Âèñ \"attendance\"\r\n      if (inCourseContext.value && !item.path.startsWith('/')) {\r\n        const moduleKey = item.path.split('/')[0]\r\n        // Â¶ÇÊûúÂΩìÂâçË∑ØÂæÑÈáåÂåÖÂê´Ëøô‰∏™Ê®°ÂùóÂêç (e.g. /teacher/course/101/attendance/detail)\r\n        if (currentPath.includes(`/${moduleKey}`)) {\r\n          return true\r\n        }\r\n      }\r\n\r\n      // Á≤æÁ°ÆÂåπÈÖç (Áî®‰∫éÈ¶ñÈ°µÁ≠â)\r\n      return currentPath === fullPath || currentPath.startsWith(fullPath + '/')\r\n    }\r\n\r\n    return {\r\n      currentMenuGroups,\r\n      currentModuleTitle,\r\n      resolvePath,\r\n      isItemActive\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* === ÂÆåÂÖ®Â§çÁî® StuSidebar.vue ÁöÑÊ†∑Âºè === */\r\n.app-sidebar {\r\n  width: 240px;\r\n  background-color: var(--sidebar-bg, #d6e2f5);\r\n  /* Â≠¶ÁîüÁ´ØÁöÑÊ∑°ËìùËâ≤ËÉåÊôØ */\r\n  height: calc(100vh - 60px);\r\n  position: fixed;\r\n  top: 60px;\r\n  left: 0;\r\n  box-shadow: var(--sidebar-shadow, 2px 0 8px rgba(0, 0, 0, 0.1));\r\n  z-index: 999;\r\n  overflow-y: auto;\r\n}\r\n\r\n.sidebar-header {\r\n  height: 50px;\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 0 15px;\r\n  border-bottom: 1px solid rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.sidebar-title {\r\n  font-weight: bold;\r\n  color: var(--primary-color, #2A5CAA);\r\n  font-size: 16px;\r\n}\r\n\r\n.sidebar-nav {\r\n  padding: 15px 0;\r\n}\r\n\r\n.menu-group {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.group-title {\r\n  padding: 0 15px 8px 15px;\r\n  font-size: 12px;\r\n  color: var(--text-light-gray, #666666);\r\n  text-transform: uppercase;\r\n  letter-spacing: 1px;\r\n  font-weight: 600;\r\n}\r\n\r\n.menu-item {\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 12px 15px;\r\n  text-decoration: none;\r\n  color: var(--text-gray, #333333);\r\n  transition: all 0.3s;\r\n  border-left: 3px solid transparent;\r\n  /* Â∑¶‰æßÈ´ò‰∫ÆÊù° */\r\n  gap: 10px;\r\n  margin: 2px 0;\r\n}\r\n\r\n.menu-item:hover {\r\n  background-color: rgba(194, 212, 240, 0.5);\r\n  color: var(--primary-color, #2A5CAA);\r\n}\r\n\r\n.menu-item.active {\r\n  background-color: var(--sidebar-active, #c2d4f0);\r\n  /* ÊøÄÊ¥ªÊó∂ÁöÑËÉåÊôØËâ≤ */\r\n  color: var(--primary-color, #2A5CAA);\r\n  border-left-color: var(--primary-color, #2A5CAA);\r\n  /* ÊøÄÊ¥ªÊó∂ÁöÑÂ∑¶‰æßËìùÊù° */\r\n}\r\n\r\n.item-icon {\r\n  font-size: 16px;\r\n  min-width: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.item-text {\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  font-weight: 500;\r\n}\r\n</style>"],"mappings":";;;EACS,SAAM;AAAa;;EACnB,SAAM;AAAgB;;EACnB,SAAM;AAAe;;EAGxB,SAAM;AAAa;;EAEf,SAAM;AAAa;;EAKhB,SAAM;AAAW;;EACjB,SAAM;AAAW;;;uBAb/BA,mBAAA,CAiBQ,SAjBRC,UAiBQ,GAhBNC,mBAAA,CAEM,OAFNC,UAEM,GADJD,mBAAA,CAA2D,QAA3DE,UAA2D,EAAAC,gBAAA,CAA5BC,MAAA,CAAAC,kBAAkB,iB,GAGnDL,mBAAA,CAWM,OAXNM,UAWM,I,kBAVJR,mBAAA,CASMS,SAAA,QAAAC,WAAA,CATeJ,MAAA,CAAAK,iBAAiB,YAA1BC,KAAK;yBAAjBZ,mBAAA,CASM;MATmCa,GAAG,EAAED,KAAK,CAACE,IAAI;MAAE,SAAM;QAC9DZ,mBAAA,CAEM,OAFNa,UAEM,EAAAV,gBAAA,CADDO,KAAK,CAACE,IAAI,mB,kBAEfd,mBAAA,CAIcS,SAAA,QAAAC,WAAA,CAJcE,KAAK,CAACI,KAAK,YAAnBC,IAAI;2BAAxBC,YAAA,CAIcC,sBAAA;QAJ4BN,GAAG,EAAEI,IAAI,CAACG,IAAI;QAAGC,EAAE,EAAEf,MAAA,CAAAgB,WAAW,CAACL,IAAI,CAACG,IAAI;QAAG,SAAKG,eAAA,EAAC,WAAW;UAAAC,MAAA,EACpFlB,MAAA,CAAAmB,YAAY,CAACR,IAAI;QAAA;;4BACnC;UAAA,OAA8C,CAA9Cf,mBAAA,CAA8C,QAA9C