{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nexport default {\n  name: 'StudentLibrary',\n  components: {},\n  data() {\n    return {\n      // 书籍数据\n      books: [{\n        id: 1,\n        title: \"JavaScript高级程序设计\",\n        author: \"Nicholas C. Zakas\",\n        image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/JS高级\",\n        tags: [\"编程\", \"前端\"],\n        status: \"available\"\n      }, {\n        id: 2,\n        title: \"CSS权威指南\",\n        author: \"Eric A. Meyer\",\n        image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/CSS权威\",\n        tags: [\"前端\", \"设计\"],\n        status: \"available\"\n      }, {\n        id: 3,\n        title: \"深入浅出Node.js\",\n        author: \"朴灵\",\n        image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/Node.js\",\n        tags: [\"后端\", \"JavaScript\"],\n        status: \"borrowed\"\n      }, {\n        id: 4,\n        title: \"算法导论\",\n        author: \"Thomas H. Cormen\",\n        image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/算法导论\",\n        tags: [\"计算机\", \"算法\"],\n        status: \"available\"\n      }, {\n        id: 5,\n        title: \"设计心理学\",\n        author: \"Donald A. Norman\",\n        image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/设计心理\",\n        tags: [\"设计\", \"心理学\"],\n        status: \"available\"\n      }, {\n        id: 6,\n        title: \"人类简史\",\n        author: \"Yuval Noah Harari\",\n        image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/人类简史\",\n        tags: [\"历史\", \"社会学\"],\n        status: \"available\"\n      }, {\n        id: 7,\n        title: \"Python编程：从入门到实践\",\n        author: \"Eric Matthes\",\n        image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/Python\",\n        tags: [\"编程\", \"Python\"],\n        status: \"borrowed\"\n      }, {\n        id: 8,\n        title: \"经济学原理\",\n        author: \"N. Gregory Mankiw\",\n        image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/经济学\",\n        tags: [\"经济\", \"教材\"],\n        status: \"available\"\n      }, {\n        id: 9,\n        title: \"百年孤独\",\n        author: \"加西亚·马尔克斯\",\n        image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/百年孤独\",\n        tags: [\"文学\", \"经典\"],\n        status: \"available\"\n      }, {\n        id: 10,\n        title: \"活着\",\n        author: \"余华\",\n        image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/活着\",\n        tags: [\"文学\", \"小说\"],\n        status: \"available\"\n      }, {\n        id: 11,\n        title: \"三体\",\n        author: \"刘慈欣\",\n        image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/三体\",\n        tags: [\"科幻\", \"文学\"],\n        status: \"available\"\n      }, {\n        id: 12,\n        title: \"数据结构与算法\",\n        author: \"严蔚敏\",\n        image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/数据结构\",\n        tags: [\"计算机\", \"算法\"],\n        status: \"available\"\n      }],\n      // 筛选和搜索\n      searchQuery: '',\n      categoryFilter: '',\n      sortBy: '',\n      // 分页\n      currentPage: 1,\n      pageSize: 12,\n      // 消息提示\n      message: {\n        show: false,\n        text: '',\n        type: 'success'\n      }\n    };\n  },\n  computed: {\n    // 筛选后的书籍\n    filteredBooks() {\n      let filtered = this.books;\n\n      // 搜索筛选\n      if (this.searchQuery) {\n        const query = this.searchQuery.toLowerCase();\n        filtered = filtered.filter(book => book.title.toLowerCase().includes(query) || book.author.toLowerCase().includes(query) || book.tags.some(tag => tag.toLowerCase().includes(query)));\n      }\n\n      // 分类筛选\n      if (this.categoryFilter) {\n        filtered = filtered.filter(book => book.tags.some(tag => tag === this.categoryFilter));\n      }\n      return filtered;\n    },\n    // 排序后的书籍\n    sortedBooks() {\n      const books = [...this.filteredBooks];\n      switch (this.sortBy) {\n        case 'title':\n          return books.sort((a, b) => a.title.localeCompare(b.title));\n        case 'newest':\n          return books.sort((a, b) => b.id - a.id);\n        case 'popular':\n          // 模拟热门度排序（这里简单使用ID倒序）\n          return books.sort((a, b) => b.id - a.id);\n        default:\n          return books;\n      }\n    },\n    // 分页后的书籍\n    paginatedBooks() {\n      const startIndex = (this.currentPage - 1) * this.pageSize;\n      return this.sortedBooks.slice(startIndex, startIndex + this.pageSize);\n    },\n    // 总页数\n    totalPages() {\n      return Math.ceil(this.sortedBooks.length / this.pageSize);\n    }\n  },\n  methods: {\n    // 借阅书籍\n    borrowBook(bookId) {\n      const book = this.books.find(b => b.id === bookId);\n      if (book && book.status === 'available') {\n        book.status = 'borrowed';\n        this.showMessage(`成功借阅《${book.title}》`, 'success');\n\n        // 重置到第一页，确保用户能看到变化\n        this.currentPage = 1;\n      }\n    },\n    // 筛选书籍\n    filterBooks() {\n      this.currentPage = 1; // 重置到第一页\n    },\n    // 排序书籍\n    sortBooks() {\n      this.currentPage = 1; // 重置到第一页\n    },\n    // 上一页\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    // 下一页\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    // 显示消息\n    showMessage(text, type = 'success') {\n      this.message.text = text;\n      this.message.type = type;\n      this.message.show = true;\n      setTimeout(() => {\n        this.message.show = false;\n      }, 3000);\n    }\n  },\n  watch: {\n    // 当筛选条件变化时重置页码\n    searchQuery() {\n      this.currentPage = 1;\n    },\n    categoryFilter() {\n      this.currentPage = 1;\n    }\n  }\n};","map":{"version":3,"names":["name","components","data","books","id","title","author","image","tags","status","searchQuery","categoryFilter","sortBy","currentPage","pageSize","message","show","text","type","computed","filteredBooks","filtered","query","toLowerCase","filter","book","includes","some","tag","sortedBooks","sort","a","b","localeCompare","paginatedBooks","startIndex","slice","totalPages","Math","ceil","length","methods","borrowBook","bookId","find","showMessage","filterBooks","sortBooks","prevPage","nextPage","setTimeout","watch"],"sources":["/Users/zhouzhou/Desktop/883Fronted/resource/src/views/Student/StudentLibrary.vue"],"sourcesContent":["<template>\n  <div class=\"student-library\">\n    <main class=\"library-container\">\n      <section class=\"search-filter-section\">\n        <div class=\"search-box\">\n          <input \n            type=\"text\" \n            placeholder=\"搜索书籍...\" \n            v-model=\"searchQuery\"\n            @input=\"filterBooks\"\n          >\n          <button class=\"search-btn\"><i class=\"bi bi-search\"></i></button>\n        </div>\n        <div class=\"filter-options\">\n          <select v-model=\"categoryFilter\" @change=\"filterBooks\">\n            <option value=\"\">所有分类</option>\n            <option value=\"文学\">文学</option>\n            <option value=\"科技\">科技</option>\n            <option value=\"历史\">历史</option>\n            <option value=\"艺术\">艺术</option>\n          </select>\n          <select v-model=\"sortBy\" @change=\"sortBooks\">\n            <option value=\"\">排序方式</option>\n            <option value=\"popular\">热门度</option>\n            <option value=\"newest\">最新上架</option>\n            <option value=\"title\">书名</option>\n          </select>\n        </div>\n      </section>\n      <!--\n      <section class=\"recommendation-section\">\n        <h2 class=\"section-title\">根据你的兴趣推荐</h2>\n        <div class=\"recommendation-grid\">\n          <div class=\"recommendation-card\">\n            <img loading=\"lazy\" src=\"https://placeholder.pics/svg/150x200/DEDEDE/555555/文学经典\" alt=\"推荐书籍\" class=\"recommendation-image\">\n            <div class=\"recommendation-info\">\n              <h3>文学经典选集</h3>\n              <p class=\"author\">多位作者</p>\n              <div class=\"tags\">\n                <span class=\"tag\">文学</span>\n                <span class=\"tag\">经典</span>\n              </div>\n            </div>\n          </div>\n          \n          <div class=\"recommendation-card\">\n            <img loading=\"lazy\" src=\"https://placeholder.pics/svg/150x200/DEDEDE/555555/现代科技\" alt=\"推荐书籍\" class=\"recommendation-image\">\n            <div class=\"recommendation-info\">\n              <h3>现代科技前沿</h3>\n              <p class=\"author\">张明远</p>\n              <div class=\"tags\">\n                <span class=\"tag\">科技</span>\n                <span class=\"tag\">前沿</span>\n              </div>\n            </div>\n          </div>\n          \n          <div class=\"recommendation-card\">\n            <img loading=\"lazy\" src=\"https://placeholder.pics/svg/150x200/DEDEDE/555555/艺术史\" alt=\"推荐书籍\" class=\"recommendation-image\">\n            <div class=\"recommendation-info\">\n              <h3>世界艺术史</h3>\n              <p class=\"author\">李艺</p>\n              <div class=\"tags\">\n                <span class=\"tag\">艺术</span>\n                <span class=\"tag\">历史</span>\n              </div>\n            </div>\n          </div>\n        </div>\n      </section>\n      -->\n      <section class=\"books-section\">\n        <h2 class=\"section-title\">根据你的兴趣推荐</h2>\n        <div class=\"books-grid\">\n          <div \n            v-for=\"book in paginatedBooks\" \n            :key=\"book.id\" \n            class=\"book-card\"\n          >\n            <img loading=\"lazy\" :src=\"book.image\" :alt=\"book.title\" class=\"book-image\">\n            <div class=\"book-info\">\n              <h3 class=\"book-title\">{{ book.title }}</h3>\n              <p class=\"book-author\">{{ book.author }}</p>\n              <div class=\"tags\">\n                <span v-for=\"tag in book.tags\" :key=\"tag\" class=\"tag\">{{ tag }}</span>\n              </div>\n              <div class=\"book-meta\">\n                <span class=\"book-status\" :class=\"`status-${book.status}`\">\n                  {{ book.status === 'available' ? '可借阅' : '已借出' }}\n                </span>\n                <button \n                  class=\"borrow-button\" \n                  @click=\"borrowBook(book.id)\"\n                  :disabled=\"book.status !== 'available'\"\n                >\n                  借阅\n                </button>\n              </div>\n            </div>\n          </div>\n          \n          <div v-if=\"filteredBooks.length === 0\" class=\"no-books\">\n            没有找到匹配的书籍\n          </div>\n        </div>\n<!--     \n        <div class=\"pagination\">\n          <button \n            class=\"pagination-button\" \n            @click=\"prevPage\"\n            :disabled=\"currentPage === 1\"\n          >\n            <i class=\"bi bi-chevron-left\"></i>\n          </button>\n          <span class=\"pagination-page\">{{ currentPage }}</span>\n          <button \n            class=\"pagination-button\" \n            @click=\"nextPage\"\n            :disabled=\"currentPage === totalPages\"\n          >\n            <i class=\"bi bi-chevron-right\"></i>\n          </button>\n        </div>\n-->\n      </section>\n    </main>\n   \n    <!-- 消息提示 -->\n    <div v-if=\"message.show\" class=\"message\" :class=\"`message-${message.type}`\">\n      {{ message.text }}\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'StudentLibrary',\n  components: {\n  },\n  data() {\n    return {\n      // 书籍数据\n      books: [\n        {\n          id: 1,\n          title: \"JavaScript高级程序设计\",\n          author: \"Nicholas C. Zakas\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/JS高级\",\n          tags: [\"编程\", \"前端\"],\n          status: \"available\"\n        },\n        {\n          id: 2,\n          title: \"CSS权威指南\",\n          author: \"Eric A. Meyer\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/CSS权威\",\n          tags: [\"前端\", \"设计\"],\n          status: \"available\"\n        },\n        {\n          id: 3,\n          title: \"深入浅出Node.js\",\n          author: \"朴灵\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/Node.js\",\n          tags: [\"后端\", \"JavaScript\"],\n          status: \"borrowed\"\n        },\n        {\n          id: 4,\n          title: \"算法导论\",\n          author: \"Thomas H. Cormen\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/算法导论\",\n          tags: [\"计算机\", \"算法\"],\n          status: \"available\"\n        },\n        {\n          id: 5,\n          title: \"设计心理学\",\n          author: \"Donald A. Norman\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/设计心理\",\n          tags: [\"设计\", \"心理学\"],\n          status: \"available\"\n        },\n        {\n          id: 6,\n          title: \"人类简史\",\n          author: \"Yuval Noah Harari\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/人类简史\",\n          tags: [\"历史\", \"社会学\"],\n          status: \"available\"\n        },\n        {\n          id: 7,\n          title: \"Python编程：从入门到实践\",\n          author: \"Eric Matthes\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/Python\",\n          tags: [\"编程\", \"Python\"],\n          status: \"borrowed\"\n        },\n        {\n          id: 8,\n          title: \"经济学原理\",\n          author: \"N. Gregory Mankiw\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/经济学\",\n          tags: [\"经济\", \"教材\"],\n          status: \"available\"\n        },\n        {\n          id: 9,\n          title: \"百年孤独\",\n          author: \"加西亚·马尔克斯\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/百年孤独\",\n          tags: [\"文学\", \"经典\"],\n          status: \"available\"\n        },\n        {\n          id: 10,\n          title: \"活着\",\n          author: \"余华\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/活着\",\n          tags: [\"文学\", \"小说\"],\n          status: \"available\"\n        },\n        {\n          id: 11,\n          title: \"三体\",\n          author: \"刘慈欣\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/三体\",\n          tags: [\"科幻\", \"文学\"],\n          status: \"available\"\n        },\n        {\n          id: 12,\n          title: \"数据结构与算法\",\n          author: \"严蔚敏\",\n          image: \"https://placeholder.pics/svg/250x180/DEDEDE/555555/数据结构\",\n          tags: [\"计算机\", \"算法\"],\n          status: \"available\"\n        }\n      ],\n      // 筛选和搜索\n      searchQuery: '',\n      categoryFilter: '',\n      sortBy: '',\n      // 分页\n      currentPage: 1,\n      pageSize: 12,\n      // 消息提示\n      message: {\n        show: false,\n        text: '',\n        type: 'success'\n      }\n    }\n  },\n  computed: {\n    // 筛选后的书籍\n    filteredBooks() {\n      let filtered = this.books\n      \n      // 搜索筛选\n      if (this.searchQuery) {\n        const query = this.searchQuery.toLowerCase()\n        filtered = filtered.filter(book => \n          book.title.toLowerCase().includes(query) || \n          book.author.toLowerCase().includes(query) ||\n          book.tags.some(tag => tag.toLowerCase().includes(query))\n        )\n      }\n      \n      // 分类筛选\n      if (this.categoryFilter) {\n        filtered = filtered.filter(book => \n          book.tags.some(tag => tag === this.categoryFilter)\n        )\n      }\n      \n      return filtered\n    },\n    // 排序后的书籍\n    sortedBooks() {\n      const books = [...this.filteredBooks]\n      \n      switch (this.sortBy) {\n        case 'title':\n          return books.sort((a, b) => a.title.localeCompare(b.title))\n        case 'newest':\n          return books.sort((a, b) => b.id - a.id)\n        case 'popular':\n          // 模拟热门度排序（这里简单使用ID倒序）\n          return books.sort((a, b) => b.id - a.id)\n        default:\n          return books\n      }\n    },\n    // 分页后的书籍\n    paginatedBooks() {\n      const startIndex = (this.currentPage - 1) * this.pageSize\n      return this.sortedBooks.slice(startIndex, startIndex + this.pageSize)\n    },\n    // 总页数\n    totalPages() {\n      return Math.ceil(this.sortedBooks.length / this.pageSize)\n    }\n  },\n  methods: {\n    // 借阅书籍\n    borrowBook(bookId) {\n      const book = this.books.find(b => b.id === bookId)\n      if (book && book.status === 'available') {\n        book.status = 'borrowed'\n        this.showMessage(`成功借阅《${book.title}》`, 'success')\n        \n        // 重置到第一页，确保用户能看到变化\n        this.currentPage = 1\n      }\n    },\n    // 筛选书籍\n    filterBooks() {\n      this.currentPage = 1 // 重置到第一页\n    },\n    // 排序书籍\n    sortBooks() {\n      this.currentPage = 1 // 重置到第一页\n    },\n    // 上一页\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--\n      }\n    },\n    // 下一页\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++\n      }\n    },\n    // 显示消息\n    showMessage(text, type = 'success') {\n      this.message.text = text\n      this.message.type = type\n      this.message.show = true\n      \n      setTimeout(() => {\n        this.message.show = false\n      }, 3000)\n    }\n  },\n  watch: {\n    // 当筛选条件变化时重置页码\n    searchQuery() {\n      this.currentPage = 1\n    },\n    categoryFilter() {\n      this.currentPage = 1\n    }\n  }\n}\n</script>\n\n<style scoped>\n/* CSS变量定义 */\n.student-library {\n  --primary-color: #2A5CAA;\n  --secondary-color: #4CAF50;\n  --background-color: #F8F9FA;\n  --text-color: #333;\n  --light-text: #666;\n  --border-color: #E0E0E0;\n  --card-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\n  --hover-shadow: 0 4px 12px rgba(0, 0, 0, 0.15);\n  --transition: all 0.3s ease;\n  --light-color: #FFFFFF;\n  --gray-color: #F5F5F5;\n  --shadow-color: rgba(0, 0, 0, 0.1);\n  --border-radius: 8px;\n}\n\n.student-library {\n  font-family: 'Roboto', 'PingFang SC', 'Microsoft YaHei', sans-serif;\n  background-color: var(--background-color);\n  color: var(--text-color);\n  margin: 0;\n  padding: 0;\n  line-height: 1.6;\n  min-height: 100vh;\n  display: flex;\n  flex-direction: column;\n}\n\n.library-container {\n  max-width: 1400px;\n  margin: 20px auto;\n  padding: 0 20px;\n  flex: 1;\n  width: 100%;\n}\n\n/* 搜索和筛选区域 - 更新为与AdminLibrary一致的样式 */\n.search-filter-section {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  gap: 1rem;\n  margin-bottom: 2rem;\n  background-color: var(--light-color);\n  padding: 1.5rem;\n  border-radius: var(--border-radius);\n  box-shadow: 0 2px 8px var(--shadow-color);\n}\n\n.search-box {\n  display: flex;\n  flex: 1;\n  min-width: 300px;\n}\n\n.search-box input {\n  flex: 1;\n  padding: 0.75rem 1rem;\n  border: 1px solid var(--border-color);\n  border-radius: var(--border-radius) 0 0 var(--border-radius);\n  font-size: 1rem;\n  transition: var(--transition);\n}\n\n.search-box input:focus {\n  outline: none;\n  border-color: var(--primary-color);\n}\n\n.search-btn {\n  padding: 0 1.25rem;\n  background-color: var(--primary-color);\n  color: var(--light-color);\n  border: none;\n  border-radius: 0 var(--border-radius) var(--border-radius) 0;\n  cursor: pointer;\n  transition: var(--transition);\n}\n\n.search-btn:hover {\n  background-color: #1E4B8B;\n}\n\n.filter-options {\n  display: flex;\n  gap: 1rem;\n}\n\n.filter-options select {\n  padding: 0.75rem;\n  border: 1px solid var(--border-color);\n  border-radius: var(--border-radius);\n  font-size: 1rem;\n  background-color: var(--light-color);\n  transition: var(--transition);\n}\n\n.filter-options select:focus {\n  outline: none;\n  border-color: var(--primary-color);\n}\n\n/* 推荐区域样式 */\n.section-title {\n  font-size: 22px;\n  color: var(--primary-color);\n  margin-bottom: 20px;\n  padding-bottom: 10px;\n  border-bottom: 1px solid var(--border-color);\n}\n\n.recommendation-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));\n  gap: 25px;\n  margin-bottom: 40px;\n  width: 100%;\n}\n\n.recommendation-card {\n  display: flex;\n  background-color: white;\n  border-radius: 8px;\n  overflow: hidden;\n  box-shadow: var(--card-shadow);\n  transition: var(--transition);\n}\n\n.recommendation-card:hover {\n  transform: translateY(-5px);\n  box-shadow: var(--hover-shadow);\n}\n\n.recommendation-image {\n  width: 120px;\n  height: 160px;\n  object-fit: cover;\n}\n\n.recommendation-info {\n  padding: 15px;\n  flex: 1;\n}\n\n.recommendation-info h3 {\n  margin: 0 0 8px 0;\n  font-size: 18px;\n}\n\n.author {\n  color: var(--light-text);\n  font-size: 14px;\n  margin-bottom: 10px;\n}\n\n.tags {\n  display: flex;\n  gap: 8px;\n  margin-top: 10px;\n}\n\n.tag {\n  background-color: #E8F5E9;\n  color: var(--secondary-color);\n  padding: 4px 8px;\n  border-radius: 4px;\n  font-size: 12px;\n}\n\n/* 书籍列表样式 */\n.books-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));\n  gap: 25px;\n  margin-bottom: 30px;\n  width: 100%;\n  max-width: 100%;\n  grid-auto-flow: dense;\n}\n\n.book-card {\n  background-color: white;\n  border-radius: 8px;\n  overflow: hidden;\n  box-shadow: var(--card-shadow);\n  transition: var(--transition);\n  width: 100%;\n}\n\n.book-card:hover {\n  transform: translateY(-5px);\n  box-shadow: var(--hover-shadow);\n}\n\n.book-image {\n  width: 100%;\n  height: 180px;\n  object-fit: cover;\n}\n\n.book-info {\n  padding: 15px;\n}\n\n.book-title {\n  margin: 0 0 8px 0;\n  font-size: 16px;\n  font-weight: 500;\n}\n\n.book-author {\n  color: var(--light-text);\n  font-size: 14px;\n  margin-bottom: 10px;\n}\n\n.book-meta {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-top: 15px;\n}\n\n.book-status {\n  font-size: 12px;\n  padding: 4px 8px;\n  border-radius: 4p