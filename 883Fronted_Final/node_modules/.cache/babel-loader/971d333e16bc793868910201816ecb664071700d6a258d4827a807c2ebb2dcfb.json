duleTitle: currentModuleTitle,\n      resolvePath: resolvePath,\n      isItemActive: isItemActive\n    };\n  }\n};","map":{"version":3,"names":["computed","ref","watch","useRoute","useRouter","name","setup","route","router","courseId","params","newId","value","immediate","inCourseContext","menuConfig","courseSmart","items","path","icon","currentMenuGroups","currentModuleTitle","resolvePath","startsWith","concat","isItemActive","item","fullPath","currentPath","moduleKey","split","includes"],"sources":["D:\\vvvvvvvvvscode_workspace\\fronted\\883Fronted_Final\\src\\components\\layout\\siderbars\\TeaSidebar.vue"],"sourcesContent":["<template>\r\n  <aside class=\"app-sidebar\">\r\n    <div class=\"sidebar-header\">\r\n      <span class=\"sidebar-title\">{{ currentModuleTitle }}</span>\r\n    </div>\r\n\r\n    <nav class=\"sidebar-nav\">\r\n      <div v-for=\"group in currentMenuGroups\" :key=\"group.name\" class=\"menu-group\">\r\n        <div class=\"group-title\">\r\n          {{ group.name }}\r\n        </div>\r\n        <router-link v-for=\"item in group.items\" :key=\"item.path\" :to=\"resolvePath(item.path)\" class=\"menu-item\"\r\n          :class=\"{ active: isItemActive(item) }\">\r\n          <span class=\"item-icon\">{{ item.icon }}</span>\r\n          <span class=\"item-text\">{{ item.name }}</span>\r\n        </router-link>\r\n      </div>\r\n    </nav>\r\n  </aside>\r\n</template>\r\n\r\n<script>\r\nimport { computed, ref, watch } from 'vue'\r\nimport { useRoute, useRouter } from 'vue-router'\r\n\r\nexport default {\r\n  name: 'TeaSidebar',\r\n  setup() {\r\n    const route = useRoute()\r\n    const router = useRouter()\r\n    const courseId = ref('')\r\n\r\n    watch(() => route.params.courseId, (newId) => {\r\n      courseId.value = newId || ''\r\n    }, { immediate: true })\r\n\r\n    const inCourseContext = computed(() => !!courseId.value)\r\n\r\n    // === ËèúÂçïÈÖçÁΩÆÔºöÂ∑¶‰æßÂè™Êîæ‚ÄúÂ§ßÊ®°Âùó‚Äù ===\r\n    const menuConfig = {\r\n      // 1. ËØæÁ®ãÂÜÖËèúÂçï\r\n      courseSmart: [\r\n        {\r\n          name: 'ÊïôÂ≠¶ÁÆ°ÁêÜ',\r\n          items: [\r\n            // ÁÇπÂáªËøô‰∫õÔºåÂéªÂæÄÂêÑËá™ÁöÑÈªòËÆ§È°µÈù¢ÔºàÈ°∂Ê†è‰ºöÊòæÁ§∫Â≠êËèúÂçïÔºâ\r\n            { name: 'ËÄÉÂã§ÁÆ°ÁêÜ', path: 'attendance/launch', icon: ' ' },\r\n            { name: '‰Ωú‰∏öÁÆ°ÁêÜ', path: 'homework/publish', icon: ' ' },\r\n            { name: 'ÊàêÁª©ÁÆ°ÁêÜ', path: 'grades/detail', icon: ' ' },\r\n            { name: 'ËØæÁ®ãËµÑÊ∫ê', path: 'resources/list', icon: ' ' },\r\n            { name: 'Â≠¶ÁîüÁÆ°ÁêÜ', path: 'students/detail', icon: ' ' },\r\n            { name: 'ËØ∑ÂÅáÂÆ°Êâπ', path: 'leave/approval', icon: ' ' } // ÂÅáËÆæ‰Ω†ÊúâËøô‰∏™\r\n          ]\r\n        },\r\n        {\r\n          name: 'ËØæÁ®ãÊ¶ÇÂÜµ',\r\n          items: [\r\n            { name: 'ËØæÁ®ãÈ¶ñÈ°µ', path: 'overview', icon: ' ' },\r\n            { name: 'Áü•ËØÜÂõæË∞±', path: 'learning-path', icon: ' ' },\r\n          ]\r\n        }\r\n      ],\r\n      // 2. ËØæÁ®ãÂ§ñËèúÂçï (ÈùûËØæÁ®ãËØ¶ÊÉÖÈ°µ)\r\n      default: [\r\n        {\r\n          name: 'Â∑•‰ΩúÂè∞',\r\n          items: [\r\n            { name: 'ÊàëÁöÑËØæÁ®ã', path: '/teacher/teachinghome', icon: ' ' },\r\n            { name: 'ËØ∑ÂÅá‰∏≠ÂøÉ', path: '/teacher/leave', icon: 'üìÖ' },\r\n          ]\r\n        },\r\n        {\r\n          name: 'ÂÖ¨ÂÖ±ËµÑÊ∫ê',\r\n          items: [\r\n            { name: 'ÊïôÂÆ§È¢ÑÁ∫¶', path: '/teacher/classroom', icon: 'üèõÔ∏è' },\r\n            { name: 'Âõæ‰π¶È¶Ü', path: '/teacher/library', icon: 'üìñ' }\r\n          ]\r\n        }\r\n      ]\r\n    }\r\n\r\n    const currentMenuGroups = computed(() => {\r\n      if (inCourseContext.value) return menuConfig.courseSmart\r\n      return menuConfig.default\r\n    })\r\n\r\n    const currentModuleTitle = computed(() => {\r\n      if (inCourseContext.value) return 'Êô∫ËÉΩÊïôÂ≠¶Á≥ªÁªü'\r\n      return 'ÊïôÂ∏à‰∏≠ÂøÉ'\r\n    })\r\n\r\n    const resolvePath = (path) => {\r\n      if (!path.startsWith('/')) {\r\n        return `/teacher/course/${courseId.value}/${path}`\r\n      }\r\n      return path\r\n    }\r\n\r\n    // È´ò‰∫ÆÈÄªËæëÔºöÂè™Ë¶ÅÂΩìÂâçË∑ØÁî±ÂåÖÂê´Ëøô‰∏™Ê®°ÂùóÁöÑÂÖ≥ÈîÆËØçÔºå‰æßËæπÊ†èÂ∞±È´ò‰∫Æ\r\n    const isItemActive = (item) => {\r\n      const fullPath = resolvePath(item.path)\r\n      const currentPath = route.path\r\n\r\n      // ÊèêÂèñÊ®°ÂùóÂêç (‰æãÂ¶Ç attendance) ËøõË°åÊ®°Á≥äÂåπÈÖç\r\n      // item.path Ê†ºÂºèÂ¶Ç \"attendance/launch\" -> Âèñ \"attendance\"\r\n      if (inCourseContext.value && !item.path.startsWith('/')) {\r\n        const moduleKey = item.path.split('/')[0]\r\n        // Â¶ÇÊûúÂΩìÂâçË∑ØÂæÑÈáåÂåÖÂê´Ëøô‰∏™Ê®°ÂùóÂêç (e.g. /teacher/course/101/attendance/detail)\r\n        if (currentPath.includes(`/${moduleKey}`)) {\r\n          return true\r\n        }\r\n      }\r\n\r\n      // Á≤æÁ°ÆÂåπÈÖç (Áî®‰∫éÈ¶ñÈ°µÁ≠â)\r\n      return currentPath === fullPath || currentPath.startsWith(fullPath + '/')\r\n    }\r\n\r\n    return {\r\n      currentMenuGroups,\r\n      currentModuleTitle,\r\n      resolvePath,\r\n      isItemActive\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* === ÂÆåÂÖ®Â§çÁî® StuSidebar.vue ÁöÑÊ†∑Âºè === */\r\n.app-sidebar {\r\n  inline-size: 240px;\r\n  background-color: var(--sidebar-bg, #d6e2f5);\r\n  /* Â≠¶ÁîüÁ´ØÁöÑÊ∑°ËìùËâ≤ËÉåÊôØ */\r\n  block-size: calc(100vh - 60px);\r\n  position: fixed;\r\n  inset-block-start: 60px;\r\n  inset-inline-start: 0;\r\n  box-shadow: var(--sidebar-shadow, 2px 0 8px rgba(0, 0, 0, 0.1));\r\n  z-index: 999;\r\n  overflow-y: auto;\r\n}\r\n\r\n.sidebar-header {\r\n  block-size: 50px;\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 0 15px;\r\n  border-block-end: 1px solid rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.sidebar-title {\r\n  font-weight: bold;\r\n  color: var(--primary-color, #2A5CAA);\r\n  font-size: 16px;\r\n}\r\n\r\n.sidebar-nav {\r\n  padding: 15px 0;\r\n}\r\n\r\n.menu-group {\r\n  margin-block-end: 20px;\r\n}\r\n\r\n.group-title {\r\n  padding: 0 15px 8px 15px;\r\n  font-size: 12px;\r\n  col