type=\"text\" placeholder=\"请输入班级ID\" class=\"input\" />\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"homework_id\" class=\"form-label required\">作业ID</label>\r\n            <input id=\"homework_id\" v-model=\"queryForm.homework_id\" type=\"text\" placeholder=\"请输入作业ID\" class=\"input\" />\r\n          </div>\r\n          <div class=\"form-actions-inline\">\r\n            <button type=\"button\" class=\"button\" @click=\"loadSubmissions\" :disabled=\"loading\">\r\n              <span v-if=\"loading\">查询中...</span>\r\n              <span v-else>查询</span>\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- AI教学建议区域 -->\r\n    <section v-if=\"submissions.length > 0\" class=\"card ai-card\">\r\n      <header class=\"card-header\">\r\n        <h3>教学建议</h3>\r\n      </header>\r\n      <div class=\"ai-section\">\r\n        <div class=\"suggestion-inputs\">\r\n          <div class=\"form-group\">\r\n            <label for=\"homeworkTitle\">作业标题</label>\r\n            <input type=\"text\" id=\"homeworkTitle\" v-model=\"homeworkTitle\" class=\"input\" placeholder=\"请输入作业标题\" />\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"homeworkDescription\">作业描述</label>\r\n            <textarea id=\"homeworkDescription\" v-model=\"homeworkDescription\" class=\"form-textarea\" placeholder=\"请输入作业描述\"\r\n              rows=\"3\"></textarea>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"student-submissions\">\r\n          <h4>学生提交摘要</h4>\r\n          <div class=\"submissions-list\">\r\n            <div v-for=\"submission in submissions.slice(0, 5)\" :key=\"submission.submission_id\" class=\"submission-item\">\r\n              <div class=\"submission-header\">\r\n                <span class=\"student-name\">{{ submission.student_name }}</span>\r\n                <span class=\"submission-time\">提交时间：{{ formatTime(submission.submitted_at) }}</span>\r\n              </div>\r\n              <div class=\"submission-content\">\r\n                <p v-if=\"submission.grade !== null\">\r\n                  得分: {{ submission.grade }}\r\n                </p>\r\n                <p v-else class=\"text-muted\">暂未评分</p>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-actions\">\r\n          <button type=\"button\" class=\"button\" @click=\"generateTeachingSuggestions\"\r\n            :disabled=\"generatingSuggestions || !homeworkTitle || !homeworkDescription\">\r\n            {{ generatingSuggestions ? '生成中...' : '生成教学建议' }}\r\n          </button>\r\n        </div>\r\n\r\n        <div v-if=\"generatingSuggestions\" class=\"loading-state\">正在生成教学建议...</div>\r\n\r\n        <div v-else-if=\"errorMessage\" class=\"error-state\">{{ errorMessage }}</div>\r\n\r\n        <div v-else-if=\"teachingSuggestions && teachingSuggestions.length > 0\" class=\"suggestions-result\">\r\n          <h4>生成的教学建议</h4>\r\n          <div class=\"suggestions-list\">\r\n            <div v-for=\"(suggestion, index) in teachingSuggestions\" :key=\"index\" class=\"suggestion-item\">\r\n              <h5>{{ suggestion.title }}</h5>\r\n              <p>{{ suggestion.content }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- 提交列表 -->\r\n    <section v-if=\"submissions.length > 0\" class=\"card submissions-card\">\r\n      <header class=\"card-header\">\r\n        <h3>作业提交列表</h3>\r\n      </header>\r\n      <div class=\"submissions-list\">\r\n        <div v-for=\"submission in submissions\" :key=\"submission.submission_id\" class=\"submission-item\"\r\n          :class=\"{ active: selectedSubmission?.submission_id === submission.submission_id }\"\r\n          @click=\"selectSubmission(submission)\">\r\n          <div class=\"submission-info\">\r\n            <span class=\"student-name\">{{ submission.student_name }}</span>\r\n            <span class=\"submission-time\">提交时间：{{ formatTime(submission.submitted_at) }}</span>\r\n            <span :class=\"['status-badge', `status-${submission.status}`]\">\r\n              {{ getStatusName(submission.status) }}\r\n            </span>\r\n            <span v-if=\"submission.grade !== null\" class=\"grade-badge\">\r\n              得分：{{ submission.grade }}\r\n            </span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- 批改表单 -->\r\n    <section v-if=\"selectedSubmission\" class=\"card grade-card\">\r\n      <header class=\"card-header\">\r\n        <div class=\"card-header__content\">\r\n          <h3>批改作业</h3>\r\n          <span class=\"student-info\">学生：{{ selectedSubmission.student_name }}</span>\r\n        </div>\r\n      </header>\r\n      <form @submit.prevent=\"handleGrade\" class=\"grade-form\">\r\n        <div class=\"form-group\">\r\n          <label for=\"grade\" class=\"form-label required\">作业得分</label>\r\n          <input id=\"grade\" v-model.number=\"gradeForm.grade\" type=\"number\" placeholder=\"请输入得分\" class=\"input\"\r\n            :class=\"{ 'input--error': errors.grade }\" min=\"0\" step=\"0.01\" />\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <label for=\"teacher_feedback\" class=\"form-label\">教师评语</label>\r\n          <textarea id=\"teacher_feedback\" v-model=\"gradeForm.teacher_feedback\" class=\"form-textarea\"\r\n            placeholder=\"请输入评语（可选）\" rows=\"6\"></textarea>\r\n        </div>\r\n\r\n        <div class=\"form-actions\">\r\n          <button type=\"submit\" class=\"button\" :disabled=\"grading\">\r\n            <span v-if=\"grading\">保存中...</span>\r\n            <span v-else>保存批改</span>\r\n          </button>\r\n        </div>\r\n      </form>\r\n    </section>\r\n\r\n    <!-- 空状态 -->\r\n    <section v-if=\"!loading && submissions.length === 0 && queryForm.class_id && queryForm.homework_id\"\r\n      class=\"c