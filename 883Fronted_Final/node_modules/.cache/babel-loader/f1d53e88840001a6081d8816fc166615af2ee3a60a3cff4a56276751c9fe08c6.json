   alert(result.msg || '操作失败');\n              }\n              _context3.n = 8;\n              break;\n            case 7:\n              _context3.p = 7;\n              _t3 = _context3.v;\n              console.error('保存失败:', _t3);\n              alert(_t3.message || '保存失败，请稍后重试');\n            case 8:\n              _context3.p = 8;\n              _this3.loading = false;\n              return _context3.f(8);\n            case 9:\n              return _context3.a(2);\n          }\n        }, _callee3, null, [[2, 7, 8, 9]]);\n      }))();\n    },\n    deleteRole: function deleteRole(roleId) {\n      var _this4 = this;\n      return _asyncToGenerator(/*#__PURE__*/_regenerator().m(function _callee4() {\n        var result, _t4;\n        return _regenerator().w(function (_context4) {\n          while (1) switch (_context4.p = _context4.n) {\n            case 0:\n              if (confirm('确定要删除该角色吗？')) {\n                _context4.n = 1;\n                break;\n              }\n              return _context4.a(2);\n            case 1:\n              _context4.p = 1;\n              _context4.n = 2;\n              return deleteRoleApi(roleId);\n            case 2:\n              result = _context4.v;\n              if (result.code === 1) {\n                alert('删除成功');\n                _this4.loadRoles();\n              } else {\n                alert(result.msg || '删除失败');\n              }\n              _context4.n = 4;\n              break;\n            case 3:\n              _context4.p = 3;\n              _t4 = _context4.v;\n              console.error('删除失败:', _t4);\n              alert(_t4.message || '删除失败，请稍后重试');\n            case 4:\n              return _context4.a(2);\n          }\n        }, _callee4, null, [[1, 3]]);\n      }))();\n    },\n    closeDialog: function closeDialog() {\n      this.showDialog = false;\n    },\n    resetSearch: function resetSearch() {\n      this.searchForm.role_name = '';\n      this.currentPage = 1;\n      this.loadRoles();