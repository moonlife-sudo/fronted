{"ast":null,"code":"import \"core-js/modules/es.error.to-string.js\";\nimport \"core-js/modules/es.array.concat.js\";\nimport \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.date.to-string.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/es.regexp.to-string.js\";\nimport \"core-js/modules/es.string.pad-start.js\";\nimport \"core-js/modules/web.timers.js\";\nimport { ref, computed, onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nexport default {\n  name: 'StuAttendance',\n  setup: function setup() {\n    var route = useRoute();\n    var router = useRouter();\n\n    // ‰ºòÂÖà‰ªé Query ÂèñÔºåÂèñ‰∏çÂà∞‰ªé LocalStorage Âèñ\n    var classId = computed(function () {\n      return route.query.classId || localStorage.getItem('currentClassId');\n    });\n\n    // Ëé∑ÂèñËØæÁ®ãÂêçÁß∞Ôºö‰ºòÂÖà‰ªéË∑ØÁî±ÂèÇÊï∞ÔºåÁÑ∂Âêé‰ªéÊú¨Âú∞Â≠òÂÇ®\n    var currentCourseName = computed(function () {\n      return route.query.courseName || localStorage.getItem('currentCourseName') || 'Êú™Áü•ËØæÁ®ã';\n    });\n    var locationGot = ref(false);\n\n    // ÊºîÁ§∫Êï∞ÊçÆ\n    var hasActiveSession = ref(true);\n    var loading = ref(false);\n    var countdown = ref(300);\n    var submitting = ref(false);\n    var hasCheckedIn = ref(false);\n    var activeSession = ref({\n      session_type: 1,\n      start_time: new Date(),\n      end_time: new Date()\n    });\n    var checkInResult = ref(null);\n    var attendanceRecords = ref([{\n      session_id: 1,\n      session_start_time: new Date(),\n      my_check_in_time: new Date(),\n      my_status: 1\n    }, {\n      session_id: 2,\n      session_start_time: new Date(),\n      my_check_in_time: null,\n      my_status: 2\n    }]);\n\n    // ËøîÂõûËØæÁ®ãÂàóË°®\n    var goBackToCourses = function goBackToCourses() {\n      router.push({\n        path: '/student/teachinghome'\n      });\n    };\n\n    // ÁªÑ‰ª∂ÊåÇËΩΩÊó∂ÔºåÂ¶ÇÊûúÊ≤°ÊúâËØæÁ®ã‰ø°ÊÅØÔºåÂ∞ùËØï‰ªéÊú¨Âú∞Â≠òÂÇ®Ëé∑Âèñ\n    onMounted(function () {\n      if (!route.query.courseName && localStorage.getItem('currentCourseName')) {\n        // Â¶ÇÊûúË∑ØÁî±ÂèÇÊï∞Ê≤°ÊúâËØæÁ®ãÂêçÔºå‰ΩÜÊú¨Âú∞Â≠òÂÇ®ÊúâÔºåÂèØ‰ª•Âú®ËøôÈáåÂ§ÑÁêÜ\n        console.log('‰ªéÊú¨Âú∞Â≠òÂÇ®Âä†ËΩΩËØæÁ®ã‰ø°ÊÅØ:', localStorage.getItem('currentCourseName'));\n      }\n    });\n    var formatCountdown = function formatCountdown(s) {\n      var m = Math.floor(s / 60).toString().padStart(2, '0');\n      var sec = (s % 60).toString().padStart(2, '0');\n      return \"\".concat(m, \":\").concat(sec);\n    };\n    var getSessionTypeText = function getSessionTypeText(t) {\n      var types = ['Êú™Áü•', '‰∫∫ËÑ∏', 'ÂÆö‰Ωç', 'Ê∑∑Âêà'];\n      return types[t] || 'ÊôÆÈÄö';\n    };\n    var formatTime = function formatTime(d) {\n      if (!d) return '--:--';\n      return new Date(d).toLocaleTimeString([], {\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    };\n    var formatDate = function formatDate(d) {\n      if (!d) return '--';\n      return new Date(d).toLocaleDateString();\n    };\n    var getStatusText = function getStatusText(s) {\n      var status = ['Êú™Áü•', 'Âá∫Âã§', 'Áº∫Âã§', 'ËøüÂà∞', 'ËØ∑ÂÅá'];\n      return status[s] || 'Êú™Áü•';\n    };\n    var getStatusClass = function getStatusClass(s) {\n      var classes = ['unknown', 'present', 'absent', 'late', 'leave'];\n      return classes[s] || 'unknown';\n    };\n    var getLocation = function getLocation() {\n      if (navigator.geolocation) {\n        navigator.geolocation.getCurrentPosition(function (position) {\n          console.log('‰ΩçÁΩÆËé∑ÂèñÊàêÂäü:', position);\n          locationGot.value = true;\n        }, function (error) {\n          console.error('‰ΩçÁΩÆËé∑ÂèñÂ§±Ë¥•:', error);\n          alert('‰ΩçÁΩÆËé∑ÂèñÂ§±Ë¥•ÔºåËØ∑Ê£ÄÊü•ÂÆö‰ΩçÊùÉÈôê');\n        });\n      } else {\n        alert('ÊµèËßàÂô®‰∏çÊîØÊåÅÂú∞ÁêÜ‰ΩçÁΩÆÂäüËÉΩ');\n      }\n    };\n    var submitCheckIn = function submitCheckIn() {\n      submitting.value = true;\n      setTimeout(function () {\n        submitting.value = false;\n        hasCheckedIn.value = true;\n        checkInResult.value = {\n          check_in_time: new Date()\n        };\n      }, 1000);\n    };\n    return {\n      classId: classId,\n      currentCourseName: currentCourseName,\n      locationGot: locationGot,\n      hasActiveSession: hasActiveSession,\n      loading: loading,\n      countdown: countdown,\n      submitting: submitting,\n      hasCheckedIn: hasCheckedIn,\n      activeSession: activeSession,\n      checkInResult: checkInResult,\n      attendanceRecords: attendanceRecords,\n      goBackToCourses: goBackToCourses,\n      formatCountdown: formatCountdown,\n      getSessionTypeText: getSessionTypeText,\n      formatTime: formatTime,\n      formatDate: formatDate,\n      getStatusText: getStatusText,\n      getStatusClass: getStatusClass,\n      getLocation: getLocation,\n      submitCheckIn: submitCheckIn\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","useRoute","useRouter","name","setup","route","router","classId","query","localStorage","getItem","currentCourseName","courseName","locationGot","hasActiveSession","loading","countdown","submitting","hasCheckedIn","activeSession","session_type","start_time","Date","end_time","checkInResult","attendanceRecords","session_id","session_start_time","my_check_in_time","my_status","goBackToCourses","push","path","console","log","formatCountdown","s","m","Math","floor","toString","padStart","sec","concat","getSessionTypeText","t","types","formatTime","d","toLocaleTimeString","hour","minute","formatDate","toLocaleDateString","getStatusText","status","getStatusClass","classes","getLocation","navigator","geolocation","getCurrentPosition","position","value","error","alert","submitCheckIn","setTimeout","check_in_time"],"sources":["/Users/zhouzhou/Desktop/883Fronted_Final/src/views/Student/Teaching/StuAttendance.vue"],"sourcesContent":["<template>\n  <div class=\"page-container\">\n    <div class=\"content-header\">\n      <div class=\"header-left\">\n        <h2 class=\"page-title\">ËØæÁ®ãËÄÉÂã§</h2>\n        <div class=\"course-badge\">\n          ÂΩìÂâçËØæÁ®ãÔºö{{ currentCourseName }}\n        </div>\n      </div>\n      <div class=\"header-actions\">\n        <button class=\"back-btn\" @click=\"goBackToCourses\">ËøîÂõûËØæÁ®ãÂàóË°®</button>\n      </div>\n    </div>\n\n    <div class=\"layout-grid\">\n      <div class=\"action-section\">\n        <div class=\"card check-in-card\">\n          <div class=\"card-header\">\n            <h3>üìù ÂÆûÊó∂Á≠æÂà∞</h3>\n            <span v-if=\"hasActiveSession\" class=\"status-badge active\">ËøõË°å‰∏≠</span>\n            <span v-else class=\"status-badge\">ÊöÇÊó†‰ªªÂä°</span>\n          </div>\n\n          <div class=\"card-body\">\n            <div v-if=\"loading\" class=\"state-text\">Ê≠£Âú®Ëé∑ÂèñÁ≠æÂà∞‰ªªÂä°...</div>\n\n            <div v-else-if=\"!hasActiveSession\" class=\"empty-session\">\n              <div class=\"icon-circle\">üí§</div>\n              <p>ÂΩìÂâçÊ≤°ÊúâÊ≠£Âú®ËøõË°åÁöÑËÄÉÂã§</p>\n              <p class=\"sub-text\">ËØ∑ÁïôÊÑèËÄÅÂ∏àÈÄöÁü•</p>\n            </div>\n\n            <div v-else class=\"active-session-content\">\n              <div class=\"timer-display\" :class=\"{ expired: countdown <= 0 }\">\n                <div class=\"timer-label\">{{ countdown > 0 ? 'Ââ©‰ΩôÊó∂Èó¥' : 'Â∑≤Êà™Ê≠¢' }}</div>\n                <div class=\"timer-value\">{{ formatCountdown(countdown) }}</div>\n              </div>\n\n              <div class=\"session-details\">\n                <div class=\"detail-row\">\n                  <span>Á±ªÂûã</span>\n                  <span>{{ getSessionTypeText(activeSession?.session_type) }}</span>\n                </div>\n                <div class=\"detail-row\">\n                  <span>Êó∂Èó¥</span>\n                  <span>{{ formatTime(activeSession?.start_time) }} - {{ formatTime(activeSession?.end_time) }}</span>\n                </div>\n              </div>\n\n              <div v-if=\"!hasCheckedIn\" class=\"action-area\">\n                <button v-if=\"[2, 3].includes(activeSession?.session_type)\" type=\"button\" class=\"tool-btn\"\n                  @click=\"getLocation\" :class=\"{ 'success': locationGot }\">\n                  <i class=\"icon\">üìç</i> {{ locationGot ? 'ÂÆö‰ΩçÂ∑≤Ëé∑Âèñ' : 'Ëé∑ÂèñÂÆö‰Ωç' }}\n                </button>\n\n                <button class=\"primary-btn block-btn\" @click=\"submitCheckIn\" :disabled=\"submitting || countdown <= 0\">\n                  {{ submitting ? 'Ê≠£Âú®Êèê‰∫§...' : 'Á´ãÂç≥Á≠æÂà∞' }}\n                </button>\n              </div>\n\n              <div v-else class=\"success-feedback\">\n                <i class=\"icon-success\">‚úÖ</i>\n                <p>ÊÇ®Â∑≤ÊàêÂäüÁ≠æÂà∞</p>\n                <span class=\"time-stamp\">{{ formatTime(checkInResult?.check_in_time) }}</span>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"history-section\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <h3>üìä ËÄÉÂã§ËÆ∞ÂΩï</h3>\n          </div>\n          <div class=\"table-container\">\n            <table class=\"custom-table\">\n              <thead>\n                <tr>\n                  <th>Êó•Êúü</th>\n                  <th>Êó∂Èó¥</th>\n                  <th>Áä∂ÊÄÅ</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-for=\"record in attendanceRecords\" :key=\"record.session_id\">\n                  <td>{{ formatDate(record.session_start_time) }}</td>\n                  <td>{{ record.my_check_in_time ? formatTime(record.my_check_in_time) : '--:--' }}</td>\n                  <td>\n                    <span :class=\"['status-tag', getStatusClass(record.my_status)]\">\n                      {{ getStatusText(record.my_status) }}\n                    </span>\n                  </td>\n                </tr>\n                <tr v-if=\"attendanceRecords.length === 0\">\n                  <td colspan=\"3\" class=\"text-center\">ÊöÇÊó†ËÆ∞ÂΩï</td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, computed, onMounted } from 'vue'\nimport { useRoute, useRouter } from 'vue-router'\n\nexport default {\n  name: 'StuAttendance',\n  setup() {\n    const route = useRoute()\n    const router = useRouter()\n    \n    // ‰ºòÂÖà‰ªé Query ÂèñÔºåÂèñ‰∏çÂà∞‰ªé LocalStorage Âèñ\n    const classId = computed(() => route.query.classId || localStorage.getItem('currentClassId'))\n    \n    // Ëé∑ÂèñËØæÁ®ãÂêçÁß∞Ôºö‰ºòÂÖà‰ªéË∑ØÁî±ÂèÇÊï∞ÔºåÁÑ∂Âêé‰ªéÊú¨Âú∞Â≠òÂÇ®\n    const currentCourseName = computed(() => {\n      return route.query.courseName || localStorage.getItem('currentCourseName') || 'Êú™Áü•ËØæÁ®ã'\n    })\n    \n    const locationGot = ref(false)\n\n    // ÊºîÁ§∫Êï∞ÊçÆ\n    const hasActiveSession = ref(true)\n    const loading = ref(false)\n    const countdown = ref(300)\n    const submitting = ref(false)\n    const hasCheckedIn = ref(false)\n    const activeSession = ref({ session_type: 1, start_time: new Date(), end_time: new Date() })\n    const checkInResult = ref(null)\n    const attendanceRecords = ref([\n      { session_id: 1, session_start_time: new Date(), my_check_in_time: new Date(), my_status: 1 },\n      { session_id: 2, session_start_time: new Date(), my_check_in_time: null, my_status: 2 },\n    ])\n\n    // ËøîÂõûËØæÁ®ãÂàóË°®\n    const goBackToCourses = () => {\n      router.push({ path: '/student/teachinghome' })\n    }\n\n    // ÁªÑ‰ª∂ÊåÇËΩΩÊó∂ÔºåÂ¶ÇÊûúÊ≤°ÊúâËØæÁ®ã‰ø°ÊÅØÔºåÂ∞ùËØï‰ªéÊú¨Âú∞Â≠òÂÇ®Ëé∑Âèñ\n    onMounted(() => {\n      if (!route.query.courseName && localStorage.getItem('currentCourseName')) {\n        // Â¶ÇÊûúË∑ØÁî±ÂèÇÊï∞Ê≤°ÊúâËØæÁ®ãÂêçÔºå‰ΩÜÊú¨Âú∞Â≠òÂÇ®ÊúâÔºåÂèØ‰ª•Âú®ËøôÈáåÂ§ÑÁêÜ\n        console.log('‰ªéÊú¨Âú∞Â≠òÂÇ®Âä†ËΩΩËØæÁ®ã‰ø°ÊÅØ:', localStorage.getItem('currentCourseName'))\n      }\n    })\n\n    const formatCountdown = (s) => {\n      const m = Math.floor(s / 60).toString().padStart(2, '0')\n      const sec = (s % 60).toString().padStart(2, '0')\n      return `${m}:${sec}`\n    }\n\n    const getSessionTypeText = (t) => {\n      const types = ['Êú™Áü•', '‰∫∫ËÑ∏', 'ÂÆö‰Ωç', 'Ê∑∑Âêà']\n      return types[t] || 'ÊôÆÈÄö'\n    }\n\n    const formatTime = (d) => {\n      if (!d) return '--:--'\n      return new Date(d).toLocaleTimeString([], { hour: '2-digit', minute: '2-digit' })\n    }\n\n    const formatDate = (d) => {\n      if (!d) return '--'\n      return new Date(d).toLocaleDateString()\n    }\n\n    const getStatusText = (s) => {\n      const status = ['Êú™Áü•', 'Âá∫Âã§', 'Áº∫Âã§', 'ËøüÂà∞', 'ËØ∑ÂÅá']\n      return status[s] || 'Êú™Áü•'\n    }\n\n    const getStatusClass = (s) => {\n      const classes = ['unknown', 'present', 'absent', 'late', 'leave']\n      return classes[s] || 'unknown'\n    }\n\n    const getLocation = () => {\n      if (navigator.geolocation) {\n        navigator.geolocation.getCurrentPosition(\n          (position) => {\n            console.log('‰ΩçÁΩÆËé∑ÂèñÊàêÂäü:', position)\n            locationGot.value = true\n          },\n          (error) => {\n            console.error('‰ΩçÁΩÆËé∑ÂèñÂ§±Ë¥•:', error)\n            alert('‰ΩçÁΩÆËé∑ÂèñÂ§±Ë¥•ÔºåËØ∑Ê£ÄÊü•ÂÆö‰ΩçÊùÉÈôê')\n          }\n        )\n      } else {\n        alert('ÊµèËßàÂô®‰∏çÊîØÊåÅÂú∞ÁêÜ‰ΩçÁΩÆÂäüËÉΩ')\n      }\n    }\n\n    const submitCheckIn = () => {\n      submitting.value = true\n      setTimeout(() => {\n        submitting.value = false\n        hasCheckedIn.value = true\n        checkInResult.value = { check_in_time: new Date() }\n      }, 1000)\n    }\n\n    return {\n      classId,\n      currentCourseName,\n      locationGot,\n      hasActiveSession,\n      loading,\n      countdown,\n      submitting,\n      hasCheckedIn,\n      activeSession,\n      checkInResult,\n      attendanceRecords,\n      goBackToCourses,\n      formatCountdown,\n      getSessionTypeText,\n      formatTime,\n      formatDate,\n      getStatusText,\n      getStatusClass,\n      getLocation,\n      submitCheckIn\n    }\n  }\n}\n</script>\n\n<style scoped>\n.page-container {\n  padding: 24px;\n  max-width: 1200px;\n  margin: 0 auto;\n}\n\n.content-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: flex-start;\n  margin-bottom: 24px;\n  gap: 20px;\n}\n\n.header-left {\n  flex: 1;\n}\n\n.page-title {\n  font-size: 24px;\n  color: #333;\n  font-weight: 600;\n  margin-bottom: 8px;\n}\n\n.course-badge {\n  background: #e6f7ff;\n  color: #1890ff;\n  padding: 6px 12px;\n  border-radius: 20px;\n  font-size: 14px;\n  font-weight: 500;\n  display: inline-block;\n}\n\n.header-actions {\n  flex-shrink: 0;\n}\n\n.back-btn {\n  padding: 8px 16px;\n  border: 1px solid #ddd;\n  background: white;\n  border-radius: 4px;\n  cursor: pointer;\n  font-size: 14px;\n}\n\n.back-btn:hover {\n  background: #f5f5f5;\n}\n\n.layout-grid {\n  display: grid;\n  grid-template-columns: 400px 1fr;\n  gap: 24px;\n  align-items: start;\n}\n\n@media (max-width: 900px) {\n  .layout-grid {\n    grid-template-columns: 1fr;\n  }\n  \n  .content-header {\n    flex-direction: column;\n    align-items: flex-start;\n    gap: 12px;\n  }\n}\n\n/* Card Styles */\n.card {\n  background: #fff;\n  border-radius: 12px;\n  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.05);\n  overflow: hidden;\n  border: 1px solid #eee;\n}\n\n.card-header {\n  padding: 16px 20px;\n  border-bottom: 1px solid #f0f0f0;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.card-header h3 {\n  font-size: 16px;\n  font-weight: 600;\n  margin: 0;\n}\n\n.card-body {\n  padding: 24px;\n}\n\n/* Active Session Styles */\n.timer-display {\n  text-align: center;\n  margin-bottom: 24px;\n  padding: 16px;\n  background: #f9f9f9;\n  border-radius: 8px;\n}\n\n.timer-value {\n  font-size: 36px;\n  font-weight: 700;\n  color: var(--primary-color, #2A5CAA);\n  font-family: monospace;\n}\n\n.timer-display.expired .timer-value {\n  color: #999;\n}\n\n.session-details {\n  margin-bottom: 24px;\n}\n\n.detail-row {\n  display: flex;\n  justify-content: space-between;\n  padding: 8px 0;\n  border-bottom: 1px dashed #eee;\n  color: #666;\n  font-size: 14px;\n}\n\n.empty-session {\n  text-align: center;\n  color: #999;\n  padding: 40px 0;\n}\n\n.icon-circle {\n  font-size: 40px;\n  margin-bottom: 16px;\n  background: #f5f5f5;\n  width: 80px;\n  height: 80px;\n  line-height: 80px;\n  border-radius: 50%;\n  margin: 0 auto 16px;\n}\n\n/* Buttons */\n.primary-btn {\n  background: var(--primary-color, #2A5CAA);\n  color: #fff;\n  border: none;\n  padding: 12px;\n  border-radius: 6px;\n  font-weight: 600;\n  cursor: pointer;\n  transition: opacity 0.2s;\n}\n\n.primary-btn:hover {\n  opacity: 0.9;\n}\n\n.primary-btn:disabled {\n  background: #ccc;\n  cursor: not-allowed;\n}\n\n.block-btn {\n  width: 100%;\n  display: block;\n}\n\n.tool-btn {\n  width: 100%;\n  padding: 10px;\n  margin-bottom: 12px;\n  background: #fff;\n  border: 1px solid #ddd;\n  border-radius: 6px;\n  color: #666;\n  cursor: pointer;\n}\n\n.tool-btn.success {\n  border-color: #52c41a;\n  color: #52c41a;\n  background: #f6ffed;\n}\n\n/* Table Styles */\n.custom-table {\n  width: 100%;\n  border-collapse: collapse;\n}\n\n.custom-table th {\n  text-align: start;\n  padding: 12px 16px;\n  background: #f8f9fa;\n  color: #666;\n  font-weight: 500;\n  font-size: 13px;\n}\n\n.custom-table td {\n  padding: 12px 16px;\n  border-bottom: 1px solid #f0f0f0;\n  color: #333;\n  font-size: 14px;\n}\n\n.status-tag {\n  padding: 2px 8px;\n  border-radius: 4px;\n  font-size: 12px;\n}\n\n.status-tag.present {\n  color: #52c41a;\n  background: #f6ffed;\n}\n\n.status-tag.absent {\n  color: #ff4d4f;\n  background: #fff1f0;\n}\n\n.status-tag.late {\n  color: #faad14;\n  background: #fffbe6;\n}\n\n.status-tag.leave {\n  color: #1890ff;\n  background: #e6f7ff;\n}\n\n.success