    const inCourseContext = computed(() => !!courseId.value)\r\n\r\n    // ËèúÂçïÈÖçÁΩÆ\r\n    const menuConfig = {\r\n      // 1. ËØæÁ®ãÂÜÖËèúÂçï\r\n      courseSmart: [\r\n        {\r\n          name: 'ÊïôÂ≠¶‰∏≠ÂøÉ',\r\n          children: [\r\n            { name: 'ËØæÁ®ãÊ¶ÇËßà', path: 'overview', icon: 'üè†' },\r\n            { name: 'Áü•ËØÜÂõæË∞±', path: 'learning-path', icon: 'üó∫Ô∏è' },\r\n            { name: 'ÂèëËµ∑Á≠æÂà∞', path: 'attendance/launch', icon: 'üìç' },\r\n            { name: 'ËÄÉÂã§ËØ¶ÊÉÖ', path: 'attendance/detail', icon: 'üìä' },\r\n            { name: 'ÂèëÂ∏É‰Ωú‰∏ö', path: 'homework/publish', icon: 'üìù' },\r\n            { name: 'ÊâπÊîπ‰Ωú‰∏ö', path: 'homework/submissions', icon: '‚úçÔ∏è' },\r\n            { name: 'ÊàêÁª©ÁÆ°ÁêÜ', path: 'grades/detail', icon: 'üìà' },\r\n            { name: 'ËØæÁ®ãËµÑÊ∫ê', path: 'resources/list', icon: 'üìÇ' },\r\n            { name: 'Â≠¶ÁîüÂàóË°®', path: 'students/detail', icon: 'üë®‚Äçüéì' },\r\n            { name: 'ËØ∑ÂÅáÂÆ°Êâπ', path: 'leave/approval', icon: '‚úÖ' }\r\n          ]\r\n        }\r\n      ],\r\n      // 2. ËµÑÊ∫ê‰∏≠ÂøÉËèúÂçï\r\n      resource: [\r\n        {\r\n          name: 'ËµÑÊ∫êÁÆ°ÁêÜ',\r\n          children: [\r\n            { name: 'ËµÑÊ∫êÊ¶ÇËßà', path: '/teacher/resourcehome', icon: 'üèõÔ∏è' },\r\n            { name: 'ÊïôÂÆ§È¢ÑÁ∫¶', path: '/teacher/classroom', icon: 'üè´' },\r\n            { name: 'Âõæ‰π¶È¶Ü', path: '/teacher/library', icon: 'üìñ' }\r\n          ]\r\n        }\r\n      ],\r\n      // 3. ÈªòËÆ§/ÊïôÂ≠¶È¶ñÈ°µËèúÂçï\r\n      teachingDefault: [\r\n        {\r\n          name: 'ÊïôÂ≠¶Â∑•‰Ωú',\r\n          children: [\r\n            { name: 'ÊàëÁöÑÊéàËØæ', path: '/teacher/teachinghome', icon: 'üìö' }\r\n          ]\r\n        }\r\n      ]\r\n    }\r\n\r\n    const currentMenuGroups = computed(() => {\r\n      const path = route.path\r\n\r\n      // A. ËØæÁ®ãÂÜÖ -> ‰ºòÂÖàÁ∫ßÊúÄÈ´ò\r\n      if (inCourseContext.value) {\r\n        return menuConfig.courseSmart\r\n      }\r\n\r\n      // B. ËµÑÊ∫ê‰∏≠ÂøÉ\r\n      if (path.includes('/teacher/resourcehome') || path.includes('/teacher/classroom') || path.includes('/teacher/library')) {\r\n        return menuConfig.resource\r\n      }\r\n\r\n      // C. ÈªòËÆ§ÊòæÁ§∫ÊïôÂ≠¶\r\n      return menuConfig.teachingDefault\r\n    })\r\n\r\n    const currentModuleTitle = computed(() => {\r\n      if (inCourseContext.value) return 'Êô∫ËÉΩÊïôÂ≠¶'\r\n      if (route.path.includes('resource') || route.path.includes('library') || route.path.includes('classroom')) return 'ËµÑÊ∫ê‰∏≠ÂøÉ'\r\n      return 'ÊïôÂ≠¶ÁÆ°ÁêÜ'\r\n    })\r\n\r\n    const resolvePath = (path) => {\r\n      if (!path.startsWith('/')) {\r\n        return `/teacher/course/${courseId.value}/${path}`\r\n      }\r\n      return path\r\n    }\r\n\r\n    const isItemActive = (item) => {\r\n      const fullPath = resolvePath(item.path)\r\n      return route.path === fullPath || route.path.startsWith(fullPath)\r\n    }\r\n\r\n    return {\r\n      currentMenuGroups,\r\n      currentModuleTitle,\r\n      resolvePath,\r\n      isItemActive\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.app-sidebar {\r\n  inline-size: 220px;\r\n  /* ‰øùÊåÅ‰∏é Layout ‰∏≠‰∏ÄËá¥ */\r\n  background-color: #ffffff;\r\n  block-size: calc(100vh - 60px);\r\n  position: fixed;\r\n  inset-block-start: 60px;\r\n  inset-inline-start: 0;\r\n  box-shadow: 2px 0 8px rgba(0, 0, 0, 0.05);\r\n  z-index: 99;\r\n  overflow-y: auto;\r\n}\r\n\r\n.sidebar-header {\r\n  block-size: 50px;\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 0 20px;\r\n  border-block-end: 1px solid #f0f0f0;\r\n  background-color: #fafafa;\r\n}\r\n\r\n.sidebar-title {\r\n  font-weight: 600;\r\n  color: #2A5CAA;\r\n  font-size: 14px;\r\n  letter-spacing: 1px;\r\n}\r\n\r\n.sidebar-nav {\r\n  padding: 16px 0;\r\n}\r\n\r\n.menu-group {\r\n  margin-block-end: 24px;\r\n}\r\n\r\n.group-title {\r\n  padding: 0 20px 8px 20px;\r\n  font-size: 12px;\r\n  color: #999;\r\n  font-weight: 600;