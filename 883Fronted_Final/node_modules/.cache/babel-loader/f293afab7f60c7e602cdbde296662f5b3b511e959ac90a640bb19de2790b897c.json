}\n              return _context.a(2);\n            case 1:\n              // 验证所有必填项\n              requiredFields = ['sleepTime', 'wakeUpTime', 'nap', 'smoking', 'mindSmoking', 'gaming', 'headphone', 'chatting', 'guest', 'activity', 'cleanliness', 'organization', 'mindMessy', 'quietStudy', 'noiseTolerance', 'eatingInRoom', 'fragrance', 'mindSmell'];\n              _i = 0, _requiredFields = requiredFields;\n            case 2:\n              if (!(_i < _requiredFields.length)) {\n                _context.n = 4;\n                break;\n              }\n              field = _requiredFields[_i];\n              if (_this.questionnaire[field]) {\n                _context.n = 3;\n                break;\n              }\n              alert('请完成所有必填问题');\n              return _context.a(2);\n            case 3:\n              _i++;\n              _context.n = 2;\n              break;\n            case 4:\n              _this.isSubmitting = true;\n              _context.p = 5;\n              // 转换表单数据为API格式\n              surveyData = {\n                sleep_time_pref: _this.questionnaire.sleepTime === 'before12' ? 1 : 2,\n                wake_time_pref: _this.questionnaire.wakeUpTime === 'before7' ? 1 : 2,\n                nap_habit: _this.questionnaire.nap === 'yes' ? 1 : 2,\n                smoking_status: _this.questionnaire.smoking === 'yes' ? 1 : 2,\n                roommate_smoke_accept: _this.questionnaire.mindSmoking === 'no' ? 1 : 2,\n                gaming_freq: _this.questionnaire.gaming === 'often' ? 1 : 2,\n                headphone_usage: _this.questionnaire.headphone === 'often' ? 1 : 2,\n                chatting_pref: _this.questionnaire.chatting === 'like' ? 1 : 2,\n                guest_acceptance: _this.questionnaire.guest === 'accept' ? 1 : 2,\n                group_activity_willingness: _this.questionnaire.activity === 'willing' ? 1 : 2,\n                hygiene_requirement: _this.questionnaire.cleanliness === 'strict' ? 1 : _this.questionnaire.cleanliness === 'normal' ? 2 : 3,\n                organization_level: _this.questionnaire.organization === 'high' ? 1 : 2,\n                roommate_hygiene_tolerance: _this.questionnaire.mindMessy === 'notMind' ? 1 : 2,\n                quiet_study_need: _this.questionnaire.quietStudy === 'yes' ? 1 : 2,\n                noise_tolerance_level: _this.questionnaire.noiseTolerance === 'low' ? 1 : 2,\n                dorm_food_freq: _this.questionnaire.eatingInRoom === 'often' ? 1 : 2,\n                fragrance_usage: _this.questionnaire.fragrance === 'often' ? 1 : 2,\n                smell_sensitivity: _this.questionnaire.mindSmell === 'mind' ? 1 : 2\n              }; // 如果有具体时间，可以添加\n              if (_this.questionnaire.sleepTime === 'before12') {\n                surveyData.sleep_time = '23:00:00';\n              } else {\n                surveyData.sleep_time = '00:30:00';\n              }\n              if (_this.questionnaire.wakeUpTime === 'before7') {\n                surveyData.wake_up_time = '06:30:00';\n              } else {\n                surveyData.wake_up_time = '07:30:00';\n              }\n              _context.n = 6;\n              return submitDormSurvey(surveyData);\n            case 6:\n              result = _context.v;\n              if (!(result.code === 1)) {\n                _context.n = 8;\n                break;\n              }\n              alert('问卷提交成功！');\n              // 重新加载问卷数据\n              _context.n = 7;\n              return _this.loadQuestionnaire();\n            case 7:\n              _context.n = 9;\n              break;\n            case 8:\n              alert(result.msg || '提交失败，请重试');\n            case 9:\n              _context.n = 11;\n              break;\n            case 10:\n              _context.p = 10;\n              _t = _context.v;\n              console.error('提交问卷失败:', _t);\n              alert(_t.message || '提交失败，请稍后重试');\n            case 11:\n              _context.p = 11;\n              _this.isSubmit