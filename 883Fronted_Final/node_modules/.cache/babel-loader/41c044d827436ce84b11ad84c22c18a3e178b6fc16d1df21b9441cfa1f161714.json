{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nexport default {\n  name: 'AdminApprove',\n  data() {\n    return {\n      searchTerm: '',\n      statusFilter: 'pending',\n      dateFilter: 'all',\n      currentPage: 1,\n      applications: [{\n        id: 1,\n        type: '教室预约申请',\n        applicant: '张老师',\n        location: '教学楼A-201',\n        date: '2023-11-15',\n        time: '14:00 - 16:00',\n        purpose: '计算机科学研讨会',\n        status: 'pending'\n      }, {\n        id: 2,\n        type: '教室预约申请',\n        applicant: '李教授',\n        location: '图书馆报告厅',\n        date: '2023-11-18',\n        time: '09:00 - 12:00',\n        purpose: '学术讲座',\n        status: 'pending'\n      }, {\n        id: 3,\n        type: '教室预约申请',\n        applicant: '王同学',\n        location: '实验楼B-105',\n        date: '2023-11-20',\n        time: '16:00 - 18:00',\n        purpose: '学生社团活动',\n        status: 'pending'\n      }]\n    };\n  },\n  computed: {\n    filteredApplications() {\n      return this.applications.filter(app => {\n        // 搜索过滤\n        const searchMatch = !this.searchTerm || Object.values(app).some(value => String(value).toLowerCase().includes(this.searchTerm.toLowerCase()));\n\n        // 状态过滤\n        const statusMatch = this.statusFilter === 'all' || app.status === this.statusFilter;\n\n        // 日期过滤\n        let dateMatch = true;\n        if (this.dateFilter !== 'all') {\n          const today = new Date();\n          const appDate = new Date(app.date);\n          if (this.dateFilter === 'today') {\n            dateMatch = appDate.toDateString() === today.toDateString();\n          } else if (this.dateFilter === 'week') {\n            const weekStart = new Date(today.setDate(today.getDate() - today.getDay()));\n            const weekEnd = new Date(today.setDate(today.getDate() + 6));\n            dateMatch = appDate >= weekStart && appDate <= weekEnd;\n          } else if (this.dateFilter === 'month') {\n            dateMatch = appDate.getMonth() === today.getMonth() && appDate.getFullYear() === today.getFullYear();\n          }\n        }\n        return searchMatch && statusMatch && dateMatch;\n      });\n    }\n  },\n  methods: {\n    getStatusText(status) {\n      const statusMap = {\n        pending: '待审批',\n        approved: '已批准',\n        rejected: '已拒绝'\n      };\n      return statusMap[status] || status;\n    },\n    handleSearch() {\n      // 搜索逻辑已经在计算属性中处理\n    },\n    applyFilters() {\n      // 筛选逻辑已经在计算属性中处理\n    },\n    approveApplication(id) {\n      const app = this.applications.find(a => a.id === id);\n      if (app && confirm('确定批准此申请吗？')) {\n        app.status = 'approved';\n      }\n    },\n    rejectApplication(id) {\n      const app = this.applications.find(a => a.id === id);\n      if (app && confirm('确定拒绝此申请吗？')) {\n        app.status = 'rejected';\n      }\n    },\n    undoApplication(id) {\n      const app = this.applications.find(a => a.id === id);\n      if (app) {\n        app.status = 'pending';\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n        this.loadPageData();\n      }\n    },\n    nextPage() {\n      this.currentPage++;\n      this.loadPageData();\n    },\n    loadPageData() {\n      // 模拟加载更多数据\n      if (this.currentPage > 1) {\n        const newApp = {\n          id: this.applications.length + 1,\n          type: '教室预约申请',\n          applicant: '新加载用户',\n          location: '新加载教室',\n          date: '2023-11-25',\n          time: '10:00 - 12:00',\n          purpose: '新加载用途',\n          status: 'pending'\n        };\n        this.applications.push(newApp);\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","searchTerm","statusFilter","dateFilter","currentPage","applications","id","type","applicant","location","date","time","purpose","status","computed","filteredApplications","filter","app","searchMatch","Object","values","some","value","String","toLowerCase","includes","statusMatch","dateMatch","today","Date","appDate","toDateString","weekStart","setDate","getDate","getDay","weekEnd","getMonth","getFullYear","methods","getStatusText","statusMap","pending","approved","rejected","handleSearch","applyFilters","approveApplication","find","a","confirm","rejectApplication","undoApplication","prevPage","loadPageData","nextPage","newApp","length","push"],"sources":["/Users/zhouzhou/Desktop/resource/src/views/AdminApprove.vue"],"sourcesContent":["<template>\n  <div class=\"admin-approve\">\n    <div class=\"approve-container\">\n      <h1 class=\"page-title\">预约审批</h1>\n      \n      <div class=\"search-filter-container\">\n        <div class=\"search-box\">\n          <i class=\"bi bi-search\"></i>\n          <input \n            type=\"text\" \n            placeholder=\"搜索申请人、教室或日期...\" \n            v-model=\"searchTerm\"\n            @input=\"handleSearch\"\n          >\n        </div>\n        \n        <div class=\"filter-group\">\n          <select v-model=\"statusFilter\" @change=\"applyFilters\">\n            <option value=\"all\">所有状态</option>\n            <option value=\"pending\" selected>待审批</option>\n            <option value=\"approved\">已批准</option>\n            <option value=\"rejected\">已拒绝</option>\n          </select>\n          \n          <select v-model=\"dateFilter\" @change=\"applyFilters\">\n            <option value=\"all\">所有日期</option>\n            <option value=\"today\">今天</option>\n            <option value=\"week\">本周</option>\n            <option value=\"month\">本月</option>\n          </select>\n        </div>\n      </div>\n      \n      <div class=\"approval-list\">\n        <div \n          class=\"approval-card\" \n          v-for=\"application in filteredApplications\" \n          :key=\"application.id\"\n        >\n          <div class=\"card-header\">\n            <h3>{{ application.type }}</h3>\n            <span class=\"badge\" :class=\"application.status\">{{ getStatusText(application.status) }}</span>\n          </div>\n          \n          <div class=\"card-content\">\n            <div class=\"info-row\">\n              <i class=\"bi bi-person\"></i>\n              <span>申请人: {{ application.applicant }}</span>\n            </div>\n            <div class=\"info-row\">\n              <i class=\"bi bi-building\"></i>\n              <span>教室: {{ application.location }}</span>\n            </div>\n            <div class=\"info-row\">\n              <i class=\"bi bi-calendar\"></i>\n              <span>日期: {{ application.date }}</span>\n            </div>\n            <div class=\"info-row\">\n              <i class=\"bi bi-clock\"></i>\n              <span>时间: {{ application.time }}</span>\n            </div>\n            <div class=\"info-row\">\n              <i class=\"bi bi-journal-text\"></i>\n              <span>用途: {{ application.purpose }}</span>\n            </div>\n          </div>\n          \n          <div class=\"card-actions\">\n            <button \n              v-if=\"application.status === 'pending'\" \n              class=\"btn approve-btn\" \n              @click=\"approveApplication(application.id)\"\n            >\n              <i class=\"bi bi-check-circle\"></i> 批准\n            </button>\n            <button \n              v-if=\"application.status === 'pending'\" \n              class=\"btn reject-btn\" \n              @click=\"rejectApplication(application.id)\"\n            >\n              <i class=\"bi bi-x-circle\"></i> 拒绝\n            </button>\n            <button \n              v-if=\"application.status !== 'pending'\" \n              class=\"btn undo-btn\" \n              @click=\"undoApplication(application.id)\"\n            >\n              <i class=\"bi bi-arrow-counterclockwise\"></i> 撤销\n            </button>\n          </div>\n        </div>\n      </div>\n      \n      <div class=\"pagination\">\n        <button class=\"btn pagination-btn\" :disabled=\"currentPage === 1\" @click=\"prevPage\">\n          <i class=\"bi bi-chevron-left\"></i>\n        </button>\n        <span class=\"current-page\">{{ currentPage }}</span>\n        <button class=\"btn pagination-btn\" @click=\"nextPage\">\n          <i class=\"bi bi-chevron-right\"></i>\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'AdminApprove',\n  data() {\n    return {\n      searchTerm: '',\n      statusFilter: 'pending',\n      dateFilter: 'all',\n      currentPage: 1,\n      applications: [\n        {\n          id: 1,\n          type: '教室预约申请',\n          applicant: '张老师',\n          location: '教学楼A-201',\n          date: '2023-11-15',\n          time: '14:00 - 16:00',\n          purpose: '计算机科学研讨会',\n          status: 'pending'\n        },\n        {\n          id: 2,\n          type: '教室预约申请',\n          applicant: '李教授',\n          location: '图书馆报告厅',\n          date: '2023-11-18',\n          time: '09:00 - 12:00',\n          purpose: '学术讲座',\n          status: 'pending'\n        },\n        {\n          id: 3,\n          type: '教室预约申请',\n          applicant: '王同学',\n          location: '实验楼B-105',\n          date: '2023-11-20',\n          time: '16:00 - 18:00',\n          purpose: '学生社团活动',\n          status: 'pending'\n        }\n      ]\n    }\n  },\n  computed: {\n  filteredApplications() {\n    return this.applications.filter(app => {\n      // 搜索过滤\n      const searchMatch = !this.searchTerm || \n        Object.values(app).some(value => \n          String(value).toLowerCase().includes(this.searchTerm.toLowerCase())\n        )\n      \n      // 状态过滤\n      const statusMatch = this.statusFilter === 'all' || app.status === this.statusFilter\n      \n      // 日期过滤\n      let dateMatch = true\n      if (this.dateFilter !== 'all') {\n        const today = new Date()\n        const appDate = new Date(app.date)\n        \n        if (this.dateFilter === 'today') {\n          dateMatch = appDate.toDateString() === today.toDateString()\n        } else if (this.dateFilter === 'week') {\n          const weekStart = new Date(today.setDate(today.getDate() - today.getDay()))\n          const weekEnd = new Date(today.setDate(today.getDate() + 6))\n          dateMatch = appDate >= weekStart && appDate <= weekEnd\n        } else if (this.dateFilter === 'month') {\n          dateMatch = appDate.getMonth() === today.getMonth() && \n                     appDate.getFullYear() === today.getFullYear()\n        }\n      }\n      \n      return searchMatch && statusMatch && dateMatch\n    })\n  }\n},\n  methods: {\n    getStatusText(status) {\n      const statusMap = {\n        pending: '待审批',\n        approved: '已批准',\n        rejected: '已拒绝'\n      }\n      return statusMap[status] || status\n    },\n    \n    handleSearch() {\n      // 搜索逻辑已经在计算属性中处理\n    },\n    \n    applyFilters() {\n      // 筛选逻辑已经在计算属性中处理\n    },\n    \n    approveApplication(id) {\n      const app = this.applications.find(a => a.id === id)\n      if (app && confirm('确定批准此申请吗？')) {\n        app.status = 'approved'\n      }\n    },\n    \n    rejectApplication(id) {\n      const app = this.applications.find(a => a.id === id)\n      if (app && confirm('确定拒绝此申请吗？')) {\n        app.status = 'rejected'\n      }\n    },\n    \n    undoApplication(id) {\n      const app = this.applications.find(a => a.id === id)\n      if (app) {\n        app.status = 'pending'\n      }\n    },\n    \n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--\n        this.loadPageData()\n      }\n    },\n    \n    nextPage() {\n      this.currentPage++\n      this.loadPageData()\n    },\n    \n    loadPageData() {\n      // 模拟加载更多数据\n      if (this.currentPage > 1) {\n        const newApp = {\n          id: this.applications.length + 1,\n          type: '教室预约申请',\n          applicant: '新加载用户',\n          location: '新加载教室',\n          date: '2023-11-25',\n          time: '10:00 - 12:00',\n          purpose: '新加载用途',\n          status: 'pending'\n        }\n        this.applications.push(newApp)\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n/* CSS变量定义 */\n:root {\n  --primary-color: #2A5CAA;\n  --secondary-color: #4CAF50;\n  --danger-color: #E74C3C;\n  --light-color: #F8F9FA;\n  --dark-color: #343A40;\n  --gray-color: #6C757D;\n  --border-radius: 8px;\n  --box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\n  --transition: all 0.3s ease;\n}\n\n.admin-approve {\n  min-height: 100vh;\n  background-color: #f5f7fa;\n  font-family: 'Roboto', sans-serif;\n  color: #333;\n  line-height: 1.6;\n}\n\n/* 主要内容区域 */\n.approve-container {\n  max-width: 1200px;\n  margin: 2rem auto;\n  padding: 0 1.5rem;\n  flex: 1;\n}\n\n.page-title {\n  font-size: 2rem;\n  color: var(--primary-color);\n  margin-bottom: 2rem;\n  font-weight: 600;\n}\n\n/* 搜索和筛选区域 */\n.search-filter-container {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 1.5rem;\n  margin-bottom: 2rem;\n  align-items: center;\n}\n\n.search-box {\n  flex: 1;\n  min-width: 300px;\n  position: relative;\n}\n\n.search-box i {\n  position: absolute;\n  left: 1rem;\n  top: 50%;\n  transform: translateY(-50%);\n  color: var(--gray-color);\n}\n\n.search-box input {\n  width: 100%;\n  padding: 0.75rem 1rem 0.75rem 2.5rem;\n  border: 1px solid #ddd;\n  border-radius: var(--border-radius);\n  font-size: 1rem;\n  transition: var(--transition);\n}\n\n.search-box input:focus {\n  outline: none;\n  border-color: var(--primary-color);\n  box-shadow: 0 0 0 2px rgba(42, 92, 170, 0.2);\n}\n\n.filter-group {\n  display: flex;\n  gap: 1rem;\n}\n\n.filter-group select {\n  padding: 0.75rem 1rem;\n  border: 1px solid #ddd;\n  border-radius: var(--border-radius);\n  font-size: 1rem;\n  background-color: white;\n  cursor: pointer;\n  transition: var(--transition);\n}\n\n.filter-group select:focus {\n  outline: none;\n  border-color: var(--primary-color);\n}\n\n/* 审批卡片列表 */\n.approval-list {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(350px, 1fr));\n  gap: 1.5rem;\n  margin-bottom: 2rem;\n}\n\n.approval-card {\n  background-color: white;\n  border-radius: var(--border-radius);\n  box-shadow: var(--box-shadow);\n  overflow: hidden;\n  transition: var(--transition);\n}\n\n.approval-card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);\n}\n\n.card-header {\n  padding: 1.25rem 1.5rem;\n  border-bottom: 1px solid #eee;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.card-header h3 {\n  font-size: 1.25rem;\n  font-weight: 500;\n  color: var(--dark-color);\n}\n\n.badge {\n  padding: 0.25rem 0.75rem;\n  border-radius: 20px;\n  font-size: 0.875rem;\n  font-weight: 500;\n}\n\n.badge.pending {\n  background-color: #FFF3CD;\n  color: #856404;\n}\n\n.badge.approved {\n  background-color: #D4EDDA;\n  color: #155724;\n}\n\n.badge.rejected {\n  background-color: #F8D7DA;\n  color: #721C24;\n}\n\n.card-content {\n  padding: 1.5rem;\n}\n\n.info-row {\n  display: flex;\n  align-items: center;\n  margin-bottom: 0.75rem;\n}\n\n.info-row i {\n  margin-right: 0.75rem;\n  color: var(--gray-color);\n  font-size: 1.1rem;\n}\n\n.info-row:last-child {\n  margin-bottom: 0;\n}\n\n/* 修复按钮样式问题 */\n.card-actions {\n  padding: 1rem 1.5rem;\n  border-top: 1px solid #eee;\n  display: flex;\n  gap: 1rem;\n  background-color: white; /* 确保背景色 */\n}\n\n.btn {\n  padding: 0.625rem 1.25rem;\n  border: none;\n  border-radius: var(--border-radius);\n  font-size: 1rem;\n  font-weight: 500;\n  cursor: pointer;\n  transition: var(--transition);\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  gap: 0.5rem;\n  min-width: 100px; /* 确保最小宽度 */\n  opacity: 1 !important; /* 强制显示 */\n  visibility: visible !important; /* 强制显示 */\n}\n\n.btn i {\n  font-size: 1.1rem;\n}\n\n/* 确保按钮基础样式 */\n.btn {\n  background-color: var(--primary-color); /* 默认颜色 */\n  color: white;\n  border: 2px solid transparent; /* 添加边框 */\n}\n\n.approve-btn {\n  background-color: var(--secondary-color);\n  color: white;\n  border: 2px solid var(--secondary-color);\n}\n\n.approve-btn:hover {\n  background-color: #3d8b40;\n  border-color: #3d8b40;\n}\n\n.reject-btn {\n  background-color: var(--danger-color);\n  color: white;\n  border: 2px solid var(--danger-color);\n}\n\n.reject-btn:hover {\n  background-color: #c0392b;\n  border-color: #c0392b;\n}\n\n.undo-btn {\n  background-color: var(--gray-color);\n  color: white;\n  border: 2px solid var(--gray-color);\n}\n\n.undo-btn:hover {\n  background-color: #5a6268;\n  border-color: #5a6268;\n}\n\n/* 确保按钮在非悬停状态也可见 */\n.btn:not(:hover) {\n  background-color: inherit; /* 继承各自颜色 */\n  opacity: 1;\n  visibility: visible;\n}\n\n/* 分页 */\n.pagination {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  gap: 1rem;\n  margin-top: 2rem;\n}\n\n.pagination-btn {\n  width: 40px;\n  height: 40px;\n  border-radius: 50%;\n  background-color: white;\n  border: 1px solid #ddd;\n  color: var(--dark-color);\n  opacity: 1 !important;\n  visibility: visible !important;\n}\n\n.pagination-btn:hover:not(:disabled) {\n  background-color: #f0f0f0;\n}\n\n.pagination-btn:disabled {\n  opacity: 0.5;\n  cursor: not-allowed;\n}\n\n.current-page {\n  font-weight: 500;\n  color: var(--primary-color);\n}\n\n/* 响应式设计 */\n@media (max-width: 768px) {\n  .search-filter-container {\n    flex-direction: column;\n    align-items: stretch;\n  }\n  \n  .filter-group {\n    width: 100%;\n  }\n  \n  .filter-group select {\n    flex: 1;\n  }\n  \n  .approval-list {\n    grid-template-columns: 1fr;\n  }\n  \n  .card-actions {\n    flex-direction: column;\n  }\n  \n  .btn {\n    width: 100%; /* 移动端全宽按钮 */\n    justify-content: center;\n  }\n}\n\n/* 调试样式 - 如果问题仍然存在，取消注释 */\n/*\n.approval-card * {\n  border: 1px solid red !important;\n}\n\n.card-actions {\n  background-color: yellow !important;\n}\n\