 (newId) => {\r\n      courseId.value = newId || ''\r\n    }, { immediate: true })\r\n\r\n    // Âà§Êñ≠ÊòØÂê¶Âú®ËØæÁ®ãÂÜÖ\r\n    const inCourseContext = computed(() => !!courseId.value)\r\n\r\n    // 2. ËèúÂçïÈÖçÁΩÆ\r\n    const menuConfig = {\r\n      // === A. ËØæÁ®ãÂÜÖËèúÂçï (Êô∫ËÉΩÊïôÂ≠¶) ===\r\n      // path ÂÜôÁõ∏ÂØπË∑ØÂæÑÔºåÂêéÈù¢‰ºöËá™Âä®Êãº‰∏ä /student/course/:id/\r\n      courseSmart: [\r\n        {\r\n          name: 'Â≠¶‰π†‰∏≠ÂøÉ',\r\n          children: [\r\n            { name: 'ËÄÉÂã§Á≠æÂà∞', path: 'attendance/index', icon: 'üìç' },\r\n            { name: 'ÊàëÁöÑ‰Ωú‰∏ö', path: 'homework/list', icon: 'üìù' },\r\n            { name: 'ÊàêÁª©Êü•ËØ¢', path: 'grades/index', icon: 'üìä' },\r\n            { name: 'ËØæÁ®ãËµÑÊ∫ê', path: 'resources/list', icon: 'üìÇ' },\r\n            { name: 'ËØ∑ÂÅáÁî≥ËØ∑', path: 'leave/apply', icon: 'üìÖ' }\r\n          ]\r\n        },\r\n        {\r\n          name: 'ËØæÁ®ãÊ¶ÇÂÜµ',\r\n          children: [\r\n            { name: 'ËØæÁ®ãÈ¶ñÈ°µ', path: 'overview', icon: 'üè†' }\r\n          ]\r\n        }\r\n      ],\r\n\r\n      // === B. ÊôÆÈÄöËèúÂçï (ËµÑÊ∫ê„ÄÅÊ†°Âõ≠) ===\r\n      resource: [\r\n        {\r\n          name: 'ËµÑÊ∫êÁÆ°ÁêÜ',\r\n          children: [\r\n            { name: 'ÊïôÂÆ§È¢ÑÁ∫¶', path: '/student/classroom', icon: 'üèõÔ∏è' },\r\n            { name: '‰π¶Á±çÂÄüÈòÖ', path: '/student/library', icon: 'üìñ' }\r\n          ]\r\n        }\r\n      ],\r\n      campus: [\r\n        {\r\n          name: 'Ê†°Âõ≠ÁîüÊ¥ª',\r\n          children: [\r\n            { name: 'ÂÆøËàçÂàÜÈÖç', path: '/student/studorm', icon: 'üõèÔ∏è' },\r\n            { name: 'Ê†°Âõ≠ËÆ∫Âùõ', path: '/student/forum', icon: 'üí¨' }\r\n          ]\r\n        }\r\n      ],\r\n      // Ê∑ªÂä†‰∏Ä‰∏™ÈªòËÆ§ÁöÑÊïôÂ≠¶ÁÆ°ÁêÜÂÖ•Âè£ÔºàÁî®‰∫éÈùûËØæÁ®ãÂÜÖÔºâ\r\n      teachingDefault: [\r\n        {\r\n          name: 'ÊïôÂ≠¶ÁÆ°ÁêÜ',\r\n          children: [\r\n            { name: 'ÊàëÁöÑËØæÁ®ã', path: '/student/teachinghome', icon: 'üìö' },\r\n            { name: 'ËØ∑ÂÅáËÆ∞ÂΩï', path: '/student/leave', icon: 'üìÖ' } // ÂÖ®Â±ÄËØ∑ÂÅáËÆ∞ÂΩï\r\n          ]\r\n        }\r\n      ]\r\n    }\r\n\r\n    // 3. ÂÜ≥ÂÆöÊòæÁ§∫Âì™ÁªÑËèúÂçï\r\n    const currentMenuGroups = computed(() => {\r\n      // Â¶ÇÊûúÂú®ËØæÁ®ãÈáåÔºåÊòæÁ§∫ËØæÁ®ã‰∏ìÂ±ûËèúÂçï\r\n      if (inCourseContext.value) {\r\n        return menuConfig.courseSmart\r\n      }\r\n      // Â¶ÇÊûúÊòØËµÑÊ∫êÁõ∏ÂÖ≥\r\n      if (route.path.includes('/resource') || route.path.includes('/library') || route.path.includes('/classroom')) {\r\n        return menuConfig.resource\r\n      }\r\n      // Â¶ÇÊûúÊòØÊ†°Âõ≠Áõ∏ÂÖ≥\r\n      if (route.path.includes('/campus') || route.path.includes('/dorm') || route.path.includes('/forum')) {\r\n        return menuConfig.campus\r\n      }\r\n      // ÈªòËÆ§ÊòæÁ§∫ÊïôÂ≠¶È¶ñÈ°µ\r\n      return menuConfig.teachingDefault\r\n    })\r\n\r\n    // 4. ËÆ°ÁÆóÊ†áÈ¢ò\r\n    const currentModuleTitle = computed(() => {\r\n      if (inCourseContext.value) return 'Êô∫ËÉΩÂ≠¶‰π†'\r\n      if (route.path.includes('/resource')) return 'ËµÑÊ∫êÁÆ°ÁêÜ'\r\n      if (route.path.includes('/campus') || route.path.includes('/dorm')) return 'Ê†°Âõ≠ÁîüÊ¥ª'\r\n      return 'ÊïôÂ≠¶ÁÆ°ÁêÜ'\r\n    })\r\n\r\n    // 5. Ë∑ØÂæÑËß£ÊûêÂ∑•ÂÖ∑ (Ê†∏ÂøÉÔºÅ)\r\n    const resolvePath = (path) => {\r\n      // Â¶ÇÊûú‰∏çÊòØ‰ª• / ÂºÄÂ§¥ÔºåËØ¥ÊòéÊòØËØæÁ®ãÂÜÖÁöÑÁõ∏ÂØπË∑ØÂæÑÔºåÈúÄË¶ÅÂä†‰∏äÂâçÁºÄ\r\n      if (!path.startsWith('/')) {\r\n        return `/student/course/${courseId.value}/${path}`\r\n      }\r\n      return path\r\n    }\r\n\r\n    // 6. È´ò‰∫ÆÈÄªËæë\r\n    const isItemActive = (item) => {\r\n      const fullPath = resolvePath(item.path)\r\n      return route.path === fullPath || route.path.startsWith(fullPath + '/')\r\n    }\r\n\r\n    return {\r\n      currentMenuGroups,\r\n      currentModuleTitle,\r\n      resolvePath,\r\n      isItemActive\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* === ‰øùÊåÅ‰Ω†ÂéüÊúâÁöÑÊ†∑ÂºèÂÆåÂÖ®‰∏çÂèò === */\r\n.app-sidebar {\r\n  