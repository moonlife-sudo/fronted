{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.object.keys.js\";\nimport \"core-js/modules/es.string.starts-with.js\";\nimport { ref, onMounted, onUnmounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nexport default {\n  name: 'TeaHeader',\n  setup: function setup() {\n    var route = useRoute();\n    var router = useRouter();\n\n    // --- 导航逻辑 (教师端菜单) ---\n    var topLevelMenu = ref([{\n      name: '首页',\n      path: '/teacher/home'\n    },\n    // 通用首页\n    {\n      name: '教学管理',\n      path: '/teacher/teachinghome'\n    },\n    // 教师核心功能\n    {\n      name: '资源中心',\n      path: '/teacher/resourcehome'\n    }]);\n    var isActive = function isActive(menuItem) {\n      if (menuItem.path === '/home') return route.path === '/home';\n      return route.path.startsWith(menuItem.path);\n    };\n\n    // --- 用户信息逻辑 ---\n    var userInfo = ref({\n      name: '周老师'\n    });\n    var hasAvatar = ref(true);\n    var showUserMenu = ref(false);\n    var avatarRef = ref(null);\n    onMounted(function () {\n      var savedUser = localStorage.getItem('userInfo');\n      if (savedUser) {\n        userInfo.value = JSON.parse(savedUser);\n      }\n    });\n    var toggleUserMenu = function toggleUserMenu() {\n      showUserMenu.value = !showUserMenu.value;\n      showMessageBox.value = false;\n    };\n    var goToProfile = function goToProfile() {\n      router.push('/teacher/profile');\n    };\n    var handleLogout = function handleLogout() {\n      if (confirm('确定要退出登录吗？')) {\n        localStorage.removeItem('token');\n        localStorage.removeItem('userInfo');\n        router.push('/login');\n      }\n    };\n\n    // --- 消息逻辑 ---\n    var showMessageBox = ref(false);\n    var messageRef = ref(null);\n    var unreadCount = ref(3);\n    var messages = ref([{\n      id: 1,\n      sender: '教务处',\n      text: '请于本周五前提交教学大纲。',\n      time: '09:00'\n    }, {\n      id: 2,\n      sender: '张同学',\n      text: '老师，请问作业