{"ast":null,"code":"require(\"core-js/modules/es.iterator.constructor.js\");\nrequire(\"core-js/modules/es.iterator.for-each.js\");\ndocument.addEventListener(\"DOMContentLoaded\", function () {\n  // 加载头部\n  fetch(\"header.html\").then(function (response) {\n    return response.text();\n  }).then(function (html) {\n    document.getElementById(\"header-container\").innerHTML = html;\n    // 加载头部CSS\n    var headerCSS = document.createElement(\"link\");\n    headerCSS.rel = \"stylesheet\";\n    headerCSS.href = \"header.css\";\n    document.head.appendChild(headerCSS);\n  });\n\n  // 加载底部\n  fetch(\"footer.html\").then(function (response) {\n    return response.text();\n  }).then(function (html) {\n    document.getElementById(\"footer-container\").innerHTML = html;\n    // 加载底部CSS\n    var footerCSS = document.createElement(\"link\");\n    footerCSS.rel = \"stylesheet\";\n    footerCSS.href = \"footer.css\";\n    document.head.appendChild(footerCSS);\n  });\n\n  // 搜索功能\n  const searchInput = document.getElementById('search-input');\n  const approvalCards = document.querySelectorAll('.approval-card');\n  searchInput.addEventListener('input', function () {\n    const searchTerm = this.value.toLowerCase();\n    approvalCards.forEach(card => {\n      const cardText = card.textContent.toLowerCase();\n      if (cardText.includes(searchTerm)) {\n        card.style.display = 'block';\n      } else {\n        card.style.display = 'none';\n      }\n    });\n  });\n\n  // 筛选功能\n  const statusFilter = document.getElementById('status-filter');\n  const dateFilter = document.getElementById('date-filter');\n  function applyFilters() {\n    const statusValue = statusFilter.value;\n    const dateValue = dateFilter.value;\n    approvalCards.forEach(card => {\n      const cardStatus = card.querySelector('.badge').classList[1];\n      const cardDate = card.querySelector('.info-row:nth-child(3) span').textContent.split(': ')[1];\n      let statusMatch = statusValue === 'all' || cardStatus === statusValue;\n      let dateMatch = dateValue === 'all';\n      if (dateValue !== 'all') {\n        const today = new Date();\n        const cardDateObj = new Date(cardDate);\n        if (dateValue === 'today') {\n          dateMatch = cardDateObj.toDateString() === today.toDateString();\n        } else if (dateValue === 'week') {\n          const weekStart = new Date(today.setDate(today.getDate() - today.getDay()));\n          const weekEnd = new Date(today.setDate(today.getDate() + 6));\n          dateMatch = cardDateObj >= weekStart && cardDateObj <= weekEnd;\n        } else if (dateValue === 'month') {\n          dateMatch = cardDateObj.getMonth() === today.getMonth() && cardDateObj.getFullYear() === today.getFullYear();\n        }\n      }\n      if (statusMatch && dateMatch) {\n        card.style.display = 'block';\n      } else {\n        card.style.display = 'none';\n      }\n    });\n  }\n  statusFilter.addEventListener('change', applyFilters);\n  dateFilter.addEventListener('change', applyFilters);\n\n  // 审批按钮功能\n  document.querySelectorAll('.approve-btn').forEach(btn => {\n    btn.addEventListener('click', function () {\n      const card = this.closest('.approval-card');\n      const badge = card.querySelector('.badge');\n      badge.classList.remove('pending');\n      badge.classList.add('approved');\n      badge.textContent = '已批准';\n      card.querySelector('.card-actions').innerHTML = `\n        <button class=\"btn undo-btn\">\n          <i class=\"bi bi-arrow-counterclockwise\"></i> 撤销\n        </button>\n      `;\n\n      // 添加撤销功能\n      card.querySelector('.undo-btn').addEventListener('click', function () {\n        badge.classList.remove('approved');\n        badge.classList.add('pending');\n        badge.textContent = '待审批';\n        card.querySelector('.card-actions').innerHTML = `\n          <button class=\"btn approve-btn\">\n            <i class=\"bi bi-check-circle\"></i> 批准\n          </button>\n          <button class=\"btn reject-btn\">\n            <i class=\"bi bi-x-circle\"></i> 拒绝\n          </button>\n        `;\n\n        // 重新绑定事件\n        bindActionButtons();\n      });\n    });\n  });\n\n  // 拒绝按钮功能\n  document.querySelectorAll('.reject-btn').forEach(btn => {\n    btn.addEventListener('click', function () {\n      const card = this.closest('.approval-card');\n      const badge = card.querySelector('.badge');\n      badge.classList.remove('pending');\n      badge.classList.add('rejected');\n      badge.textContent = '已拒绝';\n      card.querySelector('.card-actions').innerHTML = `\n        <button class=\"btn undo-btn\">\n          <i class=\"bi bi-arrow-counterclockwise\"></i> 撤销\n        </button>\n      `;\n\n      // 添加撤销功能\n      card.querySelector('.undo-btn').addEventListener('click', function () {\n        badge.classList.remove('rejected');\n        badge.classList.add('pending');\n        badge.textContent = '待审批';\n        card.querySelector('.card-actions').innerHTML = `\n          <button class=\"btn approve-btn\">\n            <i class=\"bi bi-check-circle\"></i> 批准\n          </button>\n          <button class=\"btn reject-btn\">\n            <i class=\"bi bi-x-circle\"></i> 拒绝\n          </button>\n        `;\n\n        // 重新绑定事件\n        bindActionButtons();\n      });\n    });\n  });\n\n  // 分页功能\n  const paginationNext = document.querySelector('.pagination-btn:not(:disabled)');\n  if (paginationNext) {\n    paginationNext.addEventListener('click', function () {\n      // 这里应该是AJAX加载更多数据，这里仅做UI演示\n      const currentPage = document.querySelector('.current-page');\n      currentPage.textContent = parseInt(currentPage.textContent) + 1;\n      this.previousElementSibling.disabled = false;\n\n      // 模拟加载更多数据\n      setTimeout(() => {\n        const approvalList = document.querySelector('.approval-list');\n        approvalList.innerHTML += `\n          <div class=\"approval-card\">\n            <div class=\"card-header\">\n              <h3>教室预约申请</h3>\n              <span class=\"badge pending\">待审批</span>\n            </div>\n            \n            <div class=\"card-content\">\n              <div class=\"info-row\">\n                <i class=\"bi bi-person\"></i>\n                <span>申请人: 新加载用户</span>\n              </div>\n              <div class=\"info-row\">\n                <i class=\"bi bi-building\"></i>\n                <span>教室: 新加载教室</span>\n              </div>\n              <div class=\"info-row\">\n                <i class=\"bi bi-calendar\"></i>\n                <span>日期: 2023-11-25</span>\n              </div>\n              <div class=\"info-row\">\n                <i class=\"bi bi-clock\"></i>\n                <span>时间: 10:00 - 12:00</span>\n              </div>\n              <div class=\"info-row\">\n                <i class=\"bi bi-journal-text\"></i>\n                <span>用途: 新加载用途</span>\n              </div>\n            </div>\n            \n            <div class=\"card-actions\">\n              <button class=\"btn approve-btn\">\n                <i class=\"bi bi-check-circle\"></i> 批准\n              </button>\n              <button class=\"btn reject-btn\">\n                <i class=\"bi bi-x-circle\"></i> 拒绝\n              </button>\n            </div>\n          </div>\n        `;\n\n        // 重新绑定事件\n        bindActionButtons();\n      }, 500);\n    });\n  }\n\n  // 绑定操作按钮的函数\n  function bindActionButtons() {\n    document.querySelectorAll('.approve-btn').forEach(btn => {\n      btn.addEventListener('click', handleApprove);\n    });\n    document.querySelectorAll('.reject-btn').forEach(btn => {\n      btn.addEventListener('click', handleReject);\n    });\n  }\n\n  // 批准处理函数\n  function handleApprove() {\n    const card = this.closest('.approval-card');\n    const badge = card.querySelector('.badge');\n    badge.classList.remove('pending');\n    badge.classList.add('approved');\n    badge.textContent = '已批准';\n    card.querySelector('.card-actions').innerHTML = `\n      <button class=\"btn undo-btn\">\n        <i class=\"bi bi-arrow-counterclockwise\"></i> 撤销\n      </button>\n    `;\n\n    // 添加撤销功能\n    card.querySelector('.undo-btn').addEventListener('click', handleUndo);\n  }\n\n  // 拒绝处理函数\n  function handleReject() {\n    const card = this.closest('.approval-card');\n    const badge = card.querySelector('.badge');\n    badge.classList.remove('pending');\n    badge.classList.add('rejected');\n    badge.textContent = '已拒绝';\n    card.querySelector('.card-actions').innerHTML = `\n      <button class=\"btn undo-btn\">\n        <i class=\"bi bi-arrow-counterclockwise\"></i> 撤销\n      </button>\n    `;\n\n    // 添加撤销功能\n    card.querySelector('.undo-btn').addEventListener('click', handleUndo);\n  }\n\n  // 撤销处理函数\n  function handleUndo() {\n    const card = this.closest('.approval-card');\n    const badge = card.querySelector('.badge');\n    badge.classList.remove('approved', 'rejected');\n    badge.classList.add('pending');\n    badge.textContent = '待审批';\n    card.querySelector('.card-actions').innerHTML = `\n      <button class=\"btn approve-btn\">\n        <i class=\"bi bi-check-circle\"></i> 批准\n      </button>\n      <button class=\"btn reject-btn\">\n        <i class=\"bi bi-x-circle\"></i> 拒绝\n      </button>\n    `;\n\n    // 重新绑定事件\n    bindActionButtons();\n  }\n});","map":{"version":3,"names":["document","addEventListener","fetch","then","response","text","html","getElementById","innerHTML","headerCSS","createElement","rel","href","head","appendChild","footerCSS","searchInput","approvalCards","querySelectorAll","searchTerm","value","toLowerCase","forEach","card","cardText","textContent","includes","style","display","statusFilter","dateFilter","applyFilters","statusValue","dateValue","cardStatus","querySelector","classList","cardDate","split","statusMatch","dateMatch","today","Date","cardDateObj","toDateString","weekStart","setDate","getDate","getDay","weekEnd","getMonth","getFullYear","btn","closest","badge","remove","add","bindActionButtons","paginationNext","currentPage","parseInt","previousElementSibling","disabled","setTimeout","approvalList","handleApprove","handleReject","handleUndo"],"sources":["/Users/zhouzhou/Desktop/resource/src/views/AdminApprove.vue"],"sourcesContent":["<template>\n  <!-- 来自 adminApprove.html -->\n   <!DOCTYPE html>\n<html lang=\"zh-CN\">\n<head>\n  <meta charset=\"UTF-8\">\n  <meta http-equiv=\"Content-Type\" content=\"text/html; charset=utf-8\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n  <title>管理员预约审批页</title>\n  <link rel=\"stylesheet\" href=\"adminApprove.css\">\n  <link rel=\"stylesheet\" href=\"https://gcore.jsdelivr.net/npm/bootstrap-icons@1.8.0/font/bootstrap-icons.css\">\n  <script src=\"adminApprove.js\"></script>\n  <link rel=\"stylesheet\" href=\"header.css\">\n  <link rel=\"stylesheet\" href=\"footer.css\">\n</head>\n<body>\n  <div id=\"header-container\"><div class=\"header-component\">\n  <div class=\"header-container\">\n    <div class=\"logo-container\">\n      <a href=\"/\">\n        <img \n          loading=\"lazy\" \n          src=\"https://placeholder.pics/svg/150x50/2A5CAA/FFFFFF/资源管理系统\" \n          alt=\"资源管理系统LOGO\" \n          class=\"logo\"\n        >\n      </a>\n    </div>\n    \n    <nav class=\"main-nav\">\n      <ul class=\"nav-list\">\n        <li class=\"nav-item\" data-role=\"all\">\n          <a href=\"/\" class=\"nav-link\">首页</a>\n        </li>\n        <li class=\"nav-item\" data-role=\"admin teacher student\">\n          <a href=\"/teacherStudentClassroom.html\" class=\"nav-link\">教室预约</a>\n        </li>\n        <li class=\"nav-item\" data-role=\"admin student\">\n          <a href=\"/studentLibrary.html\" class=\"nav-link\">图书馆</a>\n        </li>\n        <li class=\"nav-item\" data-role=\"admin\">\n          <a href=\"/adminClassroom.html\" class=\"nav-link\">教室管理</a>\n        </li>\n        <li class=\"nav-item\" data-role=\"admin\">\n          <a href=\"/adminApprove.html\" class=\"nav-link\">预约审批</a>\n        </li>\n      </ul>\n    </nav>\n    \n    <div class=\"search-container\">\n      <form class=\"search-form\" role=\"search\">\n        <input \n          type=\"search\" \n          class=\"search-input\" \n          placeholder=\"搜索教室或书籍...\" \n          aria-label=\"搜索教室或书籍\"\n        >\n        <button type=\"submit\" class=\"search-button\">\n          <svg viewBox=\"0 0 24 24\" width=\"20\" height=\"20\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n            <circle cx=\"11\" cy=\"11\" r=\"8\"></circle>\n            <path d=\"M21 21l-4.35-4.35\"></path>\n          </svg>\n        </button>\n      </form>\n    </div>\n    \n    <div class=\"user-container\">\n      <div class=\"user-dropdown\">\n        <button class=\"user-button\">\n          <svg viewBox=\"0 0 24 24\" width=\"24\" height=\"24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n            <path d=\"M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2\"></path>\n            <circle cx=\"12\" cy=\"7\" r=\"4\"></circle>\n          </svg>\n          <span class=\"user-text\">用户入口</span>\n        </button>\n        <div class=\"dropdown-content\">\n          <a href=\"/login.html\" class=\"dropdown-item\" id=\"login-link\">登录</a>\n          <a href=\"/profile.html\" class=\"dropdown-item\" id=\"profile-link\">个人中心</a>\n          <a href=\"#\" class=\"dropdown-item\" id=\"logout-link\">退出登录</a>\n        </div>\n      </div>\n    </div>\n  </div>\n</div></div>\n  \n  <main class=\"approve-container\">\n    <h1 class=\"page-title\">预约审批</h1>\n    \n    <div class=\"search-filter-container\">\n      <div class=\"search-box\">\n        <i class=\"bi bi-search\"></i>\n        <input type=\"text\" placeholder=\"搜索申请人、教室或日期...\" id=\"search-input\">\n      </div>\n      \n      <div class=\"filter-group\">\n        <select id=\"status-filter\">\n          <option value=\"all\">所有状态</option>\n          <option value=\"pending\" selected>待审批</option>\n          <option value=\"approved\">已批准</option>\n          <option value=\"rejected\">已拒绝</option>\n        </select>\n        \n        <select id=\"date-filter\">\n          <option value=\"all\">所有日期</option>\n          <option value=\"today\">今天</option>\n          <option value=\"week\">本周</option>\n          <option value=\"month\">本月</option>\n        </select>\n      </div>\n    </div>\n    \n    <div class=\"approval-list\">\n      <div class=\"approval-card\">\n        <div class=\"card-header\">\n          <h3>教室预约申请</h3>\n          <span class=\"badge pending\">待审批</span>\n        </div>\n        \n        <div class=\"card-content\">\n          <div class=\"info-row\">\n            <i class=\"bi bi-person\"></i>\n            <span>申请人: 张老师</span>\n          </div>\n          <div class=\"info-row\">\n            <i class=\"bi bi-building\"></i>\n            <span>教室: 教学楼A-201</span>\n          </div>\n          <div class=\"info-row\">\n            <i class=\"bi bi-calendar\"></i>\n            <span>日期: 2023-11-15</span>\n          </div>\n          <div class=\"info-row\">\n            <i class=\"bi bi-clock\"></i>\n            <span>时间: 14:00 - 16:00</span>\n          </div>\n          <div class=\"info-row\">\n            <i class=\"bi bi-journal-text\"></i>\n            <span>用途: 计算机科学研讨会</span>\n          </div>\n        </div>\n        \n        <div class=\"card-actions\">\n          <button class=\"btn approve-btn\">\n            <i class=\"bi bi-check-circle\"></i> 批准\n          </button>\n          <button class=\"btn reject-btn\">\n            <i class=\"bi bi-x-circle\"></i> 拒绝\n          </button>\n        </div>\n      </div>\n      \n      <div class=\"approval-card\">\n        <div class=\"card-header\">\n          <h3>教室预约申请</h3>\n          <span class=\"badge pending\">待审批</span>\n        </div>\n        \n        <div class=\"card-content\">\n          <div class=\"info-row\">\n            <i class=\"bi bi-person\"></i>\n            <span>申请人: 李教授</span>\n          </div>\n          <div class=\"info-row\">\n            <i class=\"bi bi-building\"></i>\n            <span>教室: 图书馆报告厅</span>\n          </div>\n          <div class=\"info-row\">\n            <i class=\"bi bi-calendar\"></i>\n            <span>日期: 2023-11-18</span>\n          </div>\n          <div class=\"info-row\">\n            <i class=\"bi bi-clock\"></i>\n            <span>时间: 09:00 - 12:00</span>\n          </div>\n          <div class=\"info-row\">\n            <i class=\"bi bi-journal-text\"></i>\n            <span>用途: 学术讲座</span>\n          </div>\n        </div>\n        \n        <div class=\"card-actions\">\n          <button class=\"btn approve-btn\">\n            <i class=\"bi bi-check-circle\"></i> 批准\n          </button>\n          <button class=\"btn reject-btn\">\n            <i class=\"bi bi-x-circle\"></i> 拒绝\n          </button>\n        </div>\n      </div>\n      \n      <div class=\"approval-card\">\n        <div class=\"card-header\">\n          <h3>教室预约申请</h3>\n          <span class=\"badge pending\">待审批</span>\n        </div>\n        \n        <div class=\"card-content\">\n          <div class=\"info-row\">\n            <i class=\"bi bi-person\"></i>\n            <span>申请人: 王同学</span>\n          </div>\n          <div class=\"info-row\">\n            <i class=\"bi bi-building\"></i>\n            <span>教室: 实验楼B-105</span>\n          </div>\n          <div class=\"info-row\">\n            <i class=\"bi bi-calendar\"></i>\n            <span>日期: 2023-11-20</span>\n          </div>\n          <div class=\"info-row\">\n            <i class=\"bi bi-clock\"></i>\n            <span>时间: 16:00 - 18:00</span>\n          </div>\n          <div class=\"info-row\">\n            <i class=\"bi bi-journal-text\"></i>\n            <span>用途: 学生社团活动</span>\n          </div>\n        </div>\n        \n        <div class=\"card-actions\">\n          <button class=\"btn approve-btn\">\n            <i class=\"bi bi-check-circle\"></i> 批准\n          </button>\n          <button class=\"btn reject-btn\">\n            <i class=\"bi bi-x-circle\"></i> 拒绝\n          </button>\n        </div>\n      </div>\n    </div>\n    \n    <div class=\"pagination\">\n      <button class=\"btn pagination-btn\" disabled>\n        <i class=\"bi bi-chevron-left\"></i>\n      </button>\n      <span class=\"current-page\">1</span>\n      <button class=\"btn pagination-btn\">\n        <i class=\"bi bi-chevron-right\"></i>\n      </button>\n    </div>\n  </main>\n  \n  <div id=\"footer-container\"><div class=\"footer-component\">\n  <div class=\"footer-container\">\n    <div class=\"footer-section\">\n      <h3 class=\"footer-title\">资源管理系统</h3>\n      <p class=\"footer-text\">为学校提供高效的教室和图书馆资源管理平台</p>\n    </div>\n    \n    <div class=\"footer-section\">\n      <h4 class=\"footer-subtitle\">联系方式</h4>\n      <ul class=\"footer-links\">\n        <li><a href=\"mailto:support@school.edu\" class=\"footer-link\">support@school.edu</a></li>\n        <li><a href=\"tel:+8610123456789\" class=\"footer-link\">010-12345678</a></li>\n      </ul>\n    </div>\n    \n    <div class=\"footer-section\">\n      <h4 class=\"footer-subtitle\">社交媒体</h4>\n      <div class=\"social-links\">\n        <a href=\"#\" class=\"social-link\" aria-label=\"微信\">\n          <img loading=\"lazy\" src=\"https://placeholder.pics/svg/32x32/2A5CAA/FFFFFF/微信\" alt=\"微信\" width=\"32\" height=\"32\">\n        </a>\n        <a href=\"#\" class=\"social-link\" aria-label=\"微博\">\n          <img loading=\"lazy\" src=\"https://placeholder.pics/svg/32x32/2A5CAA/FFFFFF/微博\" alt=\"微博\" width=\"32\" height=\"32\">\n        </a>\n        <a href=\"#\" class=\"social-link\" aria-label=\"QQ\">\n          <img loading=\"lazy\" src=\"https://placeholder.pics/svg/32x32/2A5CAA/FFFFFF/QQ\" alt=\"QQ\" width=\"32\" height=\"32\">\n        </a>\n      </div>\n    </div>\n  </div>\n  \n  <div class=\"footer-bottom\">\n    <p class=\"copyright\">&copy; 2023 学校资源管理系统. 保留所有权利.</p>\n    <p class=\"icp\">ICP备案号: 京ICP备12345678号</p>\n  </div>\n</div></div>\n  <script src=\"header.js\"></script>\n  <script src=\"footer.js\"></script>\n</body>\n</html>\n</template>\n\n<script>\ndocument.addEventListener(\"DOMContentLoaded\", function() {\n  // 加载头部\n  fetch(\"header.html\")\n    .then(function(response) { return response.text(); })\n    .then(function(html) {\n      document.getElementById(\"header-container\").innerHTML = html;\n      // 加载头部CSS\n      var headerCSS = document.createElement(\"link\");\n      headerCSS.rel = \"stylesheet\";\n      headerCSS.href = \"header.css\";\n      document.head.appendChild(headerCSS);\n    });\n  \n  // 加载底部\n  fetch(\"footer.html\")\n    .then(function(response) { return response.text(); })\n    .then(function(html) {\n      document.getElementById(\"footer-container\").innerHTML = html;\n      // 加载底部CSS\n      var footerCSS = document.createElement(\"link\");\n      footerCSS.rel = \"stylesheet\";\n      footerCSS.href = \"footer.css\";\n      document.head.appendChild(footerCSS);\n    });\n\n  // 搜索功能\n  const searchInput = document.getElementById('search-input');\n  const approvalCards = document.querySelectorAll('.approval-card');\n  \n  searchInput.addEventListener('input', function() {\n    const searchTerm = this.value.toLowerCase();\n    \n    approvalCards.forEach(card => {\n      const cardText = card.textContent.toLowerCase();\n      if (cardText.includes(searchTerm)) {\n        card.style.display = 'block';\n      } else {\n        card.style.display = 'none';\n      }\n    });\n  });\n\n  // 筛选功能\n  const statusFilter = document.getElementById('status-filter');\n  const dateFilter = document.getElementById('date-filter');\n  \n  function applyFilters() {\n    const statusValue = statusFilter.value;\n    const dateValue = dateFilter.value;\n    \n    approvalCards.forEach(card => {\n      const cardStatus = card.querySelector('.badge').classList[1];\n      const cardDate = card.querySelector('.info-row:nth-child(3) span').textContent.split(': ')[1];\n      \n      let statusMatch = statusValue === 'all' || cardStatus === statusValue;\n      let dateMatch = dateValue === 'all';\n      \n      if (dateValue !== 'all') {\n        const today = new Date();\n        const cardDateObj = new Date(cardDate);\n        \n        if (dateValue === 'today') {\n          dateMatch = cardDateObj.toDateString() === today.toDateString();\n        } else if (dateValue === 'week') {\n          const weekStart = new Date(today.setDate(today.getDate() - today.getDay()));\n          const weekEnd = new Date(today.setDate(today.getDate() + 6));\n          dateMatch = cardDateObj >= weekStart && cardDateObj <= weekEnd;\n        } else if (dateValue === 'month') {\n          dateMatch = cardDateObj.getMonth() === today.getMonth() && \n                      cardDateObj.getFullYear() === today.getFullYear();\n        }\n      }\n      \n      if (statusMatch && dateMatch) {\n        card.style.display = 'block';\n      } else {\n        card.style.display = 'none';\n      }\n    });\n  }\n  \n  statusFilter.addEventListener('change', applyFilters);\n  dateFilter.addEventListener('change', applyFilters);\n\n  // 审批按钮功能\n  document.querySelectorAll('.approve-btn').forEach(btn => {\n    btn.addEventListener('click', function() {\n      const card = this.closest('.approval-card');\n      const badge = card.querySelector('.badge');\n      \n      badge.classList.remove('pending');\n      badge.classList.add('approved');\n      badge.textContent = '已批准';\n      \n      card.querySelector('.card-actions').innerHTML = `\n        <button class=\"btn undo-btn\">\n          <i class=\"bi bi-arrow-counterclockwise\"></i> 撤销\n        </button>\n      `;\n      \n      // 添加撤销功能\n      card.querySelector('.undo-btn').addEventListener('click', function() {\n        badge.classList.remove('approved');\n        badge.classList.add('pending');\n        badge.textContent = '待审批';\n        \n        card.querySelector('.card-actions').innerHTML = `\n          <button class=\"btn approve-btn\">\n            <i class=\"bi bi-check-circle\"></i> 批准\n          </button>\n          <button class=\"btn reject-btn\">\n            <i class=\"bi bi-x-circle\"></i> 拒绝\n          </button>\n        `;\n        \n        // 重新绑定事件\n        bindActionButtons();\n      });\n    });\n  });\n\n  // 拒绝按钮功能\n  document.querySelectorAll('.reject-btn').forEach(btn => {\n    btn.addEventListener('click', function() {\n      const card = this.closest('.approval-card');\n      const badge = card.querySelector('.badge');\n      \n      badge.classList.remove('pending');\n      badge.classList.add('rejected');\n      badge.textContent = '已拒绝';\n      \n      card.querySelector('.card-actions').innerHTML = `\n        <button class=\"btn undo-btn\">\n          <i class=\"bi bi-arrow-counterclockwise\"></i> 撤销\n        </button>\n      `;\n      \n      // 添加撤销功能\n      card.querySelector('.undo-btn').addEventListener('click', function() {\n        badge.classList.remove('rejected');\n        badge.classList.add('pending');\n        badge.textContent = '待审批';\n        \n        card.querySelector('.card-actions').innerHTML = `\n          <button class=\"btn approve-btn\">\n            <i class=\"bi bi-check-circle\"></i> 批准\n          </button>\n          <button class=\"btn reject-btn\">\n            <i class=\"bi bi-x-circle\"></i> 拒绝\n          </button>\n        `;\n        \n        // 重新绑定事件\n        bindActionButtons();\n      });\n    });\n  });\n\n  // 分页功能\n  const paginationNext = document.querySelector('.pagination-btn:not(:disabled)');\n  if (paginationNext) {\n    paginationNext.addEventListener('click', function() {\n      // 这里应该是AJAX加载更多数据，这里仅做UI演示\n      const currentPage = document.querySelector('.current-page');\n      currentPage.textContent = parseInt(currentPage.textContent) + 1;\n      this.previousElementSibling.disabled = false;\n      \n      // 模拟加载更多数据\n      setTimeout(() => {\n        const approvalList = document.querySelector('.approval-list');\n        approvalList.innerHTML += `\n          <div class=\"approval-card\">\n            <div class=\"card-header\">\n              <h3>教室预约申请</h3>\n              <span class=\"badge pending\">待审批</span>\n            </div>\n            \n            <div class=\"card-content\">\n              <div class=\"info-row\">\n                <i class=\"bi bi-person\"></i>\n                <span>申请人: 新加载用户</span>\n              </div>\n              <div class=\"info-row\">\n                <i class=\"bi bi-building\"></i>\n                <span>教室: 新加载教室</span>\n              </div>\n              <div class=\"info-row\">\n                <i class=\"bi bi-calendar\"></i>\n                <span>日期: 2023-11-25</span>\n              </div>\n              <div class=\"info-row\">\n                <i class=\"bi bi-clock\"></i>\n                <span>时间: 10:00 - 12:00</span>\n              </div>\n              <div class=\"info-row\">\n                <i class=\"bi bi-journal-text\"></i>\n                <span>用途: 新加载用途</span>\n              </div>\n            </div>\n            \n            <div class=\"card-actions\">\n              <button class=\"btn approve-btn\">\n                <i class=\"bi bi-check-circle\"></i> 批准\n              </button>\n              <button class=\"btn reject-btn\">\n                <i class=\"bi bi-x-circle\"></i> 拒绝\n              </button>\n            </div>\n          </div>\n        `;\n        \n        // 重新绑定事件\n        bindActionButtons();\n      }, 500);\n    });\n  }\n\n  // 绑定操作按钮的函数\n  function bindActionButtons() {\n    document.querySelectorAll('.approve-btn').forEach(btn => {\n      btn.addEventListener('click', handleApprove);\n    });\n    \n    document.querySelectorAll('.reject-btn').forEach(btn => {\n      btn.addEventListener('click', handleReject);\n    });\n  }\n  \n  // 批准处理函数\n  function handleApprove() {\n    const card = this.closest('.approval-card');\n    const badge = card.querySelector('.badge');\n    \n    badge.classList.remove('pending');\n    badge.classList.add('approved');\n    badge.textContent = '已批准';\n    \n    card.querySelector('.card-actions').innerHTML = `\n      <button class=\"btn undo-btn\">\n        <i class=\"bi bi-arrow-counterclockwise\"></i> 撤销\n      </button>\n    `;\n    \n    // 添加撤销功能\n    card.querySelector('.undo-btn').addEventListener('click', handleUndo);\n  }\n  \n  // 拒绝处理函数\n  function handleReject() {\n    const card = this.closest('.approval-card');\n    const badge = card.querySelector('.badge');\n    \n    badge.classList.remove('pending');\n    badge.classList.add('rejected');\n    badge.textContent = '已拒绝';\n    \n    card.querySelector('.card-actions').innerHTML = `\n      <button class=\"btn undo-btn\">\n        <i class=\"bi bi-arrow-counterclockwise\"></i> 撤销\n      </button>\n    `;\n    \n    // 添加撤销功能\n    card.querySelector('.undo-btn').addEventListener('click', handleUndo);\n  }\n  \n  // 撤销处理函数\n  function handleUndo() {\n    const card = this.closest('.approval-card');\n    const badge = card.querySelector('.badge');\n    \n    badge.classList.remove('approved', 'rejected');\n    badge.classList.add('pending');\n    badge.textContent = '待审批';\n    \n    card.querySelector('.card-actions').innerHTML = `\n      <button class=\"btn approve-btn\">\n        <i class=\"bi bi-check-circle\"></i> 批准\n      </button>\n      <button class=\"btn reject-btn\">\n        <i class=\"bi bi-x-circle\"></i> 拒绝\n      </button>\n    `;\n    \n    // 重新绑定事件\n    bindActionButtons();\n  }\n});\n  \n</script>\n\n<style scoped>\n/* 全局样式 */\n:root {\n  --primary-color: #2A5CAA;\n  --secondary-color: #4CAF50;\n  --danger-color: #E74C3C;\n  --light-color: #F8F9FA;\n  --dark-color: #343A40;\n  --gray-color: #6C757D;\n  --border-radius: 8px;\n  --box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\n  --transition: all 0.3s ease;\n}\n\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: bor