  <div class=\"post-list\">\n        <article \n          v-for=\"post in filteredPosts\" \n          :key=\"post.id\"\n          class=\"post-card\"\n          @click=\"goToPostDetail(post.id)\"\n        >\n          <div class=\"post-header\">\n            <h2 class=\"post-title\">{{ post.title }}</h2>\n            <div class=\"post-meta\">\n              <span class=\"post-author\">{{ post.author }}</span>\n              <span class=\"post-date\">{{ post.date }}</span>\n            </div>\n          </div>\n          <div class=\"post-content\">\n            <p>{{ post.content }}</p>\n          </div>\n          <div class=\"post-footer\">\n            <!-- üéØ ÁÇπÂáª\"ÈòÖËØªÊõ¥Â§ö\"Ë∑≥ËΩ¨Âà∞Â∏ñÂ≠êËØ¶ÊÉÖÈ°µ -->\n            <button class=\"read-more\" @click.stop=\"goToPostDetail(post.id)\">ÈòÖËØªÊõ¥Â§ö</button>\n            <div class=\"post-actions\">\n              <span class=\"comment-count\">\n                <i class=\"bi bi-chat-left-text\"></i> {{ post.commentCount }}\n              </span>\n            </div>\n          </div>\n        </article>\n      </div>\n\n      <button \n        class=\"create-post-btn\" \n        aria-label=\"ÂàõÂª∫Êñ∞Â∏ñÂ≠ê\"\n        @click=\"createNewPost\"\n      >\n        <i class=\"bi bi-plus-lg\"></i>\n      </button>\n    </main>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'StuForum',\n  data() {\n    return {\n      searchTerm: '',\n      posts: [\n        {\n          id: 1,\n          title: 'ÂÖ≥‰∫éÂÆøËàçÂç´ÁîüÊ£ÄÊü•ÁöÑÈÄöÁü•',\n          author: 'Âº†ËÄÅÂ∏à',\n          date: '2023-10-15',\n          content: 'Êú¨Âë®‰∫î‰∏ãÂçà3ÁÇπÂ∞ÜËøõË°åÂÖ®Ê†°ÂÆøËàçÂç´ÁîüÊ£ÄÊü•ÔºåËØ∑ÂêÑ‰ΩçÂêåÂ≠¶ÊèêÂâçÂÅöÂ•ΩÂÆøËàçÂç´ÁîüÊ∏ÖÊ¥ÅÂ∑•‰Ωú„ÄÇÊ£ÄÊü•ÂÜÖÂÆπÂåÖÊã¨Â∫äÈì∫Êï¥ÁêÜ„ÄÅÂú∞Èù¢Ê∏ÖÊ¥Å„ÄÅÁâ©ÂìÅÊëÜÊîæÁ≠â...',\n          commentCount: 12\n        },\n        {\n          id: 2,\n          title: 'ÂÆøËàçÁÉ≠Ê∞¥‰æõÂ∫îÊó∂Èó¥Ë∞ÉÊï¥',\n          author: 'ÊùéÂêåÂ≠¶',\n          date: '2023-10-12',\n          content: 'Áî±‰∫éÂ≠£ËäÇÂèòÂåñÔºå‰ªé‰∏ãÂë®‰∏ÄÂºÄÂßãÂÆøËàçÁÉ≠Ê∞¥‰æõÂ∫îÊó∂Èó¥Ë∞ÉÊï¥‰∏∫ÔºöÊó©‰∏ä6:30-8:30Ôºå‰∏≠Âçà12:00-14:00ÔºåÊôö‰∏ä17:30-23:30„ÄÇËØ∑Â§ßÂÆ∂ÂêàÁêÜÂÆâÊéíÊó∂Èó¥...',\n          commentCount: 8\n        },\n        {\n          id: 3,\n          title: 'ÂØªÊâæ‰∏¢Â§±ÁöÑÊ†°Âõ≠Âç°',\n          author: 'ÁéãÂêåÂ≠¶',\n          date: '2023-10-10',\n          content: '‰ªäÂ§©‰∏ãÂçàÂú®Âõæ‰π¶È¶ÜÈôÑËøë‰∏¢Â§±Ê†°Âõ≠Âç°‰∏ÄÂº†ÔºåÂç°Âè∑‰∏∫20231001ÔºåÂç°Â•ó‰∏∫ËìùËâ≤„ÄÇÂ¶ÇÊúâÊãæÂà∞ËÄÖËØ∑ËÅîÁ≥ª13812345678ÔºåÈùûÂ∏∏ÊÑüË∞¢ÔºÅ...',\n          commentCount: 5\n        },\n        {\n          id: 4,\n          title: 'ÂÆøËàçÊ•ºËá™‰π†ÂÆ§ÂºÄÊîæÈÄöÁü•',\n          author: 'ËµµËÄÅÂ∏à',\n          date: '2023-10-08',\n          content: '‰∏∫Êñπ‰æøÂêåÂ≠¶‰ª¨Â≠¶‰π†ÔºåÂÆøËàçÊ•º1Ê•ºËá™‰π†ÂÆ§Âç≥Êó•Ëµ∑Âª∂ÈïøÂºÄÊîæÊó∂Èó¥Ëá≥Êôö‰∏ä11ÁÇπ„ÄÇËØ∑ÂêåÂ≠¶‰ª¨‰øùÊåÅÂÆâÈùôÔºåÁà±Êä§ÂÖ¨ÂÖ±ËÆæÊñΩÔºåÁ¶ªÂºÄÊó∂Â∏¶Ëµ∞‰∏™‰∫∫Áâ©ÂìÅ...',\n          commentCount: 15\n        }\n      ]\n    }\n  },\n  computed: {\n    filteredPosts() {\n      if (!this.searchTerm) {\n        return this.posts\n      }\n      \n      const term = this.searchTerm.toLowerCase()\n      return this.posts.filter(post => \n        post.title.toLowerCase().includes(term) || \n        post.content.toLowerCase().includes(term)\n      )\n    }\n  },\n  methods: {\n    searchPosts() {\n      if (this.searchTerm && this.filteredPosts.length === 0) {\n        alert('Ê≤°ÊúâÊâæÂà∞ÂåπÈÖçÁöÑÂ∏ñÂ≠ê')\n      }\n    },\n    goToPostDetail(postId) {\n      // üéØ Ë∑≥ËΩ¨Âà∞Â∏ñÂ≠êËØ¶ÊÉÖÈ°µ - ‰ΩøÁî®Ê≠£Á°ÆÁöÑË∑ØÂæÑ /student/postdetail\n      this.$router.push('/student/postdetail')\n    },\n    createNewPost() {\n      // Ë∑≥ËΩ¨Âà∞ÂàõÂª∫Êñ∞Â∏ñÂ≠êÈ°µÈù¢\n      console.log('ÂàõÂª∫Êñ∞Â∏ñÂ≠ê')\n      // this.$router.push('/post/new')\n    }\n\n    /*\n    //ËØ¶ÊÉÖÈ°µÊòæÁ§∫ÁâπÂÆöÂ∏ñÂ≠êÁöÑÂÜÖÂÆπ\n    goToPostDetail(postId) {\n    this.$router.push(`/student/postdetail/${postId}`) }\n     })\n    }\n    */\n  }\n}\n</script>\n\n<style>\n/* Ê†∑Âºè‰øùÊåÅ‰∏çÂèò */\n:root {\n  --primary-