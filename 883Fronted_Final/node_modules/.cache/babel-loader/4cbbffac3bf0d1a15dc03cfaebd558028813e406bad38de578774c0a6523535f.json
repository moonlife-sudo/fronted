{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.string.starts-with.js\";\nimport { ref } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nexport default {\n  name: 'AdminHeader',\n  setup: function setup() {\n    var route = useRoute();\n    var router = useRouter();\n\n    // ✅ 修改菜单配置：删智能教学，改名\n    var topLevelMenu = ref([{\n      name: '首页',\n      path: '/home'\n    },\n    // { name: '智能教学', path: '/admin/teaching' }, // 已删除\n    {\n      name: '资源管理',\n      path: '/admin/resourcehome'\n    }, {\n      name: '生活管理',\n      path: '/admin/campushome'\n    } // 改名\n    ]);\n    var hasAvatar = ref(true);\n    var isActive = function isActive(menuItem) {\n      if (menuItem.path === '/') return route.path === '/';\n      return route.path.startsWith(menuItem.path);\n    };\n    var goToProfile = function goToProfile() {\n      router.push('/admin/profile');\n    };\n    return {\n      topLevelMenu: topLevelMenu,\n      hasAvatar: hasAvatar,\n      isActive: isActive,\n      goToProfile: goToProfile\n    };\n  }\n};","map":{"version":3,"names":["ref","useRoute","useRouter","name","setup","route","router","topLevelMenu","path","hasAvatar","isActive","menuItem","startsWith","goToProfile","push"],"sources":["D:\\vvvvvvvvvscode_workspace\\fronted\\883Fronted_Final\\src\\components\\layout\\headers\\AdminHeader.vue"],"sourcesContent":["<template>\r\n  <header class=\"app-header\">\r\n    <div class=\"top-level-nav\">\r\n      <div class=\"nav-left\">\r\n        <router-link to=\"/\" class=\"nav-logo\">\r\n          <span class=\"logo-text\">智能教学系统 (管理端)</span>\r\n        </router-link>\r\n        <nav class=\"main-nav\">\r\n          <router-link v-for=\"item in topLevelMenu\" :key=\"item.path\" :to=\"item.path\" class=\"nav-item\"\r\n            :class=\"{ active: isActive(item) }\">\r\n            {{ item.name }}\r\n          </router-link>\r\n        </nav>\r\n      </div>\r\n\r\n      <div class=\"nav-right\">\r\n        <div class=\"search-box\">\r\n          <input type=\"text\" placeholder=\"搜索...\" class=\"search-input\">\r\n          <button class=\"search-btn\">\r\n            <img src=\"@/assets/images/search-icon.png\" alt=\"搜索\" class=\"icon-img\">\r\n          </button>\r\n        </div>\r\n\r\n        <div class=\"user-avatar-container\" @click=\"goToProfile\">\r\n          <div class=\"user-avatar\">\r\n            <img src=\"https://placeholder.pics/svg/150x150/DEDEDE/555555/管理员\" alt=\"用户头像\" class=\"avatar-img\"\r\n              v-if=\"hasAvatar\">\r\n            <div class=\"avatar-placeholder\" v-else>A</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </header>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue'\r\nimport { useRoute, useRouter } from 'vue-router'\r\n\r\nexport default {\r\n  name: 'AdminHeader',\r\n  setup() {\r\n    const route = useRoute()\r\n    const router = useRouter()\r\n\r\n    // ✅ 修改菜单配置：删智能教学，改名\r\n    const topLevelMenu = ref([\r\n      { name: '首页', path: '/home' },\r\n      // { name: '智能教学', path: '/admin/teaching' }, // 已删除\r\n      { name: '资源管理', path: '/admin/resourcehome' },\r\n      { name: '生活管理', path: '/admin/campushome' } // 改名\r\n    ])\r\n\r\n    const hasAvatar = ref(true)\r\n\r\n    const isActive = (menuItem) => {\r\n      if (menuItem.path === '/') return route.path === '/'\r\n      return route.path.startsWith(menuItem.path)\r\n    }\r\n\r\n    const goToProfile = () => {\r\n      router.push('/admin/profile')\r\n    }\r\n\r\n    return {\r\n      topLevelMenu,\r\n      hasAvatar,\r\n      isActive,\r\n      goToProfile\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.app-header {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  z-index: 1000;\r\n  background-color: var(--primary-color, #2A5CAA);\r\n  height: 60px;\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 0 20px;\r\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.top-level-nav {\r\n  width: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n}\r\n\r\n.nav-left {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 40px;\r\n}\r\n\r\n.nav-logo {\r\n  font-size: 20px;\r\n  font-weight: bold;\r\n  color: white;\r\n  text-decoration: none;\r\n  white-space: nowrap;\r\n}\r\n\r\n.logo-text {\r\n  color: white;\r\n}\r\n\r\n.main-nav {\r\n  display: flex;\r\n  gap: 10px;\r\n}\r\n\r\n.nav-item {\r\n  color: rgba(255, 255, 255, 0.85);\r\n  text-decoration: none;\r\n  padding: 8px 16px;\r\n  border-radius: 4px;\r\n  font-size: 14px;\r\n  transition: all 0.3s;\r\n}\r\n\r\n.nav-item:hover,\r\n.nav-item.active {\r\n  background-color: rgba(255, 255, 255, 0.2);\r\n  color: white;\r\n  font-weight: 500;\r\n}\r\n\r\n.nav-right {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 20px;\r\n}\r\n\r\n/* 修复搜索框样式 */\r\n.search-box {\r\n  display: flex;\r\n  align-items: center;\r\n  background: rgba(255, 255, 255, 0.15);\r\n  border-radius: 20px;\r\n  /* 圆角 */\r\n  padding: 5px 15px;\r\n  height: 36px;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.search-input {\r\n  background: transparent;\r\n  border: none;\r\n  color: white;\r\n  width: 180px;\r\n  outline: none;\r\n  font-size: 14px;\r\n}\r\n\r\n.search-input::placeholder {\r\n  color: rgba(255, 255, 255, 0.6);\r\n}\r\n\r\n.search-btn {\r\n  background: transparent;\r\n  border: none;\r\n  padding: 0;\r\n  cursor: pointer;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.icon-img {\r\n  width: 18px;\r\n  height: 18px;\r\n  filter: brightness(0) invert(1);\r\n  /* 变白 */\r\n  opacity: 0.9;\r\n}\r\n\r\n/* 头像区域 */\r\n.user-avatar-container {\r\n  cursor: pointer;\r\n  border-radius: 50%;\r\n  padding: 2px;\r\n  border: 2px solid rgba(255, 255, 255, 0.3);\r\n  transition: all 0.3s;\r\n}\r\n\r\n.user-avatar-container:hover {\r\n  border-color: white;\r\n}\r\n\r\n.user-avatar {\r\n  width: 32px;\r\n  height: 32px;\r\n  border-radius: 50%;\r\n  overflow: hidden;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background: #ddd;\r\n}\r\n\r\n.avatar-img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n</style>"],"mappings":";;AAoCA,SAASA,GAAE,QAAS,KAAI;AACxB,SAASC,QAAQ,EAAEC,SAAQ,QAAS,YAAW;AAE/C,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAK,WAALA,KAAKA,CAAA,EAAG;IACN,IAAMC,KAAI,GAAIJ,QAAQ,CAAC;IACvB,IAAMK,MAAK,GAAIJ,SAAS,CAAC;;IAEzB;IACA,IAAMK,YAAW,GAAIP,GAAG,CAAC,CACvB;MAAEG,IAAI,EAAE,IAAI;MAAEK,IAAI,EAAE;IAAQ,CAAC;IAC7B;IACA;MAAEL,IAAI,EAAE,MAAM;MAAEK,IAAI,EAAE;IAAsB,CAAC,EAC7C;MAAEL,IAAI,EAAE,MAAM;MAAEK,IAAI,EAAE;IAAoB,EAAE;IAAA,CAC7C;IAED,IAAMC,SAAQ,GAAIT,GAAG,CAAC,IAAI;IAE1B,IAAMU,QAAO,GAAI,SAAXA,QAAOA,CAAKC,QAAQ,EAAK;MAC7B,IAAIA,QAAQ,CAACH,IAAG,KAAM,GAAG,EAAE,OAAOH,KAAK,CAACG,IAAG,KAAM,GAAE;MACnD,OAAOH,KAAK,CAACG,IAAI,CAACI,UAAU,CAACD,QAAQ,CAACH,IAAI;IAC5C;IAEA,IAAMK,WAAU,GAAI,SAAdA,WAAUA,CAAA,EAAU;MACxBP,MAAM,CAACQ,IAAI,CAAC,gBAAgB;IAC9B;IAEA,OAAO;MACLP,YAAY,EAAZA,YAAY;MACZE,SAAS,EAATA,SAAS;MACTC,QAAQ,EAARA,QAAQ;MACRG,WAAU,EAAVA;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}