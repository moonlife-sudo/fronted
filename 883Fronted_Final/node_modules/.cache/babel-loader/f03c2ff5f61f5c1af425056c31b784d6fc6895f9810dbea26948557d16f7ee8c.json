seId, (newId) => {\r\n      courseId.value = newId || ''\r\n    }, { immediate: true })\r\n\r\n    // æ˜¯å¦åœ¨è¯¾ç¨‹å†…\r\n    const inCourseContext = computed(() => !!courseId.value)\r\n\r\n    const goBack = () => router.push('/teacher/teachinghome')\r\n\r\n    // 2. èœå•é…ç½®\r\n    const menuConfig = {\r\n      // === A. æ™®é€šçŠ¶æ€ä¸‹çš„èœå• ===\r\n      teaching: [\r\n        {\r\n          name: 'æ•™å­¦å·¥ä½œå°',\r\n          children: [\r\n            { name: 'æˆ‘çš„è¯¾ç¨‹', path: '/teacher/teachinghome', icon: 'ðŸ“š' }, // å…¥å£\r\n            { name: 'è¯·å‡å®¡æ‰¹', path: '/teacher/leave', icon: 'ðŸ“…' },\r\n          ]\r\n        }\r\n      ],\r\n      resource: [\r\n        {\r\n          name: 'å…¬å…±èµ„æº',\r\n          children: [\r\n            { name: 'æ•™å®¤é¢„çº¦', path: '/teacher/classroom', icon: 'ðŸ›ï¸' },\r\n            { name: 'å›¾ä¹¦é¦†', path: '/teacher/library', icon: 'ðŸ“–' }\r\n          ]\r\n        }\r\n      ],\r\n\r\n      // === B. è¯¾ç¨‹å†…çš„æ™ºèƒ½èœå• (æ ¹æ®ä½ çš„å›¾ç‰‡ç»“æž„è¿˜åŽŸ) ===\r\n      courseSmart: [\r\n        {\r\n          name: 'é¦–é¡µ',\r\n          children: [\r\n            // å¯¹åº”å›¾ç‰‡ï¼šé¦–é¡µï¼šæˆ‘çš„è¯¾ç¨‹ (è¿™é‡ŒæŒ‡è¯¾ç¨‹æ¦‚è§ˆ)\r\n            { name: 'è¯¾ç¨‹æ¦‚è§ˆ', path: 'overview', icon: 'ðŸ ' },\r\n            { name: 'çŸ¥è¯†å›¾è°±', path: 'learning-path', icon: 'ðŸ§ ' },\r\n          ]\r\n        },\r\n        {\r\n          name: 'è€ƒå‹¤ç®¡ç†', // å¯¹åº”å›¾ç‰‡\r\n          children: [\r\n            { name: 'å‘èµ·ç­¾åˆ°', path: 'attendance/launch', icon: 'ðŸ“' },\r\n            { name: 'æŸ¥çœ‹è€ƒå‹¤è¯¦æƒ…', path: 'attendance/detail', icon: 'ðŸ‘ï¸' },\r\n            { name: 'ä¿®æ”¹è€ƒå‹¤', path: 'attendance/modify', icon: 'âœï¸' },\r\n          ]\r\n        },\r\n        {\r\n          name: 'ä½œä¸šç®¡ç†', // å¯¹åº”å›¾ç‰‡\r\n          children: [\r\n            { name: 'å‘å¸ƒä½œä¸š', path: 'homework/publish', icon: 'ðŸ“¤' },\r\n            { name: 'æ‰¹æ”¹ä½œä¸š(AI)', path: 'homework/correct', icon: 'ð