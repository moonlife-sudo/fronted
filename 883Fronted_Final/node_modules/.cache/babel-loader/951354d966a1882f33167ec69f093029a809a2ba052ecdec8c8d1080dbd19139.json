tId;\n    },\n    editAssignment: function editAssignment(student) {\n      // 这里可以打开编辑模态框或跳转到编辑页面\n      alert(\"\\u7F16\\u8F91 \".concat(student.name, \" \\u7684\\u5BBF\\u820D\\u5206\\u914D\"));\n    },\n    changeAdminPage: function changeAdminPage(page) {\n      if (page >= 1 && page <= this.adminTotalPages) {\n        this.adminCurrentPage = page;\n      }\n    },\n    submitQuestionnaire: function submitQuestionnaire() {\n      var _this2 = this;\n      return _asyncToGenerator(/*#__PURE__*/_regenerator().m(function _callee() {\n        return _regenerator().w(function (_context) {\n          while (1) switch (_context.n) {\n            case 0:\n              if (!_this2.isSubmitting) {\n                _context.n = 1;\n                break;\n              }\n              return _context.a(2);\n            case 1:\n              _this2.isSubmitting = true;\n\n              // 模拟网络请求\n              _context.n = 2;\n              return new Promise(function (resolve) {\n                return setTimeout(resolve, 1500);\n              });\n            case 2:\n              // 模拟成功提交\n              console.log('提交的问卷数据:', _this2.questionnaire);\n              alert('问卷提交成功！感谢您的参与。');\n\n              // 重置表单状态\n              _this2.isSubmitting = false;\n\n              // 这里可以添加实际提交逻辑\n              // 例如：this.$api.submitQuestionnaire(this.questionnaire)\n            case 3:\n              return _context.a(2);\n          }\n        }, _callee);\n      }))();\n    }\n  },\n  mounted: function mounted() {\n    // 初始化过滤数据\n    this.filteredAdminData = _toConsumableArray(this.adminData);\n  }\n};","map":{"version":3,"names":["name","data","currentView","adminSearch","selectedBuilding","selectedStudent","adminCurrentPage","adminPageSize","isSubmitting","adminData","id","studentId","gender","building","room","bed","filteredAdminData","questionnaire","sleepTime","preferences","roomType","specialRequest","assignmentResult","computed","_this","filtered","searchTerm","