\r\n          </div>\r\n          <button class=\"enter-btn\">ËøõÂÖ•ËØæÁ®ã</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\n\r\nexport default {\r\n  name: 'StudentTeachingIndex',\r\n  setup() {\r\n    const router = useRouter()\r\n    const loading = ref(false)\r\n    const courses = ref([])\r\n\r\n    // ÁîüÊàêÈöèÊú∫ÊüîÂíåËÉåÊôØËâ≤\r\n    const getCourseColor = (id) => {\r\n      const colors = ['#5C6BC0', '#42A5F5', '#66BB6A', '#FFA726', '#EF5350', '#AB47BC']\r\n      return colors[id % colors.length]\r\n    }\r\n\r\n    const loadCourses = async () => {\r\n      loading.value = true\r\n      try {\r\n        // Ê®°ÊãüÊï∞ÊçÆÔºåËØ∑ÊõøÊç¢‰∏∫ÁúüÂÆû API Ë∞ÉÁî®\r\n        setTimeout(() => {\r\n          courses.value = [\r\n            { \r\n              class_id: 501, \r\n              course_name: 'ËÆ°ÁÆóÊú∫ÂØºËÆ∫', \r\n              teacher_name: 'Âº†‰∏â', \r\n              semester: '2025-Fall', \r\n              status: 2 \r\n            },\r\n            { \r\n              class_id: 502, \r\n              course_name: 'È´òÁ≠âÊï∞Â≠¶', \r\n              teacher_name: 'ÊùéÂõõ', \r\n              semester: '2025-Fall', \r\n              status: 2 \r\n            },\r\n            { \r\n              class_id: 503, \r\n              course_name: 'WebÂâçÁ´ØÂºÄÂèë', \r\n              teacher_name: 'Áéã‰∫î', \r\n              semester: '2025-Fall', \r\n              status: 2 \r\n            },\r\n          ]\r\n          loading.value = false\r\n        }, 500)\r\n      } catch (e) {\r\n        loading.value = false\r\n      }\r\n    }\r\n\r\n    const enterCourse = (course) => {\r\n      // 1. Â≠òÊï∞ÊçÆ (Sidebar ÈúÄË¶ÅÁî®Âà∞Ëøô‰∫õ‰ø°ÊÅØ)\r\n      localStorage.setItem('currentClassId', course.class_id.toString())\r\n      localStorage.setItem('currentCourseName', course.course_name)\r\n      localStorage.setItem('currentTeacherName',