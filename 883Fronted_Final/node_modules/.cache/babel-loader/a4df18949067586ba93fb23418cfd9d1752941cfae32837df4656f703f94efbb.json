{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nexport default {\n  name: 'AdminLibrary',\n  components: {},\n  data() {\n    return {\n      // 书籍数据\n      books: [{\n        id: 1,\n        title: \"JavaScript高级程序设计\",\n        author: \"Nicholas C. Zakas\",\n        cover: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/JavaScript高级程序设计\",\n        category: \"科技\",\n        isbn: \"9787115275790\",\n        status: \"available\",\n        description: \"本书详细讲解了JavaScript语言的核心概念和技术细节。\"\n      }, {\n        id: 2,\n        title: \"CSS揭秘\",\n        author: \"Lea Verou\",\n        cover: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/CSS揭秘\",\n        category: \"科技\",\n        isbn: \"9787115433459\",\n        status: \"borrowed\",\n        description: \"本书揭示了47个鲜为人知的CSS技巧。\"\n      }, {\n        id: 3,\n        title: \"百年孤独\",\n        author: \"加西亚·马尔克斯\",\n        cover: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/百年孤独\",\n        category: \"文学\",\n        isbn: \"9787544258974\",\n        status: \"available\",\n        description: \"拉丁美洲魔幻现实主义文学的代表作。\"\n      }, {\n        id: 4,\n        title: \"人类简史\",\n        author: \"尤瓦尔·赫拉利\",\n        cover: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/人类简史\",\n        category: \"历史\",\n        isbn: \"9787508647357\",\n        status: \"available\",\n        description: \"从认知革命到科学革命的人类发展历程。\"\n      }, {\n        id: 5,\n        title: \"艺术的故事\",\n        author: \"E.H.贡布里希\",\n        cover: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/艺术的故事\",\n        category: \"艺术\",\n        isbn: \"9787108018511\",\n        status: \"borrowed\",\n        description: \"艺术史入门经典读物。\"\n      }, {\n        id: 6,\n        title: \"设计心理学\",\n        author: \"唐纳德·A·诺曼\",\n        cover: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/设计心理学\",\n        category: \"科技\",\n        isbn: \"9787121285862\",\n        status: \"available\",\n        description: \"探讨日常用品的设计与用户心理的关系。\"\n      }],\n      // 筛选条件\n      searchQuery: '',\n      categoryFilter: '',\n      statusFilter: '',\n      // 模态框状态\n      showUploadModal: false,\n      showLoanDurationModal: false,\n      editingBook: null,\n      // 表单数据\n      bookForm: {\n        title: '',\n        author: '',\n        isbn: '',\n        category: '',\n        description: '',\n        cover: null\n      },\n      // 借书时长\n      loanDuration: 14,\n      // Toast 提示\n      toast: {\n        show: false,\n        message: ''\n      }\n    };\n  },\n  computed: {\n    // 筛选后的书籍列表\n    filteredBooks() {\n      let filtered = this.books;\n      if (this.searchQuery) {\n        const query = this.searchQuery.toLowerCase();\n        filtered = filtered.filter(book => book.title.toLowerCase().includes(query) || book.author.toLowerCase().includes(query));\n      }\n      if (this.categoryFilter) {\n        filtered = filtered.filter(book => book.category === this.categoryFilter);\n      }\n      if (this.statusFilter) {\n        filtered = filtered.filter(book => book.status === this.statusFilter);\n      }\n      return filtered;\n    }\n  },\n  methods: {\n    // 显示上传模态框\n    openUploadModal() {\n      this.editingBook = null;\n      this.bookForm = {\n        title: '',\n        author: '',\n        isbn: '',\n        category: '',\n        description: '',\n        cover: null\n      };\n      this.showUploadModal = true;\n    },\n    // 显示借书时长模态框\n    LoanDurationModal() {\n      this.showLoanDurationModal = true;\n    },\n    // 关闭所有模态框\n    closeModals() {\n      this.showUploadModal = false;\n      this.showLoanDurationModal = false;\n      this.editingBook = null;\n    },\n    // 处理书籍提交（新增或编辑）\n    handleBookSubmit() {\n      if (this.editingBook) {\n        // 更新书籍\n        const index = this.books.findIndex(b => b.id === this.editingBook.id);\n        if (index !== -1) {\n          this.books[index] = {\n            ...this.editingBook,\n            ...this.bookForm\n          };\n          this.showToast('书籍信息更新成功！');\n        }\n      } else {\n        // 新增书籍\n        const newBook = {\n          id: Date.now(),\n          ...this.bookForm,\n          status: \"available\",\n          cover: this.bookForm.cover || `https://placeholder.pics/svg/300x200/DEDEDE/555555/${encodeURIComponent(this.bookForm.title)}`\n        };\n        this.books.unshift(newBook);\n        this.showToast('书籍上传成功！');\n      }\n      this.closeModals();\n    },\n    // 编辑书籍\n    editBook(book) {\n      this.editingBook = book;\n      this.bookForm = {\n        ...book\n      };\n      this.showUploadModal = true;\n    },\n    // 删除书籍\n    deleteBook(bookId) {\n      if (confirm('确定要删除这本书吗？')) {\n        const index = this.books.findIndex(b => b.id === bookId);\n        if (index !== -1) {\n          this.books.splice(index, 1);\n          this.showToast('书籍已删除');\n        }\n      }\n    },\n    // 处理封面图片上传\n    handleCoverUpload(event) {\n      const file = event.target.files[0];\n      if (file) {\n        // 在实际应用中，这里应该上传到服务器并获取URL\n        // 这里简单使用本地URL预览\n        this.bookForm.cover = URL.createObjectURL(file);\n      }\n    },\n    // 保存借书时长设置\n    saveLoanDuration() {\n      // 在实际应用中，这里会发送到服务器保存设置\n      console.log(`借书时长设置为: ${this.loanDuration}天`);\n      this.showToast(`借书时长已设置为${this.loanDuration}天`);\n      this.closeModals();\n    },\n    // 显示 Toast 提示\n    showToast(message) {\n      this.toast.message = message;\n      this.toast.show = true;\n      setTimeout(() => {\n        this.toast.show = false;\n      }, 3000);\n    }\n  }\n};","map":{"version":3,"names":["name","components","data","books","id","title","author","cover","category","isbn","status","description","searchQuery","categoryFilter","statusFilter","showUploadModal","showLoanDurationModal","editingBook","bookForm","loanDuration","toast","show","message","computed","filteredBooks","filtered","query","toLowerCase","filter","book","includes","methods","openUploadModal","LoanDurationModal","closeModals","handleBookSubmit","index","findIndex","b","showToast","newBook","Date","now","encodeURIComponent","unshift","editBook","deleteBook","bookId","confirm","splice","handleCoverUpload","event","file","target","files","URL","createObjectURL","saveLoanDuration","console","log","setTimeout"],"sources":["/Users/zhouzhou/Desktop/resource/src/views/AdminLibrary.vue"],"sourcesContent":["<template>\n  <div class=\"admin-library\">\n    \n    <main class=\"admin-library-container\">\n      <section class=\"library-controls\">\n        <h1 class=\"page-title\">图书馆管理</h1>\n        <div class=\"action-buttons\">\n          <button @click=\"showUploadModal\" class=\"btn btn-primary\">\n            <i class=\"bi bi-plus-circle\"></i> 上传新书\n          </button>\n          <button @click=\"showLoanDurationModal\" class=\"btn btn-secondary\">\n            <i class=\"bi bi-clock\"></i> 设置借书时长\n          </button>\n        </div>\n      </section>\n\n      <section class=\"search-filter-section\">\n        <div class=\"search-box\">\n          <input \n            type=\"text\" \n            placeholder=\"搜索书籍...\" \n            v-model=\"searchQuery\"\n            @input=\"filterBooks\"\n          >\n          <button class=\"search-btn\"><i class=\"bi bi-search\"></i></button>\n        </div>\n        <div class=\"filter-options\">\n          <select v-model=\"categoryFilter\" @change=\"filterBooks\">\n            <option value=\"\">所有分类</option>\n            <option value=\"文学\">文学</option>\n            <option value=\"科技\">科技</option>\n            <option value=\"历史\">历史</option>\n            <option value=\"艺术\">艺术</option>\n          </select>\n          <select v-model=\"statusFilter\" @change=\"filterBooks\">\n            <option value=\"\">所有状态</option>\n            <option value=\"available\">可借阅</option>\n            <option value=\"borrowed\">已借出</option>\n          </select>\n        </div>\n      </section>\n\n      <section class=\"book-list\">\n        <div class=\"book-list-header\">\n          <h2>书籍列表</h2>\n          <span class=\"book-count\">共 <span id=\"total-books\">{{ filteredBooks.length }}</span> 本</span>\n        </div>\n        \n        <div class=\"books-grid\">\n          <div \n            v-for=\"book in filteredBooks\" \n            :key=\"book.id\" \n            class=\"book-card\"\n          >\n            <img :src=\"book.cover\" :alt=\"book.title\" class=\"book-cover\" loading=\"lazy\">\n            <div class=\"book-info\">\n              <h3 class=\"book-title\">{{ book.title }}</h3>\n              <p class=\"book-author\">{{ book.author }}</p>\n              <div class=\"book-meta\">\n                <span class=\"book-category\">{{ book.category }}</span>\n                <span class=\"book-status\" :class=\"book.status\">\n                  {{ book.status === 'available' ? '可借阅' : '已借出' }}\n                </span>\n              </div>\n              <div class=\"book-actions\">\n                <button @click=\"editBook(book)\" class=\"btn btn-secondary\">\n                  <i class=\"bi bi-pencil\"></i> 编辑\n                </button>\n                <button @click=\"deleteBook(book.id)\" class=\"btn btn-danger\">\n                  <i class=\"bi bi-trash\"></i> 删除\n                </button>\n              </div>\n            </div>\n          </div>\n          \n          <div v-if=\"filteredBooks.length === 0\" class=\"no-books\">\n            没有找到匹配的书籍\n          </div>\n        </div>\n      </section>\n\n      <!-- 上传书籍模态框 -->\n      <div v-if=\"showUploadModal\" class=\"modal show\" @click.self=\"closeModals\">\n        <div class=\"modal-content\">\n          <span class=\"close-modal\" @click=\"closeModals\">&times;</span>\n          <h2>{{ editingBook ? '编辑书籍' : '上传新书' }}</h2>\n          <form @submit.prevent=\"handleBookSubmit\">\n            <div class=\"form-group\">\n              <label for=\"book-title\">书名</label>\n              <input type=\"text\" id=\"book-title\" v-model=\"bookForm.title\" required>\n            </div>\n            <div class=\"form-group\">\n              <label for=\"book-author\">作者</label>\n              <input type=\"text\" id=\"book-author\" v-model=\"bookForm.author\" required>\n            </div>\n            <div class=\"form-group\">\n              <label for=\"book-isbn\">ISBN</label>\n              <input type=\"text\" id=\"book-isbn\" v-model=\"bookForm.isbn\" required>\n            </div>\n            <div class=\"form-group\">\n              <label for=\"book-category\">分类</label>\n              <select id=\"book-category\" v-model=\"bookForm.category\" required>\n                <option value=\"\">选择分类</option>\n                <option value=\"文学\">文学</option>\n                <option value=\"科技\">科技</option>\n                <option value=\"历史\">历史</option>\n                <option value=\"艺术\">艺术</option>\n              </select>\n            </div>\n            <div class=\"form-group\">\n              <label for=\"book-cover\">封面图片</label>\n              <input type=\"file\" id=\"book-cover\" accept=\"image/*\" @change=\"handleCoverUpload\">\n            </div>\n            <div class=\"form-group\">\n              <label for=\"book-description\">简介</label>\n              <textarea id=\"book-description\" rows=\"4\" v-model=\"bookForm.description\"></textarea>\n            </div>\n            <button type=\"submit\" class=\"btn btn-primary\">\n              {{ editingBook ? '更新' : '上传' }}\n            </button>\n          </form>\n        </div>\n      </div>\n\n      <!-- 设置借书时长模态框 -->\n      <div v-if=\"showLoanDurationModal\" class=\"modal show\" @click.self=\"closeModals\">\n        <div class=\"modal-content\">\n          <span class=\"close-modal\" @click=\"closeModals\">&times;</span>\n          <h2>设置借书时长</h2>\n          <form @submit.prevent=\"saveLoanDuration\">\n            <div class=\"form-group\">\n              <label for=\"loan-duration\">借书时长 (天)</label>\n              <input type=\"number\" id=\"loan-duration\" v-model.number=\"loanDuration\" min=\"1\" max=\"30\" required>\n            </div>\n            <button type=\"submit\" class=\"btn btn-primary\">保存设置</button>\n          </form>\n        </div>\n      </div>\n    </main>\n    \n\n    <!-- Toast 提示 -->\n    <div v-if=\"toast.show\" class=\"toast show\">\n      {{ toast.message }}\n    </div>\n  </div>\n</template>\n\n<script>\n\nexport default {\n  name: 'AdminLibrary',\n  components: {\n\n  },\n  data() {\n    return {\n      // 书籍数据\n      books: [\n        {\n          id: 1,\n          title: \"JavaScript高级程序设计\",\n          author: \"Nicholas C. Zakas\",\n          cover: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/JavaScript高级程序设计\",\n          category: \"科技\",\n          isbn: \"9787115275790\",\n          status: \"available\",\n          description: \"本书详细讲解了JavaScript语言的核心概念和技术细节。\"\n        },\n        {\n          id: 2,\n          title: \"CSS揭秘\",\n          author: \"Lea Verou\",\n          cover: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/CSS揭秘\",\n          category: \"科技\",\n          isbn: \"9787115433459\",\n          status: \"borrowed\",\n          description: \"本书揭示了47个鲜为人知的CSS技巧。\"\n        },\n        {\n          id: 3,\n          title: \"百年孤独\",\n          author: \"加西亚·马尔克斯\",\n          cover: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/百年孤独\",\n          category: \"文学\",\n          isbn: \"9787544258974\",\n          status: \"available\",\n          description: \"拉丁美洲魔幻现实主义文学的代表作。\"\n        },\n        {\n          id: 4,\n          title: \"人类简史\",\n          author: \"尤瓦尔·赫拉利\",\n          cover: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/人类简史\",\n          category: \"历史\",\n          isbn: \"9787508647357\",\n          status: \"available\",\n          description: \"从认知革命到科学革命的人类发展历程。\"\n        },\n        {\n          id: 5,\n          title: \"艺术的故事\",\n          author: \"E.H.贡布里希\",\n          cover: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/艺术的故事\",\n          category: \"艺术\",\n          isbn: \"9787108018511\",\n          status: \"borrowed\",\n          description: \"艺术史入门经典读物。\"\n        },\n        {\n          id: 6,\n          title: \"设计心理学\",\n          author: \"唐纳德·A·诺曼\",\n          cover: \"https://placeholder.pics/svg/300x200/DEDEDE/555555/设计心理学\",\n          category: \"科技\",\n          isbn: \"9787121285862\",\n          status: \"available\",\n          description: \"探讨日常用品的设计与用户心理的关系。\"\n        }\n      ],\n      // 筛选条件\n      searchQuery: '',\n      categoryFilter: '',\n      statusFilter: '',\n      // 模态框状态\n      showUploadModal: false,\n      showLoanDurationModal: false,\n      editingBook: null,\n      // 表单数据\n      bookForm: {\n        title: '',\n        author: '',\n        isbn: '',\n        category: '',\n        description: '',\n        cover: null\n      },\n      // 借书时长\n      loanDuration: 14,\n      // Toast 提示\n      toast: {\n        show: false,\n        message: ''\n      }\n    }\n  },\n  computed: {\n    // 筛选后的书籍列表\n    filteredBooks() {\n      let filtered = this.books\n      \n      if (this.searchQuery) {\n        const query = this.searchQuery.toLowerCase()\n        filtered = filtered.filter(book => \n          book.title.toLowerCase().includes(query) || \n          book.author.toLowerCase().includes(query)\n        )\n      }\n      \n      if (this.categoryFilter) {\n        filtered = filtered.filter(book => book.category === this.categoryFilter)\n      }\n      \n      if (this.statusFilter) {\n        filtered = filtered.filter(book => book.status === this.statusFilter)\n      }\n      \n      return filtered\n    }\n  },\n  methods: {\n    // 显示上传模态框\n    openUploadModal() {\n      this.editingBook = null\n      this.bookForm = {\n        title: '',\n        author: '',\n        isbn: '',\n        category: '',\n        description: '',\n        cover: null\n      }\n      this.showUploadModal = true\n    },\n    // 显示借书时长模态框\n    LoanDurationModal() {\n      this.showLoanDurationModal = true\n    },\n    // 关闭所有模态框\n    closeModals() {\n      this.showUploadModal = false\n      this.showLoanDurationModal = false\n      this.editingBook = null\n    },\n    // 处理书籍提交（新增或编辑）\n    handleBookSubmit() {\n      if (this.editingBook) {\n        // 更新书籍\n        const index = this.books.findIndex(b => b.id === this.editingBook.id)\n        if (index !== -1) {\n          this.books[index] = {\n            ...this.editingBook,\n            ...this.bookForm\n          }\n          this.showToast('书籍信息更新成功！')\n        }\n      } else {\n        // 新增书籍\n        const newBook = {\n          id: Date.now(),\n          ...this.bookForm,\n          status: \"available\",\n          cover: this.bookForm.cover || `https://placeholder.pics/svg/300x200/DEDEDE/555555/${encodeURIComponent(this.bookForm.title)}`\n        }\n        this.books.unshift(newBook)\n        this.showToast('书籍上传成功！')\n      }\n      \n      this.closeModals()\n    },\n    // 编辑书籍\n    editBook(book) {\n      this.editingBook = book\n      this.bookForm = { ...book }\n      this.showUploadModal = true\n    },\n    // 删除书籍\n    deleteBook(bookId) {\n      if (confirm('确定要删除这本书吗？')) {\n        const index = this.books.findIndex(b => b.id === bookId)\n        if (index !== -1) {\n          this.books.splice(index, 1)\n          this.showToast('书籍已删除')\n        }\n      }\n    },\n    // 处理封面图片上传\n    handleCoverUpload(event) {\n      const file = event.target.files[0]\n      if (file) {\n        // 在实际应用中，这里应该上传到服务器并获取URL\n        // 这里简单使用本地URL预览\n        this.bookForm.cover = URL.createObjectURL(file)\n      }\n    },\n    // 保存借书时长设置\n    saveLoanDuration() {\n      // 在实际应用中，这里会发送到服务器保存设置\n      console.log(`借书时长设置为: ${this.loanDuration}天`)\n      this.showToast(`借书时长已设置为${this.loanDuration}天`)\n      this.closeModals()\n    },\n    // 显示 Toast 提示\n    showToast(message) {\n      this.toast.message = message\n      this.toast.show = true\n      setTimeout(() => {\n        this.toast.show = false\n      }, 3000)\n    }\n  }\n}\n</script>\n\n<style scoped>\n/* 使用您提供的 adminLibrary.css 样式，转换为 scoped 样式 */\n.admin-library {\n  --primary-color: #2A5CAA;\n  --secondary-color: #4CAF50;\n  --light-color: #FFFFFF;\n  --dark-color: #333333;\n  --gray-color: #F5F5F5;\n  --border-color: #E0E0E0;\n  --shadow-color: rgba(0, 0, 0, 0.1);\n  --success-color: #4CAF50;\n  --warning-color: #FF9800;\n  --danger-color: #F44336;\n  --border-radius: 8px;\n  --transition: all 0.3s ease;\n}\n\n.admin-library {\n  font-family: 'Roboto', 'PingFang SC', 'Microsoft YaHei', sans-serif;\n  color: var(--dark-color);\n  background-color: var(--gray-color);\n  line-height: 1.6;\n  display: flex;\n  flex-direction: column;\n  min-height: 100vh;\n}\n\n/* 主内容区域 */\n.admin-library-container {\n  flex: 1;\n  padding: 2rem;\n  max-width: 1200px;\n  margin: 0 auto;\n  width: 100%;\n}\n\n.page-title {\n  font-size: 2rem;\n  color: var(--primary-color);\n  margin-bottom: 1.5rem;\n}\n\n/* 控制区域 */\n.library-controls {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 2rem;\n}\n\n.action-buttons {\n  display: flex;\n  gap: 1rem;\n}\n\n/* 按钮样式 */\n.btn {\n  padding: 0.75rem 1.5rem;\n  border: none;\n  border-radius: var(--border-radius);\n  font-weight: 500;\n  cursor: pointer;\n  transition: var(--transition);\n  display: flex;\n  align-items: center;\n  gap: 0.5rem;\n}\n\n.btn-primary {\n  background-color: var(--primary-color);\n  color: var(--light-color);\n}\n\n.btn-primary:hover {\n  background-color: #1E4B8B;\n  transform: translateY(-2px);\n}\n\n.btn-secondary {\n  background-color: var(--secondary-color);\n  color: var(--light-color);\n}\n\n.btn-secondary:hover {\n  background-color: #3E8E41;\n  transform: translateY(-2px);\n}\n\n.btn-danger {\n  background-color: var(--danger-color);\n  color: var(--light-color);\n}\n\n.btn-danger:hover {\n  background-color: #D32F2F;\n  transform: translateY(-2px);\n}\n\n/* 搜索和筛选区域 */\n.search-filter-section {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  gap: 1rem;\n  margin-bottom: 2rem;\n  background-color: var(--light-color);\n  padding: 1.5rem;\n  border-radius: var(--border-radius);\n  box-shadow: 0 2px 8px var(--shadow-color);\n}\n\n.search-box {\n  display: flex;\n  flex: 1;\n  min-width: 300px;\n}\n\n.search-box input {\n  flex: 1;\n  padding: 0.75rem 1rem;\n  border: 1px solid var(--border-color);\n  border-radius: var(--border-radius) 0 0 var(--border-radius);\n  font-size: 1rem;\n  transition: var(--transition);\n}\n\n.search-box input:focus {\n  outline: none;\n  border-color: var(--primary-color);\n}\n\n.search-btn {\n  padding: 0 1.25rem;\n  background-color: var(--primary-color);\n  color: var(--light-color);\n  border: none;\n  border-radius: 0 var(--border-radius) var(--border-radius) 0;\n  cursor: pointer;\n  transition: var(--transition);\n}\n\n.search-btn:hover {\n  background-color: #1E4B8B;\n}\n\n.filter-options {\n  display: flex;\n  gap: 1rem;\n}\n\n.filter-options select {\n  padding: 0.75rem;\n  border: 1px solid var(--border-color);\n  border-radius: var(--border-radius);\n  font-size: 1rem;\n  background-color: var(--light-color);\n  transition: var(--transition);\n}\n\n.filter-options select:focus {\n  outline: none;\n  border-color: var(--primary-color);\n}\n\n/* 书籍列表区域 */\n.book-list {\n  background-color: var(--light-color);\n  padding: 1.5rem;\n  border-radius: var(--border-radius);\n  box-shadow: 0 2px 8px var(--shadow-color);\n}\n\n.book-list-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 1.5rem;\n}\n\n.book-list-