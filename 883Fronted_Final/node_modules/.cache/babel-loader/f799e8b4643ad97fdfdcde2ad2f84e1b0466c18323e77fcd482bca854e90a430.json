        <div class=\"form-group\">\r\n          <label for=\"homeworkSelect\">ÈÄâÊã©‰Ωú‰∏ö</label>\r\n          <select id=\"homeworkSelect\" v-model=\"selectedHomework\" class=\"input\">\r\n            <option value=\"\">ËØ∑ÈÄâÊã©‰Ωú‰∏ö</option>\r\n            <option v-for=\"homework in homeworkList\" :key=\"homework.id\" :value=\"homework\">\r\n              {{ homework.title }}\r\n            </option>\r\n          </select>\r\n        </div>\r\n\r\n        <!-- Â≠¶ÁîüÈÄâÊã©Âå∫Âüü -->\r\n        <div class=\"form-group\">\r\n          <label>ÈÄâÊã©Â≠¶Áîü</label>\r\n          <div class=\"students-select\">\r\n            <div v-for=\"student in studentsList\" :key=\"student.id\" class=\"student-item\">\r\n              <input type=\"checkbox\" :id=\"`student-${student.id}`\" :value=\"student\" v-model=\"selectedStudents\" />\r\n              <label :for=\"`student-${student.id}`\">{{ student.name }}</label>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"selected-info\">\r\n          <p v-if=\"selectedStudents.length > 0\">\r\n            Â∑≤ÈÄâÊã© {{ selectedStudents.length }} ÂêçÂ≠¶Áîü\r\n          </p>\r\n          <p v-else class=\"text-muted\">\r\n            ËØ∑ÈÄâÊã©Ëá≥Â∞ë‰∏ÄÂêçÂ≠¶Áîü\r\n          </p>\r\n        </div>\r\n\r\n        <div class=\"form-actions\">\r\n          <button type=\"button\" class=\"button\" @click=\"generateTeachingSuggestions\"\r\n            :disabled=\"generatingSuggestions || !selectedHomework || selectedStudents.length === 0\">\r\n            {{ generatingSuggestions ? 'ÁîüÊàê‰∏≠...' : 'ÁîüÊàêÊïôÂ≠¶Âª∫ËÆÆ' }}\r\n          </button>\r\n        </div>\r\n\r\n        <div v-if=\"generatingSuggestions\" class=\"loading-state\">Ê≠£Âú®ÁîüÊàêÊïôÂ≠¶Âª∫ËÆÆ...</div>\r\n\r\n        <div v-else-if=\"errorMessage\" class=\"error-state\">{{ errorMessage }}</div>\r\n\r\n        <div v-else-if=\"teachingSuggestions && teachingSuggestions.length > 0\" class=\"suggestions-result\">\r\n          <h4>ÁîüÊ