r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"course-grid\">\r\n      <div v-for=\"course in courses\" :key=\"course.class_id\" class=\"course-card\" @click=\"selectCourse(course)\">\r\n        <div class=\"card-cover\" :style=\"{ background: course.color }\">\r\n          <span class=\"semester-tag\">{{ course.semester }}</span>\r\n          <span class=\"course-type\">{{ course.type }}</span>\r\n        </div>\r\n\r\n        <div class=\"card-content\">\r\n          <h3 class=\"course-title\">{{ course.course_name }}</h3>\r\n          <div class=\"course-meta\">\r\n            <div class=\"meta-item\">\r\n              <i class=\"bi bi-people-fill icon\"></i>\r\n              <span>选课人数: {{ course.current_enrollment }}</span>\r\n            </div>\r\n            <div class=\"meta-item\">\r\n              <i class=\"bi bi-geo-alt-fill icon\"></i>\r\n              <span>{{ course.location }}</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"status-row\">\r\n            <span class=\"status-badge\" :class=\"course.statusClass\">{{ course.statusText }}</span>\r\n            <button class=\"enter-btn\">进入管理</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\n\r\ndefineOptions({ name: 'TeachingIndex' })\r\nconst router = useRouter()\r\n\r\n// === 演示专用：写死的漂亮数据 ===\r\nconst courses = ref([\r\n  {\r\n    class_id: 101,\r\n    course_name: '高级软件工程与架构',\r\n    semester: '2025秋',\r\n    type: '学位课',\r\n    location: '教三-201',\r\n    current_enrollment: 45,\r\n    statusText: '授课中',\r\n    statusClass: 'ongoing',\r\n    color: 'linear-gradient(135deg, #667eea 0%, #764ba2 100%)' // 紫色渐变\r\n  },\r\n  {\r\n    class_id: 102,\r\n    course_name: '深度学习与计算机视觉',\r\n    semester: '2025秋',\r\n    type: '选修课',\r\n    location: '科技楼-405',\r\n    current_enrollment: 62,\r\n    statusText: '授课中',\r\n    statusClass: 'ongoing',\r\n    color: 'linear-gradient(135deg, #2af598 0%, #009efd 100%)' // 蓝绿渐变\r\n  },\r\n  {\r\n    class_id: 103,\r\n    course_name: '分布式系统导论',\r\n    semester: '2025秋',\r\n    type: '核心课',\r\n    location: '信工楼-102',\r\n    current_enrollment: 38,\r\n    statusText: '授课中',\r\n    statusClass: 'ongoing',\r\n    color: 'linear-gradient(135deg, #ff9a9e 0%, #fecfef 99%, #fecfef 100%)' // 粉色渐变\r\n  },\r\n  {\r\n    class_id: 104,\r\n    course_name: '城市大数据分析', // 结合你之前的专业\r\n    semester: '2025秋',\r\n    type: '跨学科',\r\n    location: '综合楼-303',\r\n    current_enrollment: 55,\r\n    statusText: '即将结课',\r\n    statusClass: 'ending',\r\n    color: 'linear-gradient(120deg, #f6d365 0%, #fda085 100%)' // 橙黄渐变\r\n  }\r\n])\r\n\r\nconst selectCourse = (course) => {\r\n  sessionStorage.setItem('selectedClassId', course.class_id);\r\n  sessionStorage.setItem('selectedCourseName', course.course_name);\r\n  router.push(`/teacher/course/${course.class_id}/overview`);\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.teaching-index { padding: 24px; max-width: 1400px; margin: 0 auto; }\r\n.page-header { margin-bottom: 32px; border-left: 5px solid #2A5CAA; padding-left: 15px; }\r\n.page-header h1 { font-size: 26px; color: #333; margin: 0 0 5px 0; }\r\n.page-header p { color: #666; margin: 0; }\r\n\r\n.course-grid { display: grid; grid-template-columns: repeat(auto-fill, minmax(320px, 1fr)); gap: 24px; }\r\n\r\n.course-card {\r\n  background: #fff; border-radius: 16px; overflow: hidden;\r\n  box-shadow: 0 8px 20px rgba(0, 0, 0, 0.05); cursor: pointer;\r\n  transition: transform 0.3s, box-shadow 0.3s; border: 1px solid #f0f0f0;\r\n}\r\n.course-card:hover { transform: translateY(-5px); box-shadow: 0 12px 28px rgba(0, 0, 0, 0.12); }\r\n\r\n.card-cover {\r\n  height: 100px; padding: 16px; position: relative;\r\n  display: flex; justify-content: space-between; align-items: flex-start;\r\n}\r\n.semester-tag, .course-type {\r\n  background: rgba(255, 255, 255, 0.25); color: #fff; padding: 4px 10px;\r\n  border-radius: 20px; font-size: 12px; backdrop-filter: blur(4px); font-weight: 600;\r\n}\r\n\r\n.card-content { padding: 20px; }\r\n.course-title { font-size: 18px; font-weight: 700; color: #333; margin: 0 0 15px 0; line-height: 1.4; height: 50px; overflow: hidden; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; }\r\n\r\n.course-meta { margin-bottom: 20px; display: flex; flex-direction: column; gap: 8px; }\r\n.meta-item { display: flex; align-items: center; gap: 8px; font-size: 13px; color: #666; }\r\n.icon { color: #999; }\r\n\r\n.status-row { display: flex; justify-content: space-between; align-items: center; border-top: 1px solid #f5f5f5; padding-top: 15px; }\r\n.status-badge { padding: 4px 12px; border-radius: 6px; font-size: 12px; font-weight: 500; }\r\n.status-badge.ongoing { background: #e6f7ff; color: #1890ff; }\r\n.status-badge.ending { background: #fff7e6; color: #fa8c16; }\r\n\r\n.enter-btn {\r\n  background: #2A5CAA; color: #fff; border: none; padding: 6px 16px;\r\n  border-radius: 20px; font-size: 13px; cursor: pointer; transition: opacity 0.2s;\r\n}\r\n.enter-btn:hover { opacity: 0.9; }\r\n</style>"],"mappings":";;EACO,SAAM;AAAgB;;EAQpB,SAAM;AAAa;;;EAGZ,SAAM;AAAc;;EACpB,SAAM;AAAa;;EAGtB,SAAM;AAAc;;EACnB,SAAM;AAAc;;EACnB,SAAM;AAAa;;EACjB,SAAM;AAAW;;EAIjB,SAAM;AAAW;;EAKnB,SAAM;AAAY;;uBA3B/BA,mBAAA,CAkCM,OAlCNC,UAkCM,G,0BAjCJC,mBAAA,CAKM;IALD,SAAM;EAAa,IACtBA,mBAAA,CAGM;IAHD,SAAM;EAAgB,IACzBA,mBAAA,CAAa,YAAT,MAAI,GACRA,mBAAA,CAAuB,WAApB,kBAAgB,E,uBAIvBA,mBAAA,CAyBM,OAzBNC,UAyBM,I,kBAxBJH,mBAAA,CAuBMI,SAAA,QAAAC,WAAA,CAvBgBC,MAAA,CAAAC,OAAO,YAAjBC,MAAM;yBAAlBR,mBAAA,CAuBM;MAvB0BS,GAAG,EAAED,MAAM,CAACE,QAAQ;MAAE,SAAM,aAAa;MAAEC,OAAK,WAALA,OAAKA,CAAAC,MAAA;QAAA,OAAEN,MAAA,CAAAO,YAAY,CAACL,MAAM;MAAA;QACnGN,mBAAA,CAGM;MAHD,SAAM,YAAY;MAAEY,KAAK,EAAAC,eAAA;QAAAC,UAAA,EAAgBR,MAAM,CAACS;MAAK;QACxDf,mBAAA,CAAuD,QAAvDgB,UAAuD,EAAAC,gBAAA,CAAzBX,MAAM,CAACY,QAAQ,kBAC7ClB,mBAAA,CAAkD,QAAlDmB,UAAkD,EAAAF,gBAAA,CAArBX,MAAM,CA