{"ast":null,"code":"import \"core-js/modules/es.error.to-string.js\";\nimport \"core-js/modules/es.array.concat.js\";\nimport \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.date.to-string.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/es.regexp.to-string.js\";\nimport \"core-js/modules/es.string.pad-start.js\";\nimport \"core-js/modules/web.timers.js\";\nimport { ref, computed, onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nexport default {\n  name: 'StuAttendance',\n  setup: function setup() {\n    var route = useRoute();\n    var router = useRouter();\n\n    // ‰ºòÂÖà‰ªé Query ÂèñÔºåÂèñ‰∏çÂà∞‰ªé LocalStorage Âèñ\n    var classId = computed(function () {\n      return route.query.classId || localStorage.getItem('currentClassId');\n    });\n\n    // Ëé∑ÂèñËØæÁ®ãÂêçÁß∞Ôºö‰ºòÂÖà‰ªéË∑ØÁî±ÂèÇÊï∞ÔºåÁÑ∂Âêé‰ªéÊú¨Âú∞Â≠òÂÇ®\n    var currentCourseName = computed(function () {\n      return route.query.courseName || localStorage.getItem('currentCourseName') || 'Êú™Áü•ËØæÁ®ã';\n    });\n    var locationGot = ref(false);\n\n    // ÊºîÁ§∫Êï∞ÊçÆ\n    var hasActiveSession = ref(true);\n    var loading = ref(false);\n    var countdown = ref(300);\n    var submitting = ref(false);\n    var hasCheckedIn = ref(false);\n    var activeSession = ref({\n      session_type: 1,\n      start_time: new Date(),\n      end_time: new Date()\n    });\n    var checkInResult = ref(null);\n    var attendanceRecords = ref([{\n      session_id: 1,\n      session_start_time: new Date(),\n      my_check_in_time: new Date(),\n      my_status: 1\n    }, {\n      session_id: 2,\n      session_start_time: new Date(),\n      my_check_in_time: null,\n      my_status: 2\n    }]);\n\n    // ËøîÂõûËØæÁ®ãÂàóË°®\n    var goBackToCourses = function goBackToCourses() {\n      router.push({\n        path: '/student/teachinghome'\n      });\n    };\n\n    // ÁªÑ‰ª∂ÊåÇËΩΩÊó∂ÔºåÂ¶ÇÊûúÊ≤°ÊúâËØæÁ®ã‰ø°ÊÅØÔºåÂ∞ùËØï‰ªéÊú¨Âú∞Â≠òÂÇ®Ëé∑Âèñ\n    onMounted(function () {\n      if (!route.query.courseName && localStorage.getItem('currentCourseName')) {\n        // Â¶ÇÊûúË∑ØÁî±ÂèÇÊï∞Ê≤°ÊúâËØæÁ®ãÂêçÔºå‰ΩÜÊú¨Âú∞Â≠òÂÇ®ÊúâÔºåÂèØ‰ª•Âú®ËøôÈáåÂ§ÑÁêÜ\n        console.log('‰ªéÊú¨Âú∞Â≠òÂÇ®Âä†ËΩΩËØæÁ®ã‰ø°ÊÅØ:', localStorage.getItem('currentCourseName'));\n      }\n    });\n    var formatCountdown = function formatCountdown(s) {\n      var m = Math.floor(s / 60).toString().padStart(2, '0');\n      var sec = (s % 60).toString().padStart(2, '0');\n      return \"\".concat(m, \":\").concat(sec);\n    };\n    var getSessionTypeText = function getSessionTypeText(t) {\n      var types = ['Êú™Áü•', '‰∫∫ËÑ∏', 'ÂÆö‰Ωç', 'Ê∑∑Âêà'];\n      return types[t] || 'ÊôÆÈÄö';\n    };\n    var formatTime = function formatTime(d) {\n      if (!d) return '--:--';\n      return new Date(d).toLocaleTimeString([], {\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    };\n    var formatDate = function formatDate(d) {\n      if (!d) return '--';\n      return new Date(d).toLocaleDateString();\n    };\n    var getStatusText = function getStatusText(s) {\n      var status = ['Êú™Áü•', 'Âá∫Âã§', 'Áº∫Âã§', 'ËøüÂà∞', 'ËØ∑ÂÅá'];\n      return status[s] || 'Êú™Áü•';\n    };\n    var getStatusClass = function getStatusClass(s) {\n      var classes = ['unknown', 'present', 'absent', 'late', 'leave'];\n      return classes[s] || 'unknown';\n    };\n    var getLocation = function getLocation() {\n      if (navigator.geolocation) {\n        navigator.geolocation.getCurrentPosition(function (position) {\n          console.log('‰ΩçÁΩÆËé∑ÂèñÊàêÂäü:', position);\n          locationGot.value = true;\n        }, function (error) {\n          console.error('‰ΩçÁΩÆËé∑ÂèñÂ§±Ë¥•:', error);\n          alert('‰ΩçÁΩÆËé∑ÂèñÂ§±Ë¥•ÔºåËØ∑Ê£ÄÊü•ÂÆö‰ΩçÊùÉÈôê');\n        });\n      } else {\n        alert('ÊµèËßàÂô®‰∏çÊîØÊåÅÂú∞ÁêÜ‰ΩçÁΩÆÂäüËÉΩ');\n      }\n    };\n    var submitCheckIn = function submitCheckIn() {\n      submitting.value = true;\n      setTimeout(function () {\n        submitting.value = false;\n        hasCheckedIn.value = true;\n        checkInResult.value = {\n          check_in_time: new Date()\n        };\n      }, 1000);\n    };\n    return {\n      classId: classId,\n      currentCourseName: currentCourseName,\n      locationGot: locationGot,\n      hasActiveSession: hasActiveSession,\n      loading: loading,\n      countdown: countdown,\n      submitting: submitting,\n      hasCheckedIn: hasCheckedIn,\n      activeSession: activeSession,\n      checkInResult: checkInResult,\n      attendanceRecords: attendanceRecords,\n      goBackToCourses: goBackToCourses,\n      formatCountdown: formatCountdown,\n      getSessionTypeText: getSessionTypeText,\n      formatTime: formatTime,\n      formatDate: formatDate,\n      getStatusText: getStatusText,\n      getStatusClass: getStatusClass,\n      getLocation: getLocation,\n      submitCheckIn: submitCheckIn\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","useRoute","useRouter","name","setup","route","router","classId","query","localStorage","getItem","currentCourseName","courseName","locationGot","hasActiveSession","loading","countdown","submitting","hasCheckedIn","activeSession","session_type","start_time","Date","end_time","checkInResult","attendanceRecords","session_id","session_start_time","my_check_in_time","my_status","goBackToCourses","push","path","console","log","formatCountdown","s","m","Math","floor","toString","padStart","sec","concat","getSessionTypeText","t","types","formatTime","d","toLocaleTimeString","hour","minute","formatDate","toLocaleDateString","getStatusText","status","getStatusClass","classes","getLocation","navigator","geolocation","getCurrentPosition","position","value","error","alert","submitCheckIn","setTimeout","check_in_time"],"sources":["D:\\vvvvvvvvvscode_workspace\\fronted\\883Fronted_Final\\src\\views\\Student\\Teaching\\course\\attendance\\Index.vue"],"sourcesContent":["<template>\r\n  <div class=\"page-container\">\r\n    <div class=\"content-header\">\r\n      <div class=\"header-left\">\r\n        <h2 class=\"page-title\">ËØæÁ®ãËÄÉÂã§</h2>\r\n        <div class=\"course-badge\">\r\n          ÂΩìÂâçËØæÁ®ãÔºö{{ currentCourseName }}\r\n        </div>\r\n      </div>\r\n      <div class=\"header-actions\">\r\n        <button class=\"back-btn\" @click=\"goBackToCourses\">ËøîÂõûËØæÁ®ãÂàóË°®</button>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"layout-grid\">\r\n      <div class=\"action-section\">\r\n        <div class=\"card check-in-card\">\r\n          <div class=\"card-header\">\r\n            <h3>üìù ÂÆûÊó∂Á≠æÂà∞</h3>\r\n            <span v-if=\"hasActiveSession\" class=\"status-badge active\">ËøõË°å‰∏≠</span>\r\n            <span v-else class=\"status-badge\">ÊöÇÊó†‰ªªÂä°</span>\r\n          </div>\r\n\r\n          <div class=\"card-body\">\r\n            <div v-if=\"loading\" class=\"state-text\">Ê≠£Âú®Ëé∑ÂèñÁ≠æÂà∞‰ªªÂä°...</div>\r\n\r\n            <div v-else-if=\"!hasActiveSession\" class=\"empty-session\">\r\n              <div class=\"icon-circle\">üí§</div>\r\n              <p>ÂΩìÂâçÊ≤°ÊúâÊ≠£Âú®ËøõË°åÁöÑËÄÉÂã§</p>\r\n              <p class=\"sub-text\">ËØ∑ÁïôÊÑèËÄÅÂ∏àÈÄöÁü•</p>\r\n            </div>\r\n\r\n            <div v-else class=\"active-session-content\">\r\n              <div class=\"timer-display\" :class=\"{ expired: countdown <= 0 }\">\r\n                <div class=\"timer-label\">{{ countdown > 0 ? 'Ââ©‰ΩôÊó∂Èó¥' : 'Â∑≤Êà™Ê≠¢' }}</div>\r\n                <div class=\"timer-value\">{{ formatCountdown(countdown) }}</div>\r\n              </div>\r\n\r\n              <div class=\"session-details\">\r\n                <div class=\"detail-row\">\r\n                  <span>Á±ªÂûã</span>\r\n                  <span>{{ getSessionTypeText(activeSession?.session_type) }}</span>\r\n                </div>\r\n                <div class=\"detail-row\">\r\n                  <span>Êó∂Èó¥</span>\r\n                  <span>{{ formatTime(activeSession?.start_time) }} - {{ formatTime(activeSession?.end_time) }}</span>\r\n                </div>\r\n              </div>\r\n\r\n              <div v-if=\"!hasCheckedIn\" class=\"action-area\">\r\n                <button v-if=\"[2, 3].includes(activeSession?.session_type)\" type=\"button\" class=\"tool-btn\"\r\n                  @click=\"getLocation\" :class=\"{ 'success': locationGot }\">\r\n                  <i class=\"icon\">üìç</i> {{ locationGot ? 'ÂÆö‰ΩçÂ∑≤Ëé∑Âèñ' : 'Ëé∑ÂèñÂÆö‰Ωç' }}\r\n                </button>\r\n\r\n                <button class=\"primary-btn block-btn\" @click=\"submitCheckIn\" :disabled=\"submitting || countdown <= 0\">\r\n                  {{ submitting ? 'Ê≠£Âú®Êèê‰∫§...' : 'Á´ãÂç≥Á≠æÂà∞' }}\r\n                </button>\r\n              </div>\r\n\r\n              <div v-else class=\"success-feedback\">\r\n                <i class=\"icon-success\">‚úÖ</i>\r\n                <p>ÊÇ®Â∑≤ÊàêÂäüÁ≠æÂà∞</p>\r\n                <span class=\"time-stamp\">{{ formatTime(checkInResult?.check_in_time) }}</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"history-section\">\r\n        <div class=\"card\">\r\n          <div class=\"card-header\">\r\n            <h3>üìä ËÄÉÂã§ËÆ∞ÂΩï</h3>\r\n          </div>\r\n          <div class=\"table-container\">\r\n            <table class=\"custom-table\">\r\n              <thead>\r\n                <tr>\r\n                  <th>Êó•Êúü</th>\r\n                  <th>Êó∂Èó¥</th>\r\n                  <th>Áä∂ÊÄÅ</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"record in attendanceRecords\" :key=\"record.session_id\">\r\n                  <td>{{ formatDate(record.session_start_time) }}</td>\r\n                  <td>{{ record.my_check_in_time ? formatTime(record.my_check_in_time) : '--:--' }}</td>\r\n                  <td>\r\n                    <span :class=\"['status-tag', getStatusClass(record.my_status)]\">\r\n                      {{ getStatusText(record.my_status) }}\r\n                    </span>\r\n                  </td>\r\n                </tr>\r\n                <tr v-if=\"attendanceRecords.length === 0\">\r\n                  <td colspan=\"3\" class=\"text-center\">ÊöÇÊó†ËÆ∞ÂΩï</td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed, onMounted } from 'vue'\r\nimport { useRoute, useRouter } from 'vue-router'\r\n\r\nexport default {\r\n  name: 'StuAttendance',\r\n  setup() {\r\n    const route = useRoute()\r\n    const router = useRouter()\r\n    \r\n    // ‰ºòÂÖà‰ªé Query ÂèñÔºåÂèñ‰∏çÂà∞‰ªé LocalStorage Âèñ\r\n    const classId = computed(() => route.query.classId || localStorage.getItem('currentClassId'))\r\n    \r\n    // Ëé∑ÂèñËØæÁ®ãÂêçÁß∞Ôºö‰ºòÂÖà‰ªéË∑ØÁî±ÂèÇÊï∞ÔºåÁÑ∂Âêé‰ªéÊú¨Âú∞Â≠òÂÇ®\r\n    const currentCourseName = computed(() => {\r\n      return route.query.courseName || localStorage.getItem('currentCourseName') || 'Êú™Áü•ËØæÁ®ã'\r\n    })\r\n    \r\n    const locationGot = ref(false)\r\n\r\n    // ÊºîÁ§∫Êï∞ÊçÆ\r\n    const hasActiveSession = ref(true)\r\n    const loading = ref(false)\r\n    const countdown = ref(300)\r\n    const submitting = ref(false)\r\n    const hasCheckedIn = ref(false)\r\n    const activeSession = ref({ session_type: 1, start_time: new Date(), end_time: new Date() })\r\n    const checkInResult = ref(null)\r\n    const attendanceRecords = ref([\r\n      { session_id: 1, session_start_time: new Date(), my_check_in_time: new Date(), my_status: 1 },\r\n      { session_id: 2, session_start_time: new Date(), my_check_in_time: null, my_status: 2 },\r\n    ])\r\n\r\n    // ËøîÂõûËØæÁ®ãÂàóË°®\r\n    const goBackToCourses = () => {\r\n      router.push({ path: '/student/teachinghome' })\r\n    }\r\n\r\n    // ÁªÑ‰ª∂ÊåÇËΩΩÊó∂ÔºåÂ¶ÇÊûúÊ≤°ÊúâËØæÁ®ã‰ø°ÊÅØÔºåÂ∞ùËØï‰ªéÊú¨Âú∞Â≠òÂÇ®Ëé∑Âèñ\r\n    onMounted(() => {\r\n      if (!route.query.courseName && localStorage.getItem('currentCourseName')) {\r\n        // Â¶ÇÊûúË∑ØÁî±ÂèÇÊï∞Ê≤°ÊúâËØæÁ®ãÂêçÔºå‰ΩÜÊú¨Âú∞Â≠òÂÇ®ÊúâÔºåÂèØ‰ª•Âú®ËøôÈáåÂ§ÑÁêÜ\r\n        console.log('‰ªéÊú¨Âú∞Â≠òÂÇ®Âä†ËΩΩËØæÁ®ã‰ø°ÊÅØ:', localStorage.getItem('currentCourseName'))\r\n      }\r\n    })\r\n\r\n    const formatCountdown = (s) => {\r\n      const m = Math.floor(s / 60).toString().padStart(2, '0')\r\n      const sec = (s % 60).toString().padStart(2, '0')\r\n      return `${m}:${sec}`\r\n    }\r\n\r\n    const getSessionTypeText = (t) => {\r\n      const types = ['Êú™Áü•', '‰∫∫ËÑ∏', 'ÂÆö‰Ωç', 'Ê∑∑Âêà']\r\n      return types[t] || 'ÊôÆÈÄö'\r\n    }\r\n\r\n    const formatTime = (d) => {\r\n      if (!d) return '--:--'\r\n      return new Date(d).toLocaleTimeString([], { hour: '2-digit', minute: '2-digit' })\r\n    }\r\n\r\n    const formatDate = (d) => {\r\n      if (!d) return '--'\r\n      return new Date(d).toLocaleDateString()\r\n    }\r\n\r\n    const getStatusText = (s) => {\r\n      const status = ['Êú™Áü•', 'Âá∫Âã§', 'Áº∫Âã§', 'ËøüÂà∞', 'ËØ∑ÂÅá']\r\n      return status[s] || 'Êú™Áü•'\r\n    }\r\n\r\n    const getStatusClass = (s) => {\r\n      const classes = ['unknown', 'present', 'absent', 'late', 'leave']\r\n      return classes[s] || 'unknown'\r\n    }\r\n\r\n    const getLocation = () => {\r\n      if (navigator.geolocation) {\r\n        navigator.geolocation.getCurrentPosition(\r\n          (position) => {\r\n            console.log('‰ΩçÁΩÆËé∑ÂèñÊàêÂäü:', position)\r\n            locationGot.value = true\r\n          },\r\n          (error) => {\r\n            console.error('‰ΩçÁΩÆËé∑ÂèñÂ§±Ë¥•:', error)\r\n            alert('‰ΩçÁΩÆËé∑ÂèñÂ§±Ë¥•ÔºåËØ∑Ê£ÄÊü•ÂÆö‰ΩçÊùÉÈôê')\r\n          }\r\n        )\r\n      } else {\r\n        alert('ÊµèËßàÂô®‰∏çÊîØÊåÅÂú∞ÁêÜ‰ΩçÁΩÆÂäüËÉΩ')\r\n      }\r\n    }\r\n\r\n    const submitCheckIn = () => {\r\n      submitting.value = true\r\n      setTimeo