ackground: #fff;\r\n  border-radius: 12px;\r\n  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.05);\r\n  overflow: hidden;\r\n  border: 1px solid #eee;\r\n}\r\n\r\n.card-header {\r\n  padding: 16px 20px;\r\n  border-block-end: 1px solid #f0f0f0;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.card-header h3 {\r\n  font-size: 16px;\r\n  font-weight: 600;\r\n  margin: 0;\r\n}\r\n\r\n.card-body {\r\n  padding: 24px;\r\n}\r\n\r\n/* Active Session Styles */\r\n.timer-display {\r\n  text-align: center;\r\n  margin-block-end: 24px;\r\n  padding: 16px;\r\n  background: #f9f9f9;\r\n  border-radius: 8px;\r\n}\r\n\r\n.timer-value {\r\n  font-size: 36px;\r\n  font-weight: 700;\r\n  color: var(--primary-color, #2A5CAA);\r\n  font-family: monospace;\r\n}\r\n\r\n.timer-display.expired .timer-value {\r\n  color: #999;\r\n}\r\n\r\n.session-details {\r\n  margin-block-end: 24px;\r\n}\r\n\r\n.detail-row {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding: 8px 0;\r\n  border-block-end: 1px dashed #eee;\r\n  color: #666;\r\n  font-size: 14px;\r\n}\r\n\r\n.empty-session {\r\n  text-align: center;\r\n  color: #999;\r\n  padding: 40px 0;\r\n}\r\n\r\n.icon-circle {\r\n  font-size: 40px;\r\n  margin-block-end: 16px;\r\n  background: #f5f5f5;\r\n  inline-size: 80px;\r\n  block-size: 80px;\r\n  line-height: 80px;\r\n  border-radius: 50%;\r\n  margin: 0 auto 16px;\r\n}\r\n\r\n/* Buttons */\r\n.primary-btn {\r\n  background: var(--primary-color, #2A5CAA);\r\n  color: #fff;\r\n  border: none;\r\n  padding: 12px;\r\n  border-radius: 6px;\r\n  font-weight: 600;\r\n  cursor: pointer;\r\n  transition: opacity 0.2s;\r\n}\r\n\r\n.primary-btn:hover {\r\n  opacity: 0.9;\r\n}\r\n\r\n.primary-btn:disabled {\r\n  background: #ccc;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.block-btn {\r\n  inline-size: 100%;\r\n  display: block;\r\n}\r\n\r\n.tool-btn {\r\n  inline-size: 100%;\r\n  padding: 10px;\r\n  margin-block-end: 12px;\r\n  background: #fff;\r\n  