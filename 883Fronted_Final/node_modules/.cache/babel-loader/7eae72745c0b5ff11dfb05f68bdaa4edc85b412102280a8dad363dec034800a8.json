{"ast":null,"code":"import StudentHeader from './headers/StuHeader.vue';\nimport StudentSidebar from './siderbars/StuSidebar.vue';\nimport { useRoute } from 'vue-router';\nimport { computed } from 'vue';\nexport default {\n  name: 'StudentLayout',\n  components: {\n    StudentHeader: StudentHeader,\n    StudentSidebar: StudentSidebar\n  },\n  setup: function setup() {\n    var route = useRoute();\n\n    // 根据路由meta决定是否显示侧边栏\n    var showSidebar = computed(function () {\n      return route.meta.showSidebar !== false;\n    });\n    return {\n      showSidebar: showSidebar\n    };\n  }\n};","map":{"version":3,"names":["StudentHeader","StudentSidebar","useRoute","computed","name","components","setup","route","showSidebar","meta"],"sources":["/Users/zhouzhou/Desktop/883Fronted_Final/src/components/layout/StuLayout.vue"],"sourcesContent":["<template>\n  <div class=\"layout\">\n    <StudentHeader />\n    <div class=\"layout-content\">\n      <AppSidebar v-if=\"showSidebar\" /> <!-- 条件渲染侧边栏 -->\n      <main class=\"main-content\" :class=\"{ 'with-sidebar': showSidebar }\">\n        <router-view />\n      </main>\n    </div>\n  </div>\n</template>\n\n<script>\nimport StudentHeader from './headers/StuHeader.vue'\nimport StudentSidebar from './siderbars/StuSidebar.vue'\nimport { useRoute } from 'vue-router'\nimport { computed } from 'vue'\n\nexport default {\n  name: 'StudentLayout',\n  components: {\n    StudentHeader,\n    StudentSidebar\n  },\n  setup() {\n    const route = useRoute()\n    \n    // 根据路由meta决定是否显示侧边栏\n    const showSidebar = computed(() => {\n      return route.meta.showSidebar !== false\n    })\n\n    return {\n      showSidebar\n    }\n  }\n}\n</script>\n\n<style scoped>\n.layout {\n  min-height: 100vh;\n}\n\n.layout-content {\n  display: flex;\n  padding-top: 60px;\n  min-height: calc(100vh - 60px);\n}\n\n.main-content {\n  flex: 1;\n  padding: 20px;\n  min-height: calc(100vh - 60px);\n  background-color: #f5f5f5;\n  transition: margin-left 0.3s;\n}\n\n.main-content.with-sidebar {\n  margin-left: 240px; /* 与侧边栏宽度一致 */\n}\n</style>"],"mappings":"AAaA,OAAOA,aAAY,MAAO,yBAAwB;AAClD,OAAOC,cAAa,MAAO,4BAA2B;AACtD,SAASC,QAAO,QAAS,YAAW;AACpC,SAASC,QAAO,QAAS,KAAI;AAE7B,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVL,aAAa,EAAbA,aAAa;IACbC,cAAa,EAAbA;EACF,CAAC;EACDK,KAAK,WAALA,KAAKA,CAAA,EAAG;IACN,IAAMC,KAAI,GAAIL,QAAQ,CAAC;;IAEvB;IACA,IAAMM,WAAU,GAAIL,QAAQ,CAAC,YAAM;MACjC,OAAOI,KAAK,CAACE,IAAI,CAACD,WAAU,KAAM,KAAI;IACxC,CAAC;IAED,OAAO;MACLA,WAAU,EAAVA;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}