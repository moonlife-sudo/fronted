n        return menuConfig.resource\n      } else if (route.path.startsWith('/student-library')) {\n        return menuConfig.resource\n      } \n      return []\n    })\n\n    // 计算当前模块标题\n    const currentModuleTitle = computed(() => {\n      if (route.path.startsWith('/teaching')) return '智能教学'\n      if (route.path.startsWith('/reservation')) return '资源预约'\n      if (route.path.startsWith('/campus')) return '校园生活'\n      if (route.path.startsWith('/tea-stu-classroom')) return '资源管理'\n      if (route.path.startsWith('/student-library')) return '资源管理'\n      if (route.path.startsWith('/admin-')) return '管理员'\n      return '功能菜单'\n    })\n\n    // 检查菜单项是否激活\n    const isItemActive = (item) => {\n      return route.path === item.path || route.path.startsWith(item.path + '/')\n    }\n\n    return {\n      currentMenuGroups,\n      currentModuleTitle,\n      isItemActive\n    }\n  }\n}\n</script>\n\n<style scoped>\n.app-sidebar {\n  width: 240px;\n  background-color: var(--sidebar-bg, #d6e2f5);\n  height: calc(100vh - 60px);\n  position: fixed;\n  top: 50px;\n  left: 0;\n  box-shadow: var(--sidebar-shadow, 2px 0 8px rgba(0, 0, 0, 0.1));\n  z-index: 999;\n  overflow-y: auto;\n}\n\n.sidebar-header {\n  height: 50px;\n  display: flex;\n  align-items: center;\n  padding: 0 15px;\n  border-bottom: 1px solid rgba(0, 0, 0, 0.1);\n}\n\n.sidebar-title {\n  font-weight: bold;\n  color: var(--primary-color, #2A5CAA);\n  font-size: 16px;\n}\n\n.sidebar-nav {\n  padding: 15px 0;\n}\n\n.menu-group {\n  margin-bottom: 20px;\n}\n\n.group-title {\n  padding: 0 15px 8px 15px;\n  font-size: 12px;\n  color: var(--text-light-gray, #666666);\n  text-transform: uppercase;\n  letter-spacing: 1px;\n  font-weight: 600;\n}\n\n.menu-item {\n  display: flex;\n  align-items: center;\n  padding: 12px 15px;\n  text-decoration: none;\n  color: var(--text-gray, #333333);\n  transition: all 0.3s;\n  border-left: 3px solid transparent;\n  gap: 10px;\n  margin: 2px 0;\n}\n\n.menu-item:hover {\n  background-color: rgba(194, 212, 240, 0.5);\n  color: var(--primary-color, #2A5CAA);\n}\n\n.menu-item.active {\n  background-color: var(--sidebar-active, #c2d4f0);\n  color: var(--primary-color, #2A5CAA);\n  border-left-color: var(--primary-color, #2A5CAA);\n}\n\n.item-icon {\n  font-size: 16px;\n  min-width: 20px;\n  text-align: center;\n}\n\n.item-text {\n  white-space: nowrap;\n  overflow: hidden;\n  font-weight: 500;\n}\n</style>"],"mappings":";;EAESA,KAAK,EAAC;AAAa;;EACnBA,KAAK,EAAC;AAAgB;;EACnBA,KAAK,EAAC;AAAe;;EAGxBA,KAAK,EAAC;AAAa;;EAEfA,KAAK,EAAC;AAAa;;EAKhBA,KAAK,EAAC;AAAW;;EACjBA,KAAK,EAAC;AAAW;;;uBAb/BC,mBAAA,CAiBQ,SAjBRC,UAiBQ,GAhBNC,mBAAA,CAEM,OAFNC,UAEM,GADJD,mBAAA,CAA2D,QAA3DE,UAA2D,EAAAC,gBAAA,CAA5BC,MAAA,CAAAC,kBAAkB,iB,GAGnDL,mBAAA,CAWM,OAXNM,UAWM,I,kBAVJR,mBAAA,CASMS,SAAA,QAAAC,WAAA,CATeJ,MAAA,CAAAK,iBAAiB,EAA1BC,KAAK;yBAAjBZ,mBAAA,CASM;MATmCa,GAAG,EAAED,KAAK,CAACE,IAAI;MAAEf,KAAK,EAAC;QAC9DG,mBAAA,CAEM,OAFNa,UAEM,EAAAV,gBAAA,CADDO,KAAK,CAACE,IAAI,mB,kBAEfd,mBAAA,CAIcS,SAAA,QAAAC,WAAA,CAJcE,KAAK,CAACI,QAAQ,EAAtBC,IAAI;2BAAxBC,YAAA,CAIcC,sBAAA;QAJ+BN,GAAG,EAAEI,IAAI,CAACG,IAAI;QAAGC,EAAE,EAAEJ,IAAI,CAACG,IAAI;QAAErB,KAAK,EAAAuB,eAAA,EAAC,WAAW;UAAAC,MAAA,EAC1EjB,MAAA,CAAAkB,YAAY,CAACP,IAAI;QAAA;;0BACnC,MAA8C,CAA9Cf,mBAAA,CAA8C,QAA9CuB,UAA8C,EAAApB,gBAAA,CAAnBY,IAAI,CAACS,IAAI,kBACpCxB,mBAAA,CAA8C,QAA9CyB,UAA8C,EAAAtB,gBAAA,CAAnBY,IAAI,CAACH,IAAI,iB","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}