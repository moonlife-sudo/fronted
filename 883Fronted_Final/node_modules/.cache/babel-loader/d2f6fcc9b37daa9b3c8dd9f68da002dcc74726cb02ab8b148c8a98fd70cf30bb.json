extPage(...args))\n  }, [...(_cache[20] || (_cache[20] = [_createElementVNode(\"i\", {\n    class: \"bi bi-chevron-right\"\n  }, null, -1 /* CACHED */)]))])])])]);\n}","map":{"version":3,"names":["class","_createElementBlock","_hoisted_1","_createElementVNode","_hoisted_2","_hoisted_3","_hoisted_4","type","placeholder","$data","searchTerm","$event","onInput","_cache","args","$options","handleSearch","_hoisted_5","statusFilter","onChange","applyFilters","value","selected","dateFilter","_hoisted_6","_Fragment","_renderList","filteredApplications","application","key","id","_hoisted_7","_toDisplayString","_normalizeClass","status","getStatusText","_hoisted_8","_hoisted_9","applicant","_hoisted_10","location","_hoisted_11","date","_hoisted_12","time","_hoisted_13","purpose","_hoisted_14","onClick","approveApplication","rejectApplication","undoApplication","_hoisted_18","disabled","currentPage","prevPage","_hoisted_20","nextPage"],"sources":["/Users/zhouzhou/Desktop/883Fronted/resource/src/views/AdminApprove.vue"],"sourcesContent":["<template>\n  <div class=\"admin-approve\">\n    <div class=\"approve-container\">\n      <h1 class=\"page-title\">预约审批</h1>\n      \n      <div class=\"search-filter-container\">\n        <div class=\"search-box\">\n          <i class=\"bi bi-search\"></i>\n          <input \n            type=\"text\" \n            placeholder=\"搜索申请人、教室或日期...\" \n            v-model=\"searchTerm\"\n            @input=\"handleSearch\"\n          >\n        </div>\n        \n        <div class=\"filter-group\">\n          <select v-model=\"statusFilter\" @change=\"applyFilters\">\n            <option value=\"all\">所有状态</option>\n            <option value=\"pending\" selected>待审批</option>\n            <option value=\"approved\">已批准</option>\n            <option value=\"rejected\">已拒绝</option>\n          </select>\n          \n          <select v-model=\"dateFilter\" @change=\"applyFilters\">\n            <option value=\"all\">所有日期</option>\n            <option value=\"today\">今天</option>\n            <option value=\"week\">本周</option>\n            <option value=\"month\">本月</option>\n          </select>\n        </div>\n      </div>\n      \n      <div class=\"approval-list\">\n        <div \n          class=\"approval-card\" \n          v-for=\"application in filteredApplications\" \n          :key=\"application.id\"\n        >\n          <div class=\"card-header\">\n            <h3>{{ application.type }}</h3>\n            <span class=\"badge\" :class=\"application.status\">{{ getStatusText(application.status) }}</span>\n          </div>\n          \n          <div class=\"card-content\">\n            <div class=\"info-row\">\n              <i class=\"bi bi-person\"></i>\n              <span>申请人: {{ application.applicant }}</span>\n            </div>\n            <div class=\"info-row\">\n              <i class=\"bi bi-building\"></i>\n              <span>教室: {{ application.location }}</span>\n            </div>\n            <div class=\"info-row\">\n              <i class=\"bi bi-calendar\"></i>\n              <span>日期: {{ application.date }}</span>\n            </div>\n            <div class=\"info-row\">\n              <i class=\"bi bi-clock\"></i>\n              <span>时间: {{ application.time }}</span>\n            </div>\n            <div class=\"info-row\">\n              <i class=\"bi bi-journal-text\"></i>\n              <span>用途: {{ application.purpose }}</span>\n            </div>\n          </div>\n          \n          <div class=\"card-actions\">\n            <button \n              v-if=\"application.status === 'pending'\" \n              class=\"btn approve-btn\" \n              @click=\"approveApplication(application.id)\"\n            >\n              <i class=\"bi bi-check-circle\"></i> 批准\n            </button>\n            <button \n              v-if=\"application.status === 'pending'\" \n              class=\"btn reject-btn\" \n              @click=\"rejectApplication(application.id)\"\n            >\n              <i class=\"bi bi-x-circle\"></i> 拒绝\n            </button>\n            <button \n              v-if=\"application.status !== 'pending'\" \n              class=\"btn undo-btn\" \n              @click=\"undoApplication(application.id)\"\n            >\n              <i class=\"bi bi-arrow-counterclockwise\"></i> 撤销\n            </button>\n          </div>\n        </div>\n      </div>\n      \n      <div class=\"pagination\">\n        <button class=\"btn pagination-btn\" :disabled=\"currentPage === 1\" @click=\"prevPage\">\n          <i class=\"bi bi-chevron-left\"></i>\n        </button>\n        <span class=\"current-page\">{{ currentPage }}</span>\n        <button class=\"btn pagination-btn\" @click=\"nextPage\">\n          <i class=\"bi bi-chevron-right\"></i>\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'AdminApprove',\n  data() {\n    return {\n      searchTerm: '',\n      statusFilter: 'pending',\n      dateFilter: 'all',\n      currentPage: 1,\n      applications: [\n        {\n          id: 1,\n          type: '教室预约申请',\n          applicant: '张老师',\n          location: '教学楼A-201',\n          date: '2023-11-15',\n          time: '14:00 - 16:00',\n          purpose: '计算机科学研讨会',\n          status: 'pending'\n        },\n        {\n          id: 2,\n          type: '教室预约申请',\n          applicant: '李教授',\n          location: '图书馆报告厅',\n          date: '2023-11-18',\n          time: '09:00 - 12:00',\n          purpose: '学术讲座',\n          status: 'pending'\n        },\n        {\n          id: 3,\n          type: '教室预约申请',\n          applicant: '王同学',\n          location: '实验楼B-105',\n          date: '2023-11-20',\n          time: '16:00 - 18:00',\n          purpose: '学生社团活动',\n          status: 'pending'\n        }\n      ]\n    }\n  },\n  computed: {\n  filteredApplications() {\n    return this.applications.filter(app => {\n      // 搜索过滤\n      const searchMatch = !this.searchTerm || \n        Object.values(app).some(value => \n          String(value).toLowerCase().includes(this.searchTerm.toLowerCase())\n        )\n      \n      // 状态过滤\n      const statusMatch = this.statusFilter === 'all' || app.status === this.statusFilter\n      \n      // 日期过滤\n      let dateMatch = true\n      if (this.dateFilter !== 'all') {\n        const today = new Date()\n        const appDate = new Date(app.date)\n        \n        if (this.dateFilter === 'today') {\n          dateMatch = appDate.toDateString() === today.toDateString()\n        } else if (this.dateFilter === 'week') {\n          const weekStart = new Date(today.setDate(today.getDate() - today.getDay()))\n          const weekEnd = new Date(today.setDate(today.getDate() + 6))\n          dateMatch = appDate >= weekStart && appDate <= weekEnd\n        } else if (this.dateFilter === 'month') {\n          dateMatch = appDate.getMonth() === today.getMonth() && \n                     appDate.getFullYear() === today.getFullYear()\n        }\n      }\n      \n      return searchMatch && statusMatch && dateMatch\n    })\n  }\n},\n  methods: {\n    getStatusText(status) {\n      const statusMap = {\n        pending: '待审批',\n        approved: '已批准',\n        rejected: '已拒绝'\n      }\n      return statusMap[status] || status\n    },\n    \n    handleSearch() {\n      // 搜索逻辑已经在计算属性中处理\n    },\n    \n    applyFilters() {\n      // 筛选逻辑已经在计算属性中处理\n    },\n    \n    approveApplication(id) {\n      const app = this.applications.find(a => a.id === id)\n      if (app && confirm('确定批准此申请吗？')) {\n        app.status = 'approved'\n      }\n    },\n    \n    rejectApplication(id) {\n      const app = this.applications.find(a => a.id === id)\n      if (app && confirm('确定拒绝此申请吗？')) {\n        app.status = 'rejected'\n      }\n    },\n    \n    undoApplication(id) {\n      const app = this.applications.find(a => a.id === id)\n      if (app) {\n        app.status = 'pending'\n      }\n    },\n    \n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--\n        this.loadPageData()\n      }\n    },\n    \n    nextPage() {\n      this.currentPage++\n      this.loadPageData()\n    },\n    \n    loadPageData() {\n      // 模拟加载更多数据\n      if (this.currentPage > 1) {\n        const newApp = {\n          id: this.applications.length + 1,\n          type: '教室预约申请',\n          applicant: '新加载用户',\n          location: '新加载教室',\n          date: '2023-11-25',\n          time: '10:00 - 12:00',\n          purpose: '新加载用途',\n          status: 'pending'\n        }\n        this.applications.push(newApp)\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n/* 在 admin-approve 内定义变量 */\n.admin-approve {\n  --primary-color: #2A5CAA;\n  --secondary-color: #4CAF50;\n  --danger-color: #E74C3C;\n  --light-color: #F8F9FA;\n  --dark-color: #343A40;\n  --gray-color: #6C757D;\n  --border-radius: 8px;\n  --box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\n  --transition: all 0.3s ease;\n}\n\n.admin-approve {\n  min-height: 100vh;\n  background-color: #f5f7fa;\n  font-family: 'Roboto', sans-serif;\n  color: #333;\n  line-height: 1.6;\n}\n\n/* 主要内容区域 */\n.approve-container {\n  max-width: 1200px;\n  margin: 2rem auto;\n  padding: 0 1.5rem;\n  flex: 1;\n}\n\n.page-title {\n  font-size: 2rem;\n  color: var(--primary-color);\n  margin-bottom: 2rem;\n  font-weight: 600;\n  text-align: center;\n}\n\n/* 搜索和筛选区域 */\n.search-filter-container {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 1.5rem;\n  margin-bottom: 2rem;\n  align-items: center;\n  background: white;\n  padding: 1.5rem;\n  border-radius: var(--border-radius);\n  box-shadow: var(--box-shadow);\n}\n\n.search-box {\n  flex: 1;\n  min-width: 300px;\n  position: relative;\n}\n\n.search-box i {\n  position: absolute;\n  left: 1rem;\n  top: 50%;\n  transform: translateY(-50%);\n  color: var(--gray-color);\n  z-index: 1;\n}\n\n.search-box input {\n  width: 100%;\n  padding: 0.75rem 1rem 0.75rem 2.5rem;\n  border: 1px solid #e1e5e9;\n  border-radius: var(--border-radius);\n  font-size: 1rem;\n  transition: var(--transition);\n  background-color: var(--light-color);\n}\n\n.search-box input:focus {\n  outline: none;\n  border-color: var(--primary-color);\n  box-shadow: 0 0 0 3px rgba(42, 92, 170, 0.1);\n  background-color: white;\n}\n\n.filter-group {\n  display: flex;\n  gap: 1rem;\n  align-items: center;\n}\n\n.filter-group select {\n  padding: 0.75rem 1rem;\n  border: 1px solid #e1e5e9;\n  border-radius: var(--border-radius);\n  font-size: 1rem;\n  background-color: var(--light-color);\n  cursor: pointer;\n  transition: var(--transition);\n  min-width: 150px;\n}\n\n.filter-group select:focus {\n  outline: none;\n  border-color: var(--primary-color);\n  box-shadow: 0 0 0 3px rgba(42, 92, 170, 0.1);\n  background-color: white;\n}\n\n/* 审批卡片列表 */\n.approval-list {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(380px, 1fr));\n  gap: 2rem;\n  margin-bottom: 2rem;\n}\n\n.approval-card {\n  background: linear-gradient(135deg, #ffffff 0%, #f8f9fa 100%);\n  border-radius: var(--border-radius);\n  box-shadow: var(--box-shadow);\n  overflow: hidden;\n  transition: var(--transition);\n  border: 1px solid #e9ecef;\n  position: relative;\n}\n\n.approval-card::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  height: 4px;\n  background: linear-gradient(90deg, var(--primary-color), var(--secondary-color));\n}\n\n.approval-card:hover {\n  transform: translateY(-8px);\n  box-shadow: 0 12px 30px rgba(0, 0, 0, 0.15);\n}\n\n.card-header {\n  padding: 1.5rem 1.5rem 1rem;\n  display: flex;\n  justify-content: space-between;\n  align-items: flex-start;\n  border-bottom: 1px solid #f1f3f4;\n}\n\n.card-header h3 {\n  font-size: 1.3rem;\n  font-weight: 600;\n  color: var(--dark-color);\n  margin: 0;\n  line-height: 1.4;\n}\n\n.badge {\n  padding: 0.4rem 1rem;\n  border-radius: 20px;\n  font-size: 0.8rem;\n  font-weight: 600;\n  text-transform: uppercase;\n  letter-spacing: 0.5px;\n}\n\n.badge.pending {\n  background: linear-gradient(135deg, #FFF3CD, #FFEAA7);\n  color: #856404;\n  border: 1px solid #FFEAA7;\n}\n\n.badge.approved {\n  background: linear-gradient(135deg, #D4EDDA, #C8E6C9);\n  color: #155724;\n  border: 1px solid #C8E6C9;\n}\n\n.badge.rejected {\n  background: linear-gradient(135deg, #F8D7DA, #FFCDD2);\n  color: #721C24;\n  border: 1px solid #FFCDD2;\n}\n\n.card-content {\n  padding: 1.5rem;\n}\n\n.info-row {\n  display: flex;\n  align-items: center;\n  margin-bottom: 1rem;\n  padding: 0.5rem 0;\n  border-bottom: 1px solid #f8f9fa;\n}\n\n.info-row:last-child {\n  margin-bottom: 0;\n  border-bottom: none;\n}\n\n.info-row i {\n  margin-right: 1rem;\n  color: var(--primary-color);\n  font-size: 1.2rem;\n  width: 20px;\n  text-align: center;\n}\n\n.info-row span {\n  color: var(--dark-color);\n  font-weight: 500;\n}\n\n.info-row .label {\n  color: var(--gray-color);\n  font-weight: 400;\n  margin-right: 0.5rem;\n}\n\n/* 按钮样式 - 恢复精美设计 */\n.card-actions {\n  padding: 1.25rem 1.5rem;\n  border-top: 1px solid #f1f3f4;\n  display: flex !important;\n  gap: 1rem;\n  background: rgba(248, 249, 250, 0.8);\n  backdrop-filter: blur(10px);\n}\n\n.btn {\n  padding: 0.75rem 1.5rem;\n  border: none;\n  border-radius: var(--border-radius);\n  font-size: 0.95rem;\n  font-weight: 600;\n  cursor: pointer;\n  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);\n  display: inline-flex !important;\n  align-items: center;\n  justify-content: center;\n  gap: 0.5rem;\n  min-width: 100px;\n  opacity: 1 !important;\n  visibility: visible !important;\n  position: relative;\n  overflow: hidden;\n}\n\n.btn::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: -100%;\n  width: 100%;\n  height: 100%;\n  background: linear-gradient(90deg, transparent, rgba(255,255,255,0.3), transparent);\n  transition: left 0.5s;\n}\n\n.btn:hover::before {\n  left: 100%;\n}\n\n.btn i {\n  font-size: 1.1rem;\n  transition: transform 0.3s ease;\n}\n\n.btn:hover i {\n  tr