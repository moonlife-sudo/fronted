const appDate = new Date(app.date)\n        \n        if (this.dateFilter === 'today') {\n          dateMatch = appDate.toDateString() === today.toDateString()\n        } else if (this.dateFilter === 'week') {\n          const weekStart = new Date(today.setDate(today.getDate() - today.getDay()))\n          const weekEnd = new Date(today.setDate(today.getDate() + 6))\n          dateMatch = appDate >= weekStart && appDate <= weekEnd\n        } else if (this.dateFilter === 'month') {\n          dateMatch = appDate.getMonth() === today.getMonth() && \n                     appDate.getFullYear() === today.getFullYear()\n        }\n      }\n      \n      return searchMatch && statusMatch && dateMatch\n    })\n  }\n},\n  methods: {\n    getStatusText(status) {\n      const statusMap = {\n        pending: '待审批',\n        approved: '已批准',\n        rejected: '已拒绝'\n      }\n      return statusMap[status] || status\n    },\n    \n    handleSearch() {\n      // 搜索逻辑已经在计算属性中处理\n    },\n    \n    applyFilters() {\n      // 筛选逻辑已经在计算属性中处理\n    },\n    \n    approveApplication(id) {\n      const app = this.applications.find(a => a.id === id)\n      if (app && confirm('确定批准此申请吗？')) {\n        app.status = 'approved'\n      }\n    },\n    \n    rejectApplication(id) {\n      const app = this.applications.find(a => a.id === id)\n      if (app && confirm('确定拒绝此申请吗？')) {\n        app.status = 'rejected'\n      }\n    },\n    \n    undoApplication(id) {\n      const app = this.applications.find(a => a.id === id)\n      if (app) {\n        app.status = 'pending'\n      }\n    },\n    \n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--\n        this.loadPageData()\n      }\n    },\n    \n    nextPage() {\n      this.currentPage++\n      this.loadPageData()\n    },\n    \n    loadPageData() {\n      // 模拟加载更多数据\n      if (this.currentPage > 1) {\n        const newApp = {\n          id: this.applications.length + 1,\n          type: '教室预约申请',\n          applicant: '新加载用户',\n          location: '新加载教室',\n          date: '2023-11-25',\n          time: '10:00 - 12:00',\n          purpose: '新加载用途',\n          status: 'pending'\n        }\n        this.applications.push(newApp)\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n/* 这里放入之前提供的完整CSS样式 */\n.admin-approve {\n  min-height: 100vh;\n  background-color: #f5f7fa;\n}\n\n.approve-container {\n  max-width: 1200px;\n  margin: 2rem auto;\n  padding: 0 1.5rem;\n  flex: 1;\n}\n\n.page-title {\n  font-size: 2rem;\n  color: #2A5CAA;\n  margin-bottom: 2rem;\n  font-weight: 600;\n}\n\n/* ... 其余CSS样式保持不变 ... */\n</style>"],"mappings":";;;;;AA2GA,eAAe;EACbA,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,YAAY,EAAE,SAAS;MACvBC,UAAU,EAAE,KAAK;MACjBC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE,CACZ;QACEC,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,QAAQ;QACdC,SAAS,EAAE,KAAK;QAChBC,QAAQ,EAAE,UAAU;QACpBC,IAAI,EAAE,YAAY;QAClBC,IAAI,EAAE,eAAe;QACrBC,OAAO,EAAE,UAAU;QACnBC,MAAM,EAAE;MACV,CAAC,EACD;QACEP,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,QAAQ;QACdC,SAAS,EAAE,KAAK;QAChBC,QAAQ,EAAE,QAAQ;QAClBC,IAAI,EAAE,YAAY;QAClBC,IAAI,EAAE,eAAe;QACrBC,OAAO,EAAE,MAAM;QACfC,MAAM,EAAE;MACV,CAAC,EACD;QACEP,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,QAAQ;QACdC,SAAS,EAAE,KAAK;QAChBC,QAAQ,EAAE,UAAU;QACpBC,IAAI,EAAE,YAAY;QAClBC,IAAI,EAAE,eAAe;QACrBC,OAAO,EAAE,QAAQ;QACjBC,MAAM,EAAE;MACV;IAEJ;EACF,CAAC;EACDC,QAAQ,EAAE;IACVC,oBAAoBA,CAAA,EAAG;MACrB,OAAO,IAAI,CAACV,YAAY,CAACW,MAAM,CAACC,GAAE,IAAK;QACrC;QACA,MAAMC,WAAU,GAAI,CAAC,IAAI,CAACjB,UAAS,IACjCkB,MAAM,CAACC,MAAM,CAACH,GAAG,CAAC,CAACI,IAAI,CAACC,KAAI,IAC1BC,MAAM,CAACD,KAAK,CAAC,CAACE,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACxB,UAAU,CAACuB,WAAW,CAAC,CAAC,CACpE;;QAEF;QACA,MAAME,WAAU,GAAI,IAAI,CAACxB,YAAW,KAAM,KAAI,IAAKe,GAAG,CAACJ,MAAK,KAAM,IAAI,CAACX,YAAW;;QAElF;QACA,IAAIyB,SAAQ,GAAI,IAAG;QACnB,IAAI,IAAI,CAACxB,UAAS,KAAM,KAAK,EAAE;UAC7B,MAAMyB,KAAI,GAAI,IAAIC,IAAI,CAAC;UACvB,MAAMC,OAAM,GAAI,IAAID,IAAI,CAACZ,GAAG,CAACP,IAAI;UAEjC,IAAI,IAAI,CAACP,UAAS,KAAM,OAAO,EAAE;YAC/BwB,SAAQ,GAAIG