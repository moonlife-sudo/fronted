nd: 6px;\n    font-weight: 500;\n    font-size: 14px;\n}\n\n.input {\n    inline-size: 100%;\n    padding: 10px;\n    border-radius: 6px;\n    border: 1px solid #ddd;\n    transition: border-color 0.2s;\n}\n\n.input:focus {\n    outline: none;\n    border-color: #2A5CAA;\n}\n\n.modal-actions {\n    display: flex;\n    justify-content: flex-end;\n    gap: 10px;\n    margin-block-start: 24px;\n}\n\n.btn {\n    background: #2A5CAA;\n    color: white;\n    border: none;\n    padding: 10px 20px;\n    border-radius: 6px;\n    cursor: pointer;\n}\n\n.btn-cancel {\n    background: white;\n    border: 1px solid #ddd;\n    padding: 10px 20px;\n    border-radius: 6px;\n    cursor: pointer;\n}\n</style>"],"mappings":"AA+DA,SAASA,GAAG,QAAQ,KAAI;;AAExB;;;;;;;;IACA,MAAMC,QAAQ,GAAGD,GAAG,CAAC,CACjB;MACIE,KAAK,EAAE,kCAAkC;MACzCC,WAAW,EAAE,4GAA4G;MACzHC,MAAM,EAAE,WAAW;MACnBC,UAAU,EAAE,WAAW;MACvBC,SAAS,EAAE,CAAC,KAAK,EAAE,UAAU;IACjC,CAAC,EACD;MACIJ,KAAK,EAAE,6BAA6B;MACpCC,WAAW,EAAE,qEAAqE;MAClFC,MAAM,EAAE,QAAQ;MAChBC,UAAU,EAAE,aAAa;MACzBC,SAAS,EAAE,CAAC,oBAAoB,EAAE,eAAe;IACrD,CAAC,EACD;MACIJ,KAAK,EAAE,4BAA4B;MACnCC,WAAW,EAAE,qEAAqE;MAClFC,MAAM,EAAE,QAAQ;MAChBC,UAAU,EAAE,aAAa;MACzBC,SAAS,EAAE,CAAC,eAAe;IAC/B,EACH;IAED,MAAMC,SAAS,GAAGP,GAAG,CAAC,KAAK;IAC3B,MAAMQ,UAAU,GAAGR,GAAG,CAAC;MACnBE,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE,EAAE;MACfG,SAAS,EAAE;IACf,CAAC;IAED,MAAMG,OAAO,GAAGA,CAAA,KAAM;MAClBD,UAAU,CAACE,KAAK,GAAG;QAAER,KAAK,EAAE,EAAE;QAAEC,WAAW,EAAE,EAAE;QAAEG,SAAS,EAAE;MAAG;MAC/DC,SAAS,CAACG,KAAK,GAAG,IAAG;IACzB;IAEA,MAAMC,UAAU,GAAGA,CAAA,KAAM;MACrB,IAAI,CAACH,UAAU,CAACE,KAAK,CAACR,KAAK,EAAE;QACzBU,KAAK,CAAC,+BAA+B;QACrC;MACJ;;MAEA;MACAX,QAAQ,CAACS,KAAK,CAACG,IAAI,CAAC;QAChBX,KAAK,EAAEM,UAAU,CAACE,KAAK,CAACR,KAAK;QAC7BC,WAAW,EAAEK,UAAU,CAACE,KAAK,CAACP,WAAW,IAAI,MAAM;QACnDC,MAAM,EAAE,QAAQ;QAChBC,UAAU,EAAE,aAAa;QAAE;QAC3BC,SAAS,EAAEE,UAAU,CAACE,KAAK,CAACJ,SAAQ,GAC9BE,UAAU,CAACE,KAAK,CAACJ,SAAS,CAACQ,KAAK,CAAC,MAAM,CAAC,CAACC,GAAG,CAACC,CAAC,IAAIA,CAAC,CAACC,IAAI,CAAC,CAAC,CAAC,CAAC;QAAA,EAC