{"ast":null,"code":"import { ref, reactive } from 'vue';\nexport default {\n  __name: 'Detail',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const submitting = ref(false);\n    const submitted = ref(false);\n    const submitTime = ref('');\n    const file = ref(null);\n    const remark = ref('');\n    const fileInput = ref(null);\n    const homework = reactive({\n      title: 'Lab 3: Cryptographic Algorithm Implementation',\n      description: 'Please implement RSA algorithm using Java or Python. Requirements:\\n1. Implement KeyGen, Encrypt, Decrypt three functions.\\n2. Be able to handle large integers.\\n3. Submit source code and experimental report in PDF format.',\n      deadline: '2025-12-31 23:59',\n      weight: 20,\n      files: ['Lab3-Guide.pdf', 'TestCases.zip']\n    });\n\n    // Trigger file selection\n    const triggerFileSelect = () => {\n      fileInput.value.click();\n    };\n\n    // Handle file selection\n    const handleFileSelect = event => {\n      const selectedFile = event.target.files[0];\n      if (selectedFile) {\n        file.value = selectedFile;\n      }\n    };\n\n    // Simulate submission\n    const submit = () => {\n      if (!file.value) return alert('Please select a file first!');\n      submitting.value = true;\n      setTimeout(() => {\n        submitting.value = false;\n        submitted.value = true;\n        submitTime.value = new Date().toLocaleString();\n      }, 1500);\n    };\n\n    // ÈáçÊñ∞Êèê‰∫§\n    const redoSubmit = () => {\n      submitted.value = false;\n      file.value = null;\n      remark.value = '';\n    };\n    const __returned__ = {\n      submitting,\n      submitted,\n      submitTime,\n      file,\n      remark,\n      fileInput,\n      homework,\n      triggerFileSelect,\n      handleFileSelect,\n      submit,\n      redoSubmit,\n      ref,\n      reactive\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });